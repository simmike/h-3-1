(function(){var a=document.createElement("div"),b=a.style;b.cssText="background-color:rgba(1,1,1,.5)",n.rgba=b.backgroundColor.indexOf("rgba")>-1}),f=a.Color.names={"aqua":"#00ffff","azure":"#f0ffff","beige":"#f5f5dc","black":"#000000","blue":"#0000ff","brown":"#a52a2a","cyan":"#00ffff","darkblue":"#00008b","darkcyan":"#008b8b","darkgrey":"#a9a9a9","darkgreen":"#006400","darkkhaki":"#bdb76b","darkmagenta":"#8b008b","darkolivegreen":"#556b2f","darkorange":"#ff8c00","darkorchid":"#9932cc","darkred":"#8b0000","darksalmon":"#e9967a","darkviolet":"#9400d3","fuchsia":"#ff00ff","gold":"#ffd700","green":"#008000","indigo":"#4b0082","khaki":"#f0e68c","lightblue":"#add8e6","lightcyan":"#e0ffff","lightgreen":"#90ee90","lightgrey":"#d3d3d3","lightpink":"#ffb6c1","lightyellow":"#ffffe0","lime":"#00ff00","magenta":"#ff00ff","maroon":"#800000","navy":"#000080","olive":"#808000","orange":"#ffa500","pink":"#ffc0cb","purple":"#800080","violet":"#800080","red":"#ff0000","silver":"#c0c0c0","white":"#ffffff","yellow":"#ffff00","transparent":[null,null,null,0],"_default":"#ffffff"}}(jQuery);
/** krpano-content/krpano-wrapper-style/js/styling.js **/
'use strict';
var Styling = function(krpanoEmbeded, krpano, detectedEvent, api) {
    this.krpanoEmbeded = krpanoEmbeded;
    this.krpano = krpano;
    this.detectedEvent = detectedEvent;
    this.api = api;
    CommonPlayer.call(this, krpanoEmbeded);

};

Styling.prototype = Object.create(CommonPlayer.prototype);
Styling.prototype.constructor = Styling;


Styling.prototype.editModeInitialization = function(){
    var instance = this;
    this.krpanoEmbeded.find("#demo-setting").on(this.detectedEvent, function() {
        instance.hideMenu();
        $(".demo").toggleClass("activate hidden");
        $(".demo").hasClass('activate') ? $(this).removeClass('left-0') : $(this).addClass('left-0');
    });
    this.krpanoEmbeded.find('.demo .checkbox').change(function () {
        var element = $(this).attr('data-name');
        if($(this).is(':checked')){
            window.jsonData.visibility[element] = true;
            $('#' + $(this).attr('data-element')).removeClass('hidden');
        } else {
            window.jsonData.visibility[element] = false;
            $('#' + $(this).attr('data-element')).addClass('hidden')
        }
        instance.checkForItemsInsideWrapper('#control-panel-right .buttons-wrapper li', '#control-panel-right');
        instance.checkForItemsInsideWrapper('#control-panel-left .buttons-wrapper li', '#control-panel-left');
        instance.hideControlPanleInCase();
        instance.hideSettingsPanleInCase();
    });
    this.krpanoEmbeded.find(".upload-wrapper a").on(this.detectedEvent,function () {
        var $uploadBtn = $(this).next();
        $uploadBtn.trigger('click');
        $uploadBtn.on('change', function() {
            var form = $(this).closest("form");
            var val = $(this).val();
            $(this).parent().next('span').text(val);
            instance.handleUploadedFileAndReturnUrl(form[0], form.prop("id").replace("_upload_form", ""), $uploadBtn);
        });

    });

    this.krpanoEmbeded.find('#colorSelector2 div').css('backgroundColor', ' rgb(' + window.jsonData.styles.mainColor.r +','+ window.jsonData.styles.mainColor.g +','+ window.jsonData.styles.mainColor.b + ') ');
    this.krpanoEmbeded.find('#colorSelector2').ColorPicker({
        color: ({r:window.jsonData.styles.mainColor.r, g:window.jsonData.styles.mainColor.g, b:window.jsonData.styles.mainColor.b}) ,
        onChange: function(hsb, hex, color) {
            var colorRgb = ' rgb(' + color.r +','+ color.g +','+ color.b + ') ';
            $('#specific-them-color').attr('values', '0 0 0 0 ' +color.r/255+ ' 0 0 0 0 '  +color.g/255+ ' 0 0 0 0 '  +color.b/255+ ' 0  0 0 1  0');
            $('.them-color').css('backgroundColor', colorRgb);
            $('.slider-gallery-section .slider-gallery-wrapper .slider-element.active-element').css('border', '5px solid' + colorRgb);
            $('.live-follow-guest-interaction').css('border-color', colorRgb);
            instance.initMainThemColor(color);
            $('#colorSelector2 div').css('backgroundColor', colorRgb);
            window.jsonData.styles.mainColor.r = color.r;
            window.jsonData.styles.mainColor.g = color.g;
            window.jsonData.styles.mainColor.b = color.b;
        }
    });

    this.krpanoEmbeded.find('#colorpickerField1, #colorpickerField2, #colorpickerField3').ColorPicker({
        onSubmit: function(hsb, hex, rgb, el) {
            $(el).val(hex);
            $(el).ColorPickerHide();
        },
        onBeforeShow: function () {
            $(this).ColorPickerSetColor(this.value);
        }
    })
    .bind('keyup', function(){
        $(this).ColorPickerSetColor(this.value);
    });

    this.krpanoEmbeded.find('#save-list').on('change', function () {
        console.log($(this).val());
        if($(this).val() !== 'choose-email'){
            instance.krpanoEmbeded.find('#save-changes-btn').removeAttr('disabled');
        } else{
            instance.krpanoEmbeded.find('#save-changes-btn').attr('disabled', 'disabled');
        }
    });

    this.krpanoEmbeded.find('#save-changes-btn').on(this.detectedEvent, function () {
        instance.savePlayerInitializazion($(this));
    });
}

Styling.prototype.processUrlsUpload = function(image_type, url){
    if(image_type === 'logoUrl'){
        this.krpanoEmbeded.find("#company-logo img").attr("src", url);
    }else if(image_type === 'hotspotLogo'){
        this.krpano.get("hotspot").getArray().forEach(function(hotspot) {
           if(hotspot.name !== "logo_at_bottom")
            hotspot.url=url;
        })
    }else if(image_type === 'tripodCoverLogo'){
         this.krpano.get("hotspot[logo_at_bottom]").url=url;
     }else if(image_type === 'agentUrl'){
         this.krpanoEmbeded.find(".avatar-section img, .immo-popup.left-section img").attr("src", url);
     } else {
         console.warn("Unexpected image type: '" + image_type + "'");
    }
}

Styling.prototype.handleUploadedFileAndReturnUrl = function(form, image_type, button){
    var instance = this;
    var file = button[0].files[0];
    var name = file.name;
    var size = file.size;
    var type = file.type;
    var imageTypeForJSON;
    if (image_type === "NORMAL") {   // see LogoType.java
        imageTypeForJSON = "logoUrl";  // see PlayerStyles.java
    } else if (image_type === "HOTSPOT") {
        imageTypeForJSON = "hotspotLogo";
    } else if (image_type === "ROUNDED") {
        imageTypeForJSON = "tripodCoverLogo";
    } else if (image_type === "AGENT") {
        imageTypeForJSON = "agentUrl";
    } else {
        console.warn("Unexpected image_type: '" + image_type + "'");
    }
    var doAfter = this.api.after(function(data){
        window.jsonData.styles[imageTypeForJSON] = data.details;
        instance.processUrlsUpload(imageTypeForJSON, data.details);
    });
    this.api.uploadFile(image_type, new FormData(form), doAfter);
}

Styling.prototype.savePlayerInitializazion = function(icon){
    var selected=$("select#editor-select-agent option:selected");
    var id = selected && selected.length > 0 && selected.val().length > 0 ? selected.val() : window.jsonData.ids.userID;
    icon.prop("disabled", true);
    var iconImage=icon.find("i");
    var oldClasses=iconImage.attr("class");
    iconImage.removeClass();
    iconImage.addClass("fa fa-spinner fa-spin fa-fw");
    var onSaveBlock=icon.parent();

    var doAfter = this.api.after(function(data){
          onSaveBlock.find(".success").show("fast");
          setTimeout(function () {
              onSaveBlock.find(".success").hide("slow");
          },4000);
    },
    function(data){
          onSaveBlock.find(".fail").show("fast");
          setTimeout(function () {
              onSaveBlock.find(".fail").hide("slow");
          },4000);
    },
    function(data){
        icon.prop("disabled", false);
        iconImage.removeClass();
        iconImage.addClass(oldClasses);
    });
    this.api.storeInitialization(id, doAfter);
}

/** krpano-content/krpano-wrapper-style/js/screenshot_area.js **/
'use strict';
var Screenshot = function(krpanoEmbeded, krpano, api, detectedEvent, plugin) {
    this.krpanoEmbeded = krpanoEmbeded;
    this.krpano = krpano;
    this.api = api;
    this.detectedEvent = detectedEvent;
    this.plugin = plugin;
    CommonPlayer.call(this, krpanoEmbeded);

};

Screenshot.prototype = Object.create(CommonPlayer.prototype);
Screenshot.prototype.constructor = Screenshot;

Screenshot.prototype.init = function(){
    var ratio = window.jsonData.ratio;
    var instance = this;
    var $snapshotIcon = this.krpanoEmbeded.find('#screenshotIcon');
    var drag = false;
    var screenWidth, screenHeight, newHeight, newWidth;
    var isToolActive = false;

    $('body').off(this.detectedEvent, '#screenshotIcon').on(this.detectedEvent, '#screenshotIcon', function (){
        if (instance.krpanoEmbeded.find('.snapshot-section').hasClass('hidden')) instance.getSnapshotAreaResolution();

        if (instance.isIS24Theme()) {
            toggleIntro();
        }

        instance.krpanoEmbeded.find('.snapshot-section').toggleClass('hidden');
        instance.hideMenu();
        instance.hideGallery();
        instance.plugin.disablerotate();

        var that = this;
        var sliderValue, number, snapshotWidth, snapshotHeight, snapshotMinWidthPercentage, snapshotMinWidth, snapshotMinHeight, ratioOption, ratioOption2;
        var $ratioSelect = $('.ratio-type');
        var $snapshotWrapper =  instance.krpanoEmbeded.find('.snapshot-wrapper');
        number = +$('#aspect-ratio-btn').attr('value');

        snapshotWidth = ratio[number].width;
        snapshotHeight = (ratio[number].v * snapshotWidth) / ratio[number].h;

        snapshotMinWidth = ratio[number].width;
        snapshotMinHeight = (ratio[number].v * snapshotWidth) / ratio[number].h;

        $snapshotWrapper.width(snapshotWidth).height(snapshotHeight);
        ratioOption = 4;
        ratioOption2 = 3;
        instance.changeSliderMinMaxValue($snapshotWrapper, snapshotWidth, snapshotHeight, ratioOption, ratioOption2);

        instance.krpanoEmbeded.find('.ratio-type').on(instance.detectedEvent, function () {
            instance.krpanoEmbeded.find('#aspect-ratio-btn .active-text').text($(this).children('a').text());
            $ratioSelect.each(function () {
                $(this).removeClass('ratio-active')
            });
            $(this).addClass('ratio-active');

            number = +$('.ratio-type.ratio-active').attr('value');
            snapshotMinWidth = ratio[number].width;
            snapshotMinHeight = (ratio[number].v * snapshotWidth) / ratio[number].h;

            snapshotWidth = $snapshotWrapper.width();
            snapshotHeight = (ratio[number].v * snapshotWidth) / ratio[number].h;

            ratioOption = ratio[number].h;
            ratioOption2 = ratio[number].v;
            $snapshotWrapper.width(snapshotWidth).height(snapshotHeight);
            instance.changeSliderMinMaxValue($snapshotWrapper, snapshotMinWidth, snapshotMinHeight, ratioOption, ratioOption2);
            instance.getSnapshotAreaResolution();
        });

        instance.krpanoEmbeded.bind('resizeEnd', function() {
            number = +$('.ratio-type.ratio-active').attr('value');
            snapshotMinWidth = ratio[number].width;
            snapshotMinHeight = (ratio[number].v * snapshotWidth) / ratio[number].h;
            instance.changeSliderMinMaxValue($snapshotWrapper, snapshotMinWidth, snapshotMinHeight,  ratioOption, ratioOption2);
        });

        instance.krpanoEmbeded.resize(function() {
            if($(this).resizeTO) clearTimeout($(this).resizeTO);
            $(this).resizeTO = setTimeout(function() {
                instance.krpanoEmbeded.trigger('resizeEnd');
            }, 500);
        });

        $('.drag-snapshot-area').on('mousedown touchstart', function () {
            drag = true;
        });
        $('body').on('mousemove touchmove', '#krpanoSWFObject', function (e) {
            if (!drag) return;
            var sizeObj = {};
            $('.drag-snapshot-area').on('dragstart', function(event) { event.preventDefault(); });
            screenWidth = $(this).width();
            screenHeight = $(this).height();

            if (e.originalEvent.changedTouches) {
                newWidth =  screenWidth - 2 * e.originalEvent.changedTouches[0].clientX;
                newHeight = screenHeight - 2 * e.originalEvent.changedTouches[0].clientY;
            }
            else {
                newWidth =  screenWidth - 2 * e.clientX;
                newHeight = screenHeight - 2 * e.clientY;
            }

            sizeObj['width'] = newWidth;
            sizeObj['height'] = newHeight;

            instance.changeSnapshotSize(sizeObj, ratioOption, ratioOption2);
            instance.getSnapshotAreaResolution();
            $('.drag-snapshot-area').tooltip('hide');
        }).on('mouseup touchend', function () {
            drag = false;

        }).on('hover', function () {
            $('.drag-snapshot-area').tooltip('show');
        });

        instance.krpanoEmbeded.find('.snapshot-slider .ui-slider-range, .snapshot-slider .ui-slider-handle')
                 .css('background-color', 'rgba('+ jsonData.styles.mainColor.r +','+ jsonData.styles.mainColor.g +','+ jsonData.styles.mainColor.b +','+'0.85)')
                 .addClass('them-color');
    });

    this.krpanoEmbeded.on(this.detectedEvent, '.snapshot-section .close-btn', function(){
        instance.krpanoEmbeded.find('.snapshot-section').addClass('hidden');
        isToolActive = false;
    });

    this.krpanoEmbeded.on(this.detectedEvent, '.snapshot-btn', function () {
        var $snapshotWrapper = instance.krpanoEmbeded.find('.snapshot-wrapper');
        var x1 = $snapshotWrapper.offset().left, y1 = $snapshotWrapper.offset().top;
        var width = Math.round($snapshotWrapper.width());
        var height = Math.round($snapshotWrapper.height());
        var x2 = x1 + width, y2 = y1 + height;

        var middleTop = instance.krpano.screentosphere(x1+width/2, y1);
        var middleBottom = instance.krpano.screentosphere(x1+width/2, y2);
        var frameFov = Math.abs(middleTop.y - middleBottom.y);
        var centerX = instance.krpano.get("view.hlookat");
        var centerY = -instance.krpano.get("view.vlookat");
        // local testing:
        //var url = "http://localhost:8080/render?x=1.0&y=2.0&fov=3.0&width=1&height=2"
        var url = window.jsonData.urls.backendUrls.takeScreens
            .replace("x=1.0", "x=" + centerX)
            .replace("y=2.0", "y=" + centerY)
            .replace("fov=3.0", "fov=" + frameFov)
            .replace("%7BFILENAME%7D", $("#actualPano").prop("value"))
            .replace("width=1", "width=" + width)
            .replace("height=2", "height=" + height);
        var $downloadFile=$("#downloadFile");
        $downloadFile.attr("href", url);
        $downloadFile[0].click();
    });

    function toggleIntro() {
        if (!isToolActive) {
            isToolActive = true;

            var notShowAgain = window.localStorage.getItem('screenshotIntroNotShowAgain');

            if (!notShowAgain) {
                $('#screenshot-intro').find('input[name="not-show-again"]').prop('checked', notShowAgain);
                $('#screenshot-intro').modal('show');
            }
        } else {
            isToolActive = false;
        }
    }

    this.krpanoEmbeded.on('change', '#screenshot-intro input[name="not-show-again"]', function () {
        if ($(this).is(':checked')) {
            window.localStorage.setItem('screenshotIntroNotShowAgain', true);
        } else {
            window.localStorage.removeItem('screenshotIntroNotShowAgain');
        }
    });
};

Screenshot.prototype.addSnapshotRatioData = function(){
    var ratioArr = {ratio : [
        {name: 'screnshot.ratio.4.3', h: 4, v: 3, width: 400},
        {name: 'screnshot.ratio.16.9', h: 16, v: 9, width: 500},
        {name: 'screnshot.ratio.1.1', h: 1, v: 1, width: 284}
    ]};
    $.extend(window.jsonData, ratioArr);
};

Screenshot.prototype.getSnapshotAreaResolution = function(){
    this.krpanoEmbeded.find('.resolution-wrapper .resolution-width').text(Math.round($('.snapshot-wrapper').width()) + 6);
    this.krpanoEmbeded.find('.resolution-wrapper .resolution-height').text(Math.round($('.snapshot-wrapper').height()) + 6);
};

Screenshot.prototype.changeSnapshotSize = function(data, ratioOption, ratioOption2){
    var $snapshotWrapper =  $('.snapshot-wrapper');
    var screenWidth = this.krpanoEmbeded.width() - 80;
    var screenHeight = this.krpanoEmbeded.height() - 50;
    if($snapshotWrapper.height() > $snapshotWrapper.width()){
        if (data.width < 280) return;
        var height = (data.width * ratioOption2) / ratioOption;
        if (height <= screenHeight) $snapshotWrapper.css({width: data.width, height: height});
    } else{
        if (data.height < 280) return;
        var width = (data.height * ratioOption) / ratioOption2;
        if (width <= screenWidth) $snapshotWrapper.css({width: width, height: data.height});
    }
};

Screenshot.prototype.changeSliderMinMaxValue = function($snapshotWrapper, snapshotWrapperMinWidth, snapshotWrapperMinHeight, ratioOption, ratioOption2){
    var minVal, maxVal, width, height;
    var $this = $('#krpanoSWFObject');
    var screenWidth = $this.width() - 80;
    var screenHeight = $this.height() - 32;
    if ($snapshotWrapper.width() > screenWidth){
        $snapshotWrapper.width(screenWidth);
        height = (ratioOption2 * $snapshotWrapper.width())/ratioOption;
        $snapshotWrapper.height(height);
    }
    if ($snapshotWrapper.height() > screenHeight){
        $snapshotWrapper.height(screenHeight);
        width = (ratioOption * $snapshotWrapper.height())/ratioOption2;
        $snapshotWrapper.width(width);
    }
    if($snapshotWrapper.width() > $snapshotWrapper.height()){
        minVal = snapshotWrapperMinWidth;
        maxVal = screenWidth;
    } else if($snapshotWrapper.height() > $snapshotWrapper.width()){
        minVal = snapshotWrapperMinHeight;
        maxVal = screenHeight;
    } else{
        minVal = snapshotWrapperMinWidth;
        maxVal = screenWidth >= screenHeight ? screenHeight : screenWidth;
    }
    return {minVal: minVal, maxVal: maxVal}
};

/** krpano-content/krpano-wrapper-style/js/immoviewer_api.js **/
'use strict';
var ImmoviewerAPI = function(krpanoEmbeded) {
    this.krpanoEmbeded = krpanoEmbeded;
    CommonPlayer.call(this, krpanoEmbeded);
};

ImmoviewerAPI.prototype = Object.create(CommonPlayer.prototype);
ImmoviewerAPI.prototype.constructor = ImmoviewerAPI;


ImmoviewerAPI.prototype.after = function(done, fail, always) {
     if (!done)
       done = function() {};
     if (!fail)
       fail = function() {};
     if (!always)
       always = function() {};
     var feedback = function(){};
     var feedbackFail = function(){};

    return {done: done, fail: fail, always: always, feedback: feedback, feedbackFail: feedbackFail};
}

ImmoviewerAPI.prototype.uploadCommon = function(url, type, contentType, dataRequest, doAfter, button, async, process, withoutFeedback, warning) {
    var instance = this;
    this.buttonLoading(button);
    $.ajax({
      type: type,
      url: url,
      async: async,
      contentType: contentType,
      beforeSend: function (xhr) {
          if(window.jsonData && window.jsonData.username && window.jsonData.pass)
            xhr.setRequestHeader ("Authorization", "Basic " + instance.b64EncodeUnicode(window.jsonData.username + ":" + decodeURIComponent(window.jsonData.pass)));
      },
      data: dataRequest,
      cache: false,
      processData: process
    }).done(function(responseData){
        if (doAfter !== null){
            doAfter.done(responseData);
            if (!warning) {
                doAfter.feedback();
            } else{
                doAfter.feedbackWarning(warning);
            }
        }
    }).fail(function(responseData){
        if (doAfter !== null){
            doAfter.fail(responseData);
            doAfter.feedbackFail();
        }
    }).always(function(){
        instance.buttonUnloading(button);
        if (doAfter !== null) doAfter.always();
    });

    if (!withoutFeedback) {
        this.addFeedbackModal(doAfter);
    }
}

ImmoviewerAPI.prototype.deleteAttachment = function(button, fileID, ID, doAfter, withoutFeedback) {
    var url = window.jsonData.urls.backendUrls.deleteAttachment.replace("%7BFILENAME%7D", fileID).replace("%7BATTACHMENT%7D", ID);
    this.uploadCommon(url, "DELETE", false, null, doAfter, button, true, false, withoutFeedback);
}

ImmoviewerAPI.prototype.createTour = function(button, name, agentID, dataRequest, doAfter) {
    var url = window.jsonData.baseBackendUrl + "tour/create?name=" + encodeURIComponent(name);
    if(!!agentID){
        url += "&agentID=" + agentID;
    }

    this.uploadCommon(url, "PUT", "application/x-www-form-urlencoded; charset=UTF-8", dataRequest, doAfter, button, true, true);
}

ImmoviewerAPI.prototype.uploadImage = function(tourID, floor, dataRequest, doAfter) {
    var url = window.jsonData.baseBackendUrl + "tour/"+jsonData.tour.id+"/pano?floor=" + floor;
    this.uploadCommon(url, "POST", false, dataRequest, doAfter, null, true, false);
}

ImmoviewerAPI.prototype.uploadMap = function(button, floorSelected, dataRequest, doAfter) {
    var url = window.jsonData.urls.backendUrls.storeMap.replace("map/9999", "map/" + floorSelected);
    this.uploadCommon(url, "POST", false, dataRequest, doAfter, button, true, false);
}
ImmoviewerAPI.prototype.deleteMap = function(button, floorSelected, doAfter) {
    var url = window.jsonData.urls.backendUrls.storeMap.replace("map/9999", "map/" + floorSelected);
    doAfter.feedbackFail = function(){button.closest(".modal").find(".subtitle.error").show("fast")}
    this.uploadCommon(url, "DELETE", false, null, doAfter, null, true, false, true);
}
ImmoviewerAPI.prototype.changeMapName = function(button, floorSelected, name, doAfter) {
    var url = window.jsonData.baseBackendUrl + "tour/"+jsonData.tour.id+"/map/" + floorSelected + "/name/"+ name;
    if(button){
        doAfter.feedbackFail = function(){button.closest(".modal").find(".subtitle.error").show("fast")}
    }
    this.uploadCommon(url, "PUT", false, null, doAfter, button, true, false);
}

ImmoviewerAPI.prototype.changeFloorIndex = function(button, modifiedFloorIndex, newFloorIndex, doAfter) {
    var url = window.jsonData.baseBackendUrl + "tour/"+jsonData.tour.id+"/map/" + modifiedFloorIndex + "/change/index/"+ newFloorIndex;
    if(button){
        doAfter.feedbackFail = function(){button.closest(".modal").find(".subtitle.error").show("fast")}
    }
    this.uploadCommon(url, "PUT", false, null, doAfter, button, true, false);
}

ImmoviewerAPI.prototype.changeDefaultFloor = function(button, floorSelected, doAfter) {
    var url = window.jsonData.baseBackendUrl + "tour/"+jsonData.tour.id+"/map/" + floorSelected + "/default?defaultFloor=true"
    if(button){
        doAfter.feedbackFail = function(){button.closest(".modal").find(".subtitle.error").show("fast")}
    }
    this.uploadCommon(url, "PUT", false, null, doAfter, button, true, false);
}


ImmoviewerAPI.prototype.uploadMapHotspot = function(floorSelected, dataRequest, doAfter) {
    var url = window.jsonData.urls.backendUrls.storeMapHotspot.replace("map/9999", "map/" + floorSelected);
    this.uploadCommon(url, "POST", "application/json", dataRequest, doAfter, null, true, false);
}
ImmoviewerAPI.prototype.getJsonDataForAddNewTours = function(doAfter) {
    var url = "/portal/tour/create/initialize";

    this.uploadCommon(url, "GET", "application/json", null, doAfter, null, true, false);
}

