    var url = window.jsonData.baseBackendUrl + "user/me/agents?scope=contact";
    this.uploadCommon(url, "GET", "application/json", null, doAfter, null, true, false);
}

ImmoviewerAPI.prototype.getUser = function(doAfter) {
    var url = window.jsonData.baseBackendUrl + "user/me";
    this.uploadCommon(url, "GET", "application/json", null, doAfter, null, true, false);
}

ImmoviewerAPI.prototype.deleteMapHotspot = function(floorSelected, filename, doAfter) {
    var url = window.jsonData.urls.backendUrls.deleteMapHotspot.replace("map/9999", "map/" + floorSelected).replace("%7BFILENAME%7D", filename);
    this.uploadCommon(url, "DELETE", false, null, doAfter, null, true, false);
}

ImmoviewerAPI.prototype.uploadFile = function(image_type, dataRequest, doAfter) {
    var url = window.jsonData.urls.backendUrls.storeLogo.replace("%7BTYPE%7D", image_type);
    this.uploadCommon(url, "POST", false, dataRequest, doAfter, null, true, false);
}

ImmoviewerAPI.prototype.limitedView = function(button, actualPano, hlookatmin, hlookatmax, vlookatmin, vlookatmax, doAfter, warning) {
    var url = window.jsonData.urls.backendUrls.limitedView.replace("%7BFILENAME%7D", actualPano)
    .replace("9991.0", hlookatmin).replace("9992.0", hlookatmax).replace("9993.0", vlookatmin).replace("9994.0", vlookatmax);
    this.uploadCommon(url, "PUT", false, null, doAfter, button, true, false, false, warning);
}
ImmoviewerAPI.prototype.limitedViewDelete = function(button, actualPano, doAfter) {
    var url = window.jsonData.urls.backendUrls.limitedView.replace("%7BFILENAME%7D", actualPano);
    url = url.substring(0,url.indexOf("?"));
    this.uploadCommon(url, "DELETE", false, null, doAfter, button, true, false);
}

ImmoviewerAPI.prototype.modifyAttachment = function(button, fileID, ID, dataRequest, doAfter) {
    var url = window.jsonData.urls.backendUrls.deleteAttachment.replace("%7BFILENAME%7D", fileID).replace("%7BATTACHMENT%7D", ID);
    this.uploadCommon(url, "PUT", "application/json; charset=utf-8", dataRequest, doAfter, button, true);
}
ImmoviewerAPI.prototype.addAttachment = function(button, fileID, dataRequest, doAfter) {
    var url = window.jsonData.urls.backendUrls.addAttachment.replace("%7BFILENAME%7D", fileID);
    this.uploadCommon(url, "POST", false, dataRequest, doAfter, button, true, false);
}

ImmoviewerAPI.prototype.storeInitialization = function(id, doAfter) {
    var url = window.jsonData.urls.backendUrls.storePlayerInitialization.replace("%7BID%7D", id);
    var dataRequest = JSON.stringify({visibility: window.jsonData.visibility, styles: window.jsonData.styles});
    this.uploadCommon(url, "POST", "application/json; charset=utf-8", dataRequest, doAfter, null, true, false);
}

ImmoviewerAPI.prototype.changeAttachmentPosition = function(fileID, hotspot, krpanoHotspot, doAfter) {
    var url = window.jsonData.urls.backendUrls.changeAttachmentPosition
        .replace("%7BFILENAME%7D", fileID)
        .replace("x=0.0", "x=" + hotspot.xPos)
        .replace("y=0.0", "y=" + hotspot.yPos)
        .replace("newX=0.0", "newX=" + krpanoHotspot.ath)
        .replace("newY=0.0", "newY=" + krpanoHotspot.atv);

    url += "&attachmentID=" + hotspot.content[0].ID;
    this.uploadCommon(url, "PUT", false, null, doAfter, null, true, false);
}

ImmoviewerAPI.prototype.deleteRoom = function(button, filename, doAfter) {
    var url = window.jsonData.baseBackendUrl + "tour/"+jsonData.tour.id+"/pano/" + filename;
    this.uploadCommon(url, "DELETE", false, null, doAfter, button, true, false);
}

ImmoviewerAPI.prototype.uploadRoom = function(button, dataRequest, floorSelected, doAfter) {
    var url = window.jsonData.urls.backendUrls.uploadRoom.replace("floor=9999", "floor=" + floorSelected);
    this.uploadCommon(url, "POST", false, dataRequest, doAfter, button, true, false);
}

ImmoviewerAPI.prototype.replaceImage = function(button, dataRequest, filename, floorSelected, doAfter) {
    var url = window.jsonData.urls.backendUrls.replaceImage.replace("floor=9999", "floor=" + floorSelected).replace("%7BFILENAME%7D", filename);
    this.uploadCommon(url, "POST", false, dataRequest, doAfter, button, true, false);
}

ImmoviewerAPI.prototype.blurRoom = function(button, fileID, x, y, width, height, doAfter, warning) {
    var url = window.jsonData.urls.backendUrls.blurUrl.replace("%7BFILENAME%7D", fileID);
    url = url + "?figureType=Rectangle&x=" + x + "&y=" + y + "&width=" + width + "&height=" + height;
    this.uploadCommon(url, "POST", false, null, doAfter, button, true, false, false, warning);
}

ImmoviewerAPI.prototype.blurModify = function(button, fileID, x, y, width, height, blurID, doAfter, warning) {
    var url = window.jsonData.urls.backendUrls.blurUrl.replace("%7BFILENAME%7D", fileID) + "/" + blurID;
    url = url + "?figureType=Rectangle&x=" + x + "&y=" + y + "&width=" + width + "&height=" + height;
    this.uploadCommon(url, "PUT", false, null, doAfter, button, true, false, false, warning);
}

ImmoviewerAPI.prototype.executeBlur = function(filename, async, warning) {
    var url = window.jsonData.urls.backendUrls.blurExecuteUrl;
    if(filename !== null && filename && filename.length > 0)
        url = url + "?panorama=" + filename
    console.log("Execute blur to url=" + url);
    this.uploadCommon(url, "GET", false, null, null, null, async, false, false, warning);
}


ImmoviewerAPI.prototype.blurDelete = function(button, fileID, blurID, doAfter, warning) {
    var url = window.jsonData.urls.backendUrls.blurUrl.replace("%7BFILENAME%7D", fileID) + "/" + blurID;
    this.uploadCommon(url, "DELETE", false, null, doAfter, button, true, false, false, warning);
}

ImmoviewerAPI.prototype.addFeedbackModal = function(doAfter){
    if(doAfter && window.lang){
        var instance = this;
        doAfter.feedback = function(){instance.showInfo(window.lang.translate("default.message.top.success.default"), 4000);}
        doAfter.feedbackFail = function(){instance.showError(window.lang.translate("modals.contact.error.description"), 4000);}
        doAfter.feedbackWarning = function(warning){
            var message = warning;
            if (warning === true) {
                message = "default.message.top.warning.description";
            }
            instance.showWarning(window.lang.translate(message), 4000);
        }
        console.log("Adding doAfter feedbacks ", doAfter);
    }
}


ImmoviewerAPI.prototype.editRoomContent = function(button, name, floor, filename,limitedAccess, firstRoom, doAfter, withoutFeedback) {
    var url = window.jsonData.baseBackendUrl + "tour/"+jsonData.tour.id+"/pano/" + filename;
    var dataRequest = JSON.stringify({name: name, plan: floor, limitedAccess: limitedAccess, isFirst: firstRoom});
    this.uploadCommon(url, "PUT", "application/json; charset=utf-8", dataRequest, doAfter, button, true, false, withoutFeedback);
}

ImmoviewerAPI.prototype.presetCenter = function(button, actualPano, presetCenterH, presetCenterV, presetCenterFOV, doAfter) {
    var url = window.jsonData.urls.backendUrls.presetCenter.replace("%7BFILENAME%7D", actualPano)
    .replace("9991.0", presetCenterH).replace("9992.0", presetCenterV).replace("9993.0", presetCenterFOV);
    this.uploadCommon(url, "PUT", false, null, doAfter, button, true, false);
}
ImmoviewerAPI.prototype.presetCenterDelete = function(button, actualPano, doAfter) {
    var url = window.jsonData.urls.backendUrls.presetCenterDelete.replace("%7BFILENAME%7D", actualPano);
    this.uploadCommon(url, "DELETE", false, null, doAfter, button, true, false);
}

ImmoviewerAPI.prototype.buttonLoading = function(button) {
    if(button !== null && typeof button !== 'undefined'){
        var loadingText = (window.lang) ? window.lang.translate('modals.contact.form.sending') : "Sending";
        var newButton = button.clone();
        button.hide();
        newButton.text("");
        newButton.append('<i class="fa fa-spinner fa-spin fa-fw"></i> ' +loadingText);
        newButton.prop("disabled", true);
        newButton.css("pointer-events", "none");
        newButton.addClass("tempLoading").appendTo(button.parent());
    }
}

ImmoviewerAPI.prototype.buttonUnloading = function(button) {
    if(button !== null && typeof button !== 'undefined'){
        button.show();
        button.parent().find(".tempLoading").remove();
        button.prop("disabled", false);
        button.css("pointer-events", "initial");
    }
}

ImmoviewerAPI.prototype.sendTrackingEvent = function(eventType) {
    var url = window.jsonData.urls.backendUrls.trackUserEventAnonymous;
    var dataRequest = JSON.stringify({ resourceType: 'VIRTUAL_TOUR', modelID: jsonData.tour.id, eventType: eventType });
    this.uploadCommon(url, "POST", "application/json; charset=utf-8", dataRequest, null, null, true, false, true);
}

ImmoviewerAPI.prototype.acceptPrivacyPolicy = function(email) {
    var url = window.jsonData.urls.backendUrls.acceptPrivacyPolicy;
    var dataRequest = JSON.stringify({ "email": email });

    this.uploadCommon(url, "PUT", "application/json; charset=UTF-8", dataRequest, null, null, true);
}

ImmoviewerAPI.prototype.deleteTour = function(tourID, doAfter) {
    var url = window.jsonData.urls.backendUrls.deleteTour.replace('%7BTOURID%7D', tourID);
    return this.uploadCommon(url, "DELETE", false, null, doAfter);
};
/** krpano-content/krpano-wrapper-style/js/immoviewer_promise_api.js **/
'use strict';

var ImmoviewerPromiseApi = function ImmoviewerPromiseApi() { };

ImmoviewerPromiseApi.prototype.requestCommon = function(url, type, dataRequest, contentType, async) {
    return new Promise(function(resolve, reject) {
        $.ajax({
            type: type,
            url: url,
            async: async,
            contentType: contentType,
            beforeSend: function beforeSend(xhr) {
                // TODO: use jwtToken instead
                if (jsonData && jsonData.username && jsonData.pass) {
                    xhr.setRequestHeader('Authorization', 'Basic ' + btoa(jsonData.username + ':' + decodeURIComponent(jsonData.pass)));
                }
            },
            data: dataRequest,
            cache: false,
            processData: false
        }).done(function(responseData) {
            try {
                responseData = JSON.parse(responseData);
            } catch (e) { }

            resolve(responseData);
        }).fail(function(error) {
            console.error('Request', url, 'has failed:', error);
            reject(error);
        });
    });
};

ImmoviewerPromiseApi.prototype.putRequest = function(url, dataRequest, contentType) {
    return this.requestCommon(url, 'PUT', dataRequest, contentType);
};

ImmoviewerPromiseApi.prototype.postRequest = function(url, dataRequest, contentType) {
    return this.requestCommon(url, 'POST', dataRequest, contentType);
};

ImmoviewerPromiseApi.prototype.deleteRequest = function(url) {
    return this.requestCommon(url, 'DELETE');
};

ImmoviewerPromiseApi.prototype.createTimelineTour = function(originalTourID, name, agentID) {
    var url = window.jsonData.baseBackendUrl + 'tour/create?name=' + encodeURIComponent(name) + '&agentID=' + agentID + '&timelineOf=' + originalTourID;
    return this.putRequest(url, JSON.stringify({}), 'application/json; charset=UTF-8');
};

ImmoviewerPromiseApi.prototype.createTimelinePano = function(tourID, dataRequest) {
    var url = window.jsonData.urls.backendUrls.createPano.replace('%7BTOURID%7D', tourID);
    return this.postRequest(url, dataRequest, 'application/json; charset=UTF-8');
};

ImmoviewerPromiseApi.prototype.uploadTimelinePano = function(tourID, panoID, dataRequest) {
    var url = window.jsonData.urls.backendUrls.uploadImageToPano.replace('%7BTOURID%7D', tourID).replace('%7BPANOID%7D', panoID);
    return this.postRequest(url, dataRequest, false);
};

ImmoviewerPromiseApi.prototype.changeTimelinePano = function(tourID, panoID, dataRequest) {
    var url = window.jsonData.urls.backendUrls.uploadImageToPano.replace('%7BTOURID%7D', tourID).replace('%7BPANOID%7D', panoID);
    return this.putRequest(url, dataRequest, false);
};

ImmoviewerPromiseApi.prototype.deleteTimelinePano = function(tourID, panoID) {
    var url = window.jsonData.urls.backendUrls.editPano.replace('/v2/', '/v1/').replace('%7BTOURID%7D', tourID).replace('%7BPANOID%7D', panoID);
    return this.deleteRequest(url);
};

ImmoviewerPromiseApi.prototype.deleteTour = function(tourID) {
    var url = window.jsonData.urls.backendUrls.deleteTour.replace('%7BTOURID%7D', tourID);
    return this.deleteRequest(url);
};

/** krpano-content/krpano-wrapper-style/js/new.design.js **/
'use strict';

var ImmoviewerDefaultPlayer = function(plugin, krpanoEmbeded, detectedEvent, floorplan) {
    this.plugin = plugin;
    this.krpanoEmbeded = krpanoEmbeded;
    this.detectTouchClickEvent = detectedEvent;
    this.floorplan = floorplan;
    this.pBody = $("body");
    this.init();
};

ImmoviewerDefaultPlayer.prototype.init = function () {
    var that = this;
    setTimeout(function () {
        that.initPopovers();
        that.appendThemColorToDynamicElements();
        if (window.jsonData.propertyInfo) { that.initRightMenuMapPlugin('g-map-wrapper'); }
        that.initFloorplan();
    }, 0);

    this.setRightMenuEvents();
    this.handleMainEvents();
};


ImmoviewerDefaultPlayer.prototype.initRightMenuMapPlugin = function(mapWrapperId){
    var location = window.jsonData.propertyInfo.location;
    var address = window.jsonData.propertyInfo.simpleAddress;
    var that = this;

    var mouseEnterHandler = function (e) {
        that.krpanoEmbeded.find('#' + mapWrapperId).addClass('expand');
        mymap.invalidateSize();
    };

    var mouseLeaveHandler  = function (e) {
        that.krpanoEmbeded.find('#' + mapWrapperId).removeClass('expand');
        mymap.invalidateSize();
    };

    if(location && location.center && location.center.latitude && location.center.longitude){
        L.Icon.Default.imagePath = window.baseUrl + "/krpano-common/js/images/";
        var latitude = location.center.latitude;
        var longitude = location.center.longitude;
        var mymap = L.map(mapWrapperId).setView([latitude, longitude], 15);

        L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
            attribution: '&copy; <a href="http://osm.org/copyright">OpenStreetMap</a> contributors',
            minZoom: 10,
            maxZoom: 18,
            center: [latitude, longitude]
        }).addTo(mymap);
        L.marker([latitude, longitude]).addTo(mymap).bindPopup(address);

        if(location.accuracy === "APPROXIMATE" ){
            var distance = this.initRightMenuMapPluginGetDistance(location.ne, location.center);
            if(distance > 0){
                var circle=L.circle([latitude, longitude], {radius: distance});
                circle.addTo(mymap);
                mymap.fitBounds(circle.getBounds());
            }
        }

        this.krpanoEmbeded.find('#' + mapWrapperId)
            .on("mouseenter", mouseEnterHandler)
            .on("mouseleave", mouseLeaveHandler);

        this.krpanoEmbeded.on(this.detectTouchClickEvent, '#map-btn', function () {
            that.krpanoEmbeded.find('#' + mapWrapperId).toggleClass('hidden');
            mymap.invalidateSize();
        });
    }
};


ImmoviewerDefaultPlayer.prototype.initRightMenuMapPluginGetDistance = function (ne, center) {
    var mapBoundNorthEast = L.latLng(ne.latitude, ne.longitude);
    var mapDistance = mapBoundNorthEast.distanceTo(L.latLng(center.latitude, center.longitude));
    return mapDistance;
}

ImmoviewerDefaultPlayer.prototype.appendThemColorToDynamicElements = function () {
    var themColor = '#333';

    if(!jsonData.visibility.defaultColor) {themColor = 'rgba('+ jsonData.styles.mainColor.r +','+ jsonData.styles.mainColor.g +','+ jsonData.styles.mainColor.b +','+'0.85)'}

    $('.language-switcher').on('select2:open', function (e) {
        $('.select2-dropdown').css('background-color', themColor);
    });

    $('#share-button, #wrench-button').on('shown.bs.popover', function () {
        $('.popover-content').css('background-color', themColor);
    });
};


Array.prototype.find = Array.prototype.find || function (callback) {
        for (var i = 0; i < this.length; i++) {
            if (callback(this[i], i)) {
                return this[i];
            }
        }
    };

ImmoviewerDefaultPlayer.prototype.getInfopointTypes = function () {
    var infopointsTypes = [];
    for (var roomname in window.jsonData.tour.rooms) {
        if ( window.jsonData.tour.rooms.hasOwnProperty(roomname) && !$.isEmptyObject(window.jsonData.tour.rooms[roomname].infopoints) ){
            for (var infopoint in window.jsonData.tour.rooms[roomname].infopoints) {
                if (window.jsonData.tour.rooms[roomname].infopoints.hasOwnProperty(infopoint)) {
                    var infopointContent = window.jsonData.tour.rooms[roomname].infopoints[infopoint].content[0];
                    var infopointType = infopointsTypes.find(function (element) {
                        return element === infopointContent.type
                    });
                    if (!infopointType) {
                        infopointsTypes.push(infopointContent.type)
                    }
                }
            }
        }
    }
    return infopointsTypes;
};

ImmoviewerDefaultPlayer.prototype.initPopovers = function () {
  var popoverMenuData = [
      {
          id: '#share-button',
          contentTemplate: '<div class="menu txt-color-white">' +
                              '<a class="menu-tool-item" href="https://www.linkedin.com/shareArticle?url='+window.jsonData.urls.currentUrl+'" target="_blank">'+
                              '<i class="fa fa-lg fa-linkedin-square"></i>' +
                              '</a>'+
                              '<a class="menu-tool-item" href="https://plus.google.com/share?hl=en-US&url='+window.jsonData.urls.currentUrl+'" target="_blank">'+
                              '<i class="fa fa-lg fa-google-plus-square"></i>' +
                              '</a>'+
                              '<a class="menu-tool-item" href="https://twitter.com/share?url='+window.jsonData.urls.currentUrl+'" target="_blank">'+
                              '<i class="fa fa-lg fa-twitter-square"></i>' +
                              '</a>'+
                              '<a class="menu-tool-item" href="https://www.facebook.com/sharer/sharer.php?u='+window.jsonData.urls.currentUrl+'" target="_blank">'+
                              '<i class="fa fa-lg fa-facebook-square"></i>' +
                              '</a>'+
                              '<a class="menu-tool-item ' + checkForVisibility(window.jsonData.visibility.showShareEmail) + '" href="#" id="shareEmail">' +
                              '<i class="fa fa-lg fa-envelope"></i>' +
                              '</a>' +
                              '<i class="menu-tool-item fa fa-lg fa-clipboard link-to-clipboard" data-clipboard-text="'+window.jsonData.urls.currentUrl+'"></i>' +
                           '</div>'
      },
      {
          id: '#wrench-button',
          contentTemplate: '<div class="menu txt-color-white buttons-wrapper">' +
          '<span id="measureIcon" class="menu-tool-item"><img src="'+window.baseUrl+'/krpano-common/img/ruler.png" alt="" class="rule '+checkForVisibility(window.jsonData.visibility.showMeasurementTool)+'"></span>' +
          '<i id="screenshotIcon" class="fa fa-lg fa-camera menu-tool-item ' +checkForVisibility(window.jsonData.visibility.showTakeScreens)+ '"></i>' +
          '</div>'
      }
  ];
  return this.initControlMenuPopovers(popoverMenuData);
};

function checkForVisibility(elementVisible) {
    return elementVisible ? '' : 'hidden';
}

function togglePopover() {
    $(this).popover('toggle');
}

function hidePopover() {
    $(this).popover('hide');
}
ImmoviewerDefaultPlayer.prototype.initControlMenuPopovers = function (itemsCollection) {
    var i = 0;
    var options;

    var selector = this.krpanoEmbeded;

    for (i; itemsCollection.length > i; i++) {
        options = {
            placement: 'top',
            html: true,
            trigger: "manual",
            content: itemsCollection[i].contentTemplate,
            template: '<div class="popover no-padding custom-popover" tabindex="0" role="button"><h3 class="popover-title"></h3><div class="popover-content popover-menu-tool-item" style="border-radius: unset"><div></div></div></div>'
        };
        selector.find(itemsCollection[i].id).popover(options).blur(hidePopover).click(togglePopover);
    }
};

ImmoviewerDefaultPlayer.prototype.initFloorplan = function () {
    var plan = jsonData.tour.activeRoom.plan;

    if (!this.floorplan.isAvailableForCurrentFloor(plan)) {
        plan = this.floorplan.checkActiveFloorplanForOutsideAndDrone(plan);

        if (!this.floorplan.isAvailableForCurrentFloor(plan)) {
            return;
        }
    }

    var floorplanMobileBlock = this.krpanoEmbeded.find("#floor-plan-tab-slider");
    floorplanMobileBlock.find(".floor-plane-tab.mobile.map-tab[href='#ss" + plan + "']").trigger("click");
};

ImmoviewerDefaultPlayer.prototype.handleMainEvents = function () {
    var that = this;

    this.krpanoEmbeded.on(this.detectTouchClickEvent, '.live-user-wrapper', function () {
        var activeGuestName = $(this).data('user-name');
        $('#chat-page-mobile').removeClass('hidden').toggle(true).find('.title').text(activeGuestName);
        $('#invite-participants-page-mobile').addClass('hidden').toggle(false);
    });

    this.krpanoEmbeded.on(this.detectTouchClickEvent, '#angle-left', function () {
        $('#invite-participants-page-mobile').removeClass('hidden').toggle(true);
    });

    this.krpanoEmbeded.on(this.detectTouchClickEvent, '.agent-info-header', function () {
        $('.agent-info-wrapper').toggleClass('active');
    });

    this.krpanoEmbeded.on(this.detectTouchClickEvent, '.mobile-page-switcher, #side-menu-btn', function (e) {
        $('#gallery-switcher, button.mobile-page-switcher, #side-menu-btn').addClass('hidden');
    });

    this.krpanoEmbeded.on(this.detectTouchClickEvent, '.mobile-page-switcher, .close-btn, #show-tour-list', function (e) {
        var pageToActivate = $(this).data('active-page');
        $('#' + pageToActivate).hasClass('hidden') ? $('#' + pageToActivate).removeClass('hidden').toggle(true)
                                                   : $('#' + pageToActivate).toggle();
        $('#mobile-side-menu, .side-menu-overflow-bg').removeClass('active');
    });

    this.krpanoEmbeded.on(this.detectTouchClickEvent, '#side-menu-btn', function () {
        $(this).addClass('hidden');
        $('#mobile-side-menu, .side-menu-overflow-bg').addClass('active');
    });

    this.krpanoEmbeded.on(this.detectTouchClickEvent, '.side-menu-overflow-bg', function () {
        $('#mobile-side-menu, .side-menu-overflow-bg').removeClass('active');
        $('#side-menu-btn, #gallery-switcher, .mobile-page-switcher:not(.side-menu-item), .mobile-page-switcher').removeClass('hidden');
    });

    this.krpanoEmbeded.on(this.detectTouchClickEvent, '#vrIcon', function () {
        $('.side-menu-overflow-bg').removeClass('active');
    });
};

ImmoviewerDefaultPlayer.prototype.setRightMenuEvents = function() {
    var instance = this;

    this.krpanoEmbeded.on(this.detectTouchClickEvent, '.close-right-menu', function() {
        $(this).parents('.right-menu').toggle("slide", { direction: 'right' });
        $('#side-menu-btn, #gallery-switcher').removeClass('hidden');
    });

    this.krpanoEmbeded.on(this.detectTouchClickEvent, '.close-right-menu, .close-btn', function() {
        $('.toggle-right-menu').removeClass('active-button');
        $('.contact-information-section, .infopoints-expose-section, ' +
          '#gallery-switcher, .mobile-page-switcher:not(.side-menu-item), #side-menu-btn').removeClass('hidden');
    });

    this.krpanoEmbeded.on(this.detectTouchClickEvent, '.infopoints-expose-section.block .infopoints', function() {
        instance.krpanoEmbeded.find('.info-point-list-section').toggle("slide", { direction: 'right' });
    });

    this.krpanoEmbeded.on(this.detectTouchClickEvent, '.infopoints-expose-section.block .expose', function() {
        instance.krpanoEmbeded.find('.expose-section').toggle("slide", { direction: 'right' });
    });

    this.krpanoEmbeded.on(this.detectTouchClickEvent, '.arrow', function() {
        $(this).find('.glyphicon').toggleClass('glyphicon glyphicon-chevron-right glyphicon glyphicon-chevron-left');
        $(this).siblings('.block').toggle("slide", { direction: 'right' });
    });

    this.krpanoEmbeded.on(this.detectTouchClickEvent, '.toggle-right-menu', function() {
        var activeSection = '#' + $(this).data('open');
        var activePage = instance.krpanoEmbeded.find(activeSection);

        if (!$(this).hasClass('active-button')) {
            $('.toggle-right-menu').removeClass('active-button');
            $(this).addClass('active-button');
            $('.contact-information-section, .infopoints-expose-section').addClass('hidden');
        } else  {
            $('.toggle-right-menu').removeClass('active-button');
            $('.contact-information-section, .infopoints-expose-section').removeClass('hidden');
        }

        if (activePage.hasClass('hidden')) {
            activePage.removeClass('hidden')
        } else {
            instance.krpanoEmbeded.find(activeSection).toggle("slide", { direction: 'right' });
        }
        $('.right-menu:not('+activeSection+'), .immoview-default:not('+activeSection+')').toggle(false).removeClass('hidden');
    });
};

/** krpano-content/krpano-wrapper-style/js/clipnow.player.tracking.js **/
var lastTrackTime = Date.now();

var sessionClosed = false;
//8 seconds
var panoDelayTime = 8000;
//20 seconds
var sessionTimeoutCheck=20000
//3min
var maxInactivity=180000

//Global variables which define the actual coordinates of the image
var timeoutDelay=0
var oldVlookat;
var oldHlookat;
var oldFov;
var oldSceneName;

function Tracking(krpano, welComeModal) {
    return{
        doTrack: doTrack,
        closeSession: closeSession,
        onStart: onStart
    };

    //After the tracking coordinates have been updated,
    //and when the image has changed or the position of the image has changed
    //this method will be triggered to perform a controller call and save in DB
    //the actual tracking value
    function doTrack(email) {
        sessionClosed=false;
        // Web Sockets would be better but require MSIE >= 10, so we use the old way for now:
        var actualFilename=$("#actualPano").prop('value')
        var actualTracking=$("#pano-" + actualFilename)
        var session = $("#session")
        var url=window.jsonData.urls.backendUrls.track.replace("%7BEMAIL%7D", window.jsonData.email && window.jsonData.email.length > 0 ? window.jsonData.email : "nobody");
        url = url.replace("%7BID%7D", actualTracking.prop("name") ? actualTracking.prop("name"): "");
        url = url.replace("%7BROOM_FILENAME%7D", actualFilename);
        url = url.replace("%7BSESSION_ID%7D", session.val())
        $.getJSON(url, function( data ) {
            timeoutDelay=0;
            actualTracking.prop("name", data.id);
            session.prop("value", data.sessionID);
        });
    }

    //Close session function
    function closeSession(){
        if(!sessionClosed){
            var email=$("#pbEmail").val()
            var actualTracking=$("#pano-" + $("#actualPano").prop('value'))
            var session = $("#session")
            var url=window.jsonData.urls.backendUrls.trackFinish.replace("%7BEMAIL%7D", window.jsonData.email && window.jsonData.email.length > 0 ? window.jsonData.email : "nobody");
            url = url.replace("%7BID%7D", actualTracking.prop("name") ? actualTracking.prop("name"): "");
            url = url.replace("%7BSESSION_ID%7D", session.val())
            $.getJSON(url, function( data ) {});
            actualTracking.prop("name","");
            $("#session").val("");
            sessionClosed=true
        }
    }

    function trackPanoEvent(){
        // This is a simple tracking/analytics code
       //var krpano = $("#krpanoSWFObject")[0];
       var sceneName = krpano.get("xml.scene");
       var vlookat = krpano.get("view.vlookat");
       var hlookat = krpano.get("view.hlookat");
       var fov = krpano.get("view.fov");
       // console.log("fov=" + fov + " hlookat=" + hlookat + " vlookat=" + vlookat + " sceneName=" + sceneName)
       var changedView = sceneName !== oldSceneName || vlookat !== oldVlookat || hlookat !== oldHlookat || oldFov !== fov;
       timeoutDelay = timeoutDelay + panoDelayTime;
       if (changedView) {
           var email = $("#pbEmail").val();
           if(email && welComeModal && !welComeModal.is(":visible")) {  // don't send too many requests...
               doTrack(email);
           }
       }
       oldVlookat = vlookat;
       oldHlookat = hlookat;
       oldFov = fov;
       oldSceneName = sceneName;
       window.setTimeout(trackPanoEvent, panoDelayTime);
    };

    function onStart(){
        //Callback triggered before the user close a tab/window
        $(window).on("beforeunload", function() {
            closeSession();
        })

        //Scheduled function (every 20sec)
        //which close the session after
        //an inactivity time of 3min
        window.setTimeout(timeoutSession, sessionTimeoutCheck);
        function timeoutSession() {
            if(timeoutDelay >= maxInactivity && !sessionClosed){
                closeSession()
            }
            window.setTimeout(timeoutSession, sessionTimeoutCheck);
        }

        //Scheduled function (every 8sec, starting at second 2)
        //which check if the image has moved (user interaction)
        //and updates the needed cursors consequently
        window.setTimeout(trackPanoEvent, 2000);
    }
}

function TrackingShareEvent(api, detectedEvent) {
    $(document).on(detectedEvent, '.share-page li.share a', function(e) {
        var eventType = 'UI_SHARE_'+ $(this).parents('.share').data('share-resource');
        api.sendTrackingEvent(eventType);
    });
}
/** krpano-content/krpano-wrapper-style/js/corrector.js **/
/*
 * pitch/roll camera values corrector functionality. Creates a grid + 2 inputs where user can change pitch and roll
 */
var Corrector = (function(krpano, plugin, lineMeasurement) {
    var savePitchRollUrl = "https://app.docusketch.com/rest/v1/tour/%tour_id%/pano/%panorama_id%/eulerAngles";

    var sBlock = ".image-settings";
    var sSuggest = ".suggestion";

    var pitch = window.jsonData.tour.activeRoom.pitch || 0;
    var roll = window.jsonData.tour.activeRoom.roll || 0;
    var calibrated = window.jsonData.tour.activeRoom.calibrated || false;

    var krpanoMagnifier = null;

    function addBlock() {
        $("body").append("<div class='image-settings'></div>");
        $(sBlock).html("" +
            "<label for=''>Pitch: </label>" +
            "<input type='number' class='image-settings__input image-settings__input-pitch form-control' step='0.1' " +
            "data-subject='pitch' value='" + pitch + "' />" +
            "<label for=''>Roll: </label>" +
            "<input type='number' class='image-settings__input image-settings__input-roll form-control' step='0.1' " +
            "data-subject='roll' value='" + roll + "' />" +
            "");
        if (calibrated) {
            $(".image-settings__input-pitch").prop("disabled", true);
            $(".image-settings__input-roll").prop("disabled", true);
            $(sBlock).append("<button class='js-calibrate image-settings__button btn btn-success'>Calibrate again</button>");
        }
    }
    function addHandlers() {
        function moveBack(pitch, roll, diffP, diffR) {
            var counter = 0, p0, p1;
            for (var i in lineMeasurement.arLines) {
                if (lineMeasurement.arLines[i].type === "floor" && counter < 3) {
                    p0 = krpano.get("hotspot[distanceFloorLine" + lineMeasurement.arLines[i].id + "].point[0]");
                    var point = rotate(p0.ath, p0.atv, 0, 0, -diffP, diffR);
                    krpano.set("hotspot[distanceFloorLine" + i + "].point[0].ath", point.ath);
                    krpano.set("hotspot[distanceFloorLine" + i + "].point[0].atv", point.atv);

                    p1 = krpano.get("hotspot[distanceFloorLine" + lineMeasurement.arLines[i].id + "].point[1]");
                    var point2 = rotate(p1.ath, p1.atv, 0, 0, -diffP, diffR);
                    krpano.set("hotspot[distanceFloorLine" + i + "].point[1].ath", point2.ath);
                    krpano.set("hotspot[distanceFloorLine" + i + "].point[1].atv", point2.atv);

                    counter++;
                }
            }
        }

        $(".image-settings__input-pitch").on("change", function() {
            var diff = +$(this).val() - pitch;

            pitch = $(this).val();
            krpano.set("image.prealign", (-pitch)+"|0|"+(-roll));
            krpano.call("updatescreen();");
            krpanoMagnifier.set("image.prealign", (-pitch)+"|0|"+(-roll));
            krpanoMagnifier.call("updatescreen();");

            moveBack(pitch, roll, diff, 0);
        });
        $(".image-settings__input-roll").on("change", function() {
            var diff = +$(this).val() - roll;

            roll = $(this).val();
            krpano.set("image.prealign", (-pitch)+"|0|"+(-roll));
            krpano.call("updatescreen();");
            krpanoMagnifier.set("image.prealign", (-pitch)+"|0|"+(-roll));
            krpanoMagnifier.call("updatescreen();");

            moveBack(pitch, roll, 0, diff);
        });
        $(".js-calibrate").on("click", function() {
            $(".image-settings__input-pitch").prop("disabled", false);
            $(".image-settings__input-roll").prop("disabled", false);
            $(this).remove();
        });
        $(sBlock).on("click", ".js-apply", function() { //apply suggestion and send to backend
            $(".js-apply").html('<span class="glyphicon glyphicon-time" aria-hidden="true"></span>');
            $.when(applyChanges(pitch, roll)).then(function() {
                $(".js-apply, .js-reset").remove();
                $(sSuggest).show();
                $(".suggestion__input").val("");
            });
        });
        $(sBlock).on("click", ".js-reset", function() {
            $(".js-apply, .js-reset").remove();
            $(sSuggest).show();
        });

        //update values in every new room
        $(document).on("krpano:loadscene", function() {
            pitch = window.jsonData.tour.activeRoom.pitch || 0;
            $(".image-settings__input-pitch").val(pitch);
            roll = window.jsonData.tour.activeRoom.roll || 0;
            $(".image-settings__input-roll").val(roll);

            //set in magnifier
            krpanoMagnifier.set("image.prealign", (-pitch)+"|0|"+(-roll));
            krpanoMagnifier.call("updatescreen();");
        });
    }
    function polar2carthesian(polar) {
        var carthesian = new THREE.Vector3();
        carthesian.x = Math.sin(THREE.Math.degToRad(90-polar.y)) * Math.cos(THREE.Math.degToRad(polar.x));
        carthesian.y = Math.sin(THREE.Math.degToRad(90-polar.y)) * Math.sin(THREE.Math.degToRad(polar.x));
        carthesian.z = Math.cos(THREE.Math.degToRad(90-polar.y));

        return carthesian;
    }

    function rotate(ath1, atv1, rot1, ath2, atv2, rot2) {
        // rotate a vector specified in spherical coordinates by another vector in spherical coordinates
        //console.log(ath1, atv1, rot1, ath2, atv2, rot2);

        var p = new THREE.Euler(THREE.Math.degToRad(rot1), THREE.Math.degToRad(atv1), THREE.Math.degToRad(ath1));
        var r = new THREE.Euler(THREE.Math.degToRad(rot2), THREE.Math.degToRad(atv2), THREE.Math.degToRad(ath2));
        var matrix = new THREE.Matrix4().makeRotationFromEuler(p);
        var matrix2 = new THREE.Matrix4().makeRotationFromEuler(r);
        matrix.multiply(matrix2);
        var euler = new THREE.Euler().setFromRotationMatrix(matrix, "XYZ");
        return {
            "euler": euler,
            "ath": THREE.Math.radToDeg(euler.z),
            "atv": THREE.Math.radToDeg(euler.y)
        }
    }
    function addGrid() {
        var sGrid = ".grid",
            w = $(window).width(),
            h = $(window).height();

        $("body").append("<div class='grid'></div>");
        for (var i=0, k=0; i<10; k+=w/10, i++) {
            $(sGrid).append("<div class='grid__item' style='top: "+k+"px;'></div>");
        }
        $(sGrid).append("<div class='grid__item grid__item-vert'></div>");
    }
    function addStitches() {
        function addLine(nameIndex, color, horiz, vert) {
            vert = vert || 50;
            krpano.call("addhotspot(" + nameIndex + ")");

            krpano.set('hotspot[' + nameIndex + '].point[0].ath', horiz);
            krpano.set('hotspot[' + nameIndex + '].point[0].atv', -vert);
            krpano.set('hotspot[' + nameIndex + '].point[1].ath', horiz);
            krpano.set('hotspot[' + nameIndex + '].point[1].atv', vert);

            krpano.set('hotspot[' + nameIndex + '].keep', false);
            krpano.set('hotspot[' + nameIndex + '].handcursor', false);
            krpano.set('hotspot[' + nameIndex + '].visible', true);
            krpano.set('hotspot[' + nameIndex + '].enabled', true);
            krpano.set('hotspot[' + nameIndex + '].alpha', '0.4');
            krpano.set('hotspot[' + nameIndex + '].bordercolor', color);
            krpano.set('hotspot[' + nameIndex + '].borderwidth', "2.0");
            krpano.set('hotspot[' + nameIndex + '].borderalpha', "0.5");
            krpano.set('hotspot[' + nameIndex + '].fillalpha', "1.0");
            krpano.set('hotspot[' + nameIndex + '].fillcolor', color);
            krpano.set('hotspot[' + nameIndex + '].polyline', true);
        }
        function addField(nameIndex, color, horiz) {
            addLine(nameIndex, color, horiz[0], 80);
            krpano.set('hotspot[' + nameIndex + '].point[2].ath', horiz[1]);
            krpano.set('hotspot[' + nameIndex + '].point[2].atv', 80);
            krpano.set('hotspot[' + nameIndex + '].point[3].ath', horiz[1]);
            krpano.set('hotspot[' + nameIndex + '].point[3].atv', -80);
            krpano.set('hotspot[' + nameIndex + '].point[4].ath', horiz[0]);
            krpano.set('hotspot[' + nameIndex + '].point[4].atv', -80);
            krpano.set('hotspot[' + nameIndex + '].fillcolor', color);
            krpano.set('hotspot[' + nameIndex + '].fillalpha', 0.15);
            krpano.set('hotspot[' + nameIndex + '].polyline', false);
            krpano.set('hotspot[' + nameIndex + '].capture', false);
        }

        addLine("stitch0", "0x000000", 0);
        addLine("stitch1", "0x000000", 180);
        addLine("stitch2", "0x0000ff", -90);
        addLine("stitch3", "0x0000ff", 90);
        addField("field0", "0x00ff00", [-45, 45]);
        addField("field1", "0x00ff00", [135, 225]);
    }
    function addMagnifier() {
        $("body").append("<div class='magnifier' id='magnifier'><div class='magnifier__cross'></div></div>");

        embedpano({
            swf: krpano.__swfpath + "krpano.swf",
            js: krpano.__swfpath + "krpano.js",
            xml: window.baseUrl + "/krpano-wrapper-style/hardcoded-content/magnifier.xml",
            target: "magnifier",
            id: "krpanoSWFObject_magnifier",
            html5: "prefer",
            webglsettings:{depth:true},
            passQueryParameters: true,
            wmode: "opaque",
            onready: magnifierChangeScene,
            consolelog: true
        });

        $(window).mousemove( function( event ) {
            var msg = "Handler for .mousemove() called at ";
            msg += event.pageX + ", " + event.pageY;
            var mousex = krpano.get( "mouse.x" );
            var mousey = krpano.get( "mouse.y" );
            var mousePos = krpano.screentosphere( mousex, mousey );
            krpanoMagnifier.set( "view.hlookat", mousePos.x );
            krpanoMagnifier.set( "view.vlookat", mousePos.y );
            krpanoMagnifier.set( "view.fov", 12 );
        } );
    }
    function magnifierChangeScene(krpano){
        if (!krpanoMagnifier) krpanoMagnifier = document.getElementById( "krpanoSWFObject_magnifier" ); // the magnifier
        krpanoMagnifier.call( "setimage(" + window.jsonData.tour.activeRoom.url + ");" );
    }
    function addSuggester() {
        $(sBlock).append(
            "<div class='suggestion'>" +
            "<button class='suggestion__btn btn btn-success'>Suggest</button>" +
            "<input class='suggestion__input suggestion__input-w1 form-control' placeholder='first'" +
            "   data-toggle='tooltip' data-placement='bottom' title='Provide a real length in fts for first measurement in a list' />" +
            "<input class='suggestion__input suggestion__input-w2 form-control' placeholder='second' " +
            "   data-toggle='tooltip' data-placement='bottom' title='Same for second measurement' />" +
            "<input class='suggestion__input suggestion__input-w3 form-control' placeholder='third' " +
            "   data-toggle='tooltip' data-placement='bottom' title='And also for third one.' />" +
            "</div>");
        $(sSuggest).find("button, input").attr("disabled", true).css("opacity", 0.3);
        $(".suggestion__btn").on("click", function (e) {
            e.preventDefault();
            var w1 = parseFloat($(".suggestion__input-w1").val());
            var w2 = parseFloat($(".suggestion__input-w2").val());
            var w3 = parseFloat($(".suggestion__input-w3").val());
            if (w1 > 0 && w2 > 0) suggest(pitch, roll, [w1, w2, w3]);
            else console.info("no real wall lengths. Please fill in them");
        });
        $(".suggestion__input").tooltip({'trigger':'focus'});

        //activate suggester when 2lines are available
        $(lineMeasurement.container).on("click dblclick", function() {
            if (lineMeasurement.floorLinesCount >= 2) {
                $(sSuggest).find("button, input")
                    .removeAttr("disabled")
                    .css("opacity", 1.0);
            }
        });
    }
    function suggest(defaultPitch, defaultRoll, wall1Lengths) {
        function calcLengthWithPitchRoll(pp1, pp2, pitch, roll, lensHeight) {
            var _p1 = rotate(pp1.ath, pp1.atv, 0, 0, pitch, roll);
            var _p2 = rotate(pp2.ath, pp2.atv, 0, 0, pitch, roll);
            distances = {
                start: getFloorDistanceFromLens(_p1, lensHeight),
                end: getFloorDistanceFromLens(_p2, lensHeight)
            };
            horizCoords = {start: _p1, end: _p2};
            var dist = twodwiew.calcLengthBetweenTwoPoints(distances, horizCoords);
            if (lineMeasurement.lensHeightUnit === "ft") dist /= 12;

            return dist;
        }

        var dispertion = 10, lengthValues = {0: [], 1: [], 2: []},
            shifts = [], iter=0, lensHeight = lineMeasurement.getLensHeight(),
            distances, horizCoords, pt = {0: [], 1: [], 2: []};

        var counter = 0;
        for (var n in lineMeasurement.arLines) {
            if (lineMeasurement.arLines[n].type === "floor" && counter < 3) {
                pt[counter][0] = krpano.get("hotspot[distanceFloorLine" + lineMeasurement.arLines[n].id + "].point[0]");
                pt[counter][1] = krpano.get("hotspot[distanceFloorLine" + lineMeasurement.arLines[n].id + "].point[1]");
                counter++;
            }
        }

        for (var p=defaultPitch-dispertion/2, diffP=p-defaultPitch; p<=defaultPitch+dispertion/2; p+=0.1, diffP=p-defaultPitch) {
            for (var r=defaultRoll-dispertion/2, diffR=r-defaultRoll; r<=defaultRoll+dispertion/2; r+=0.1, diffR=r-defaultRoll) {
                shifts.push({pitch: p, roll: r});

                //calc first wall's length
                lengthValues[0].push({index: iter, len: calcLengthWithPitchRoll(pt[0][0], pt[0][1], -diffP, diffR, lensHeight)});
                lengthValues[1].push({index: iter, len: calcLengthWithPitchRoll(pt[1][0], pt[1][1], -diffP, diffR, lensHeight)});

                if (pt[2][0]) lengthValues[2].push({index: iter, len: calcLengthWithPitchRoll(pt[2][0], pt[2][1], -diffP, diffR, lensHeight)});

                console.log(
                    format(p),
                    format(r),
                    format(lengthValues[0][iter].len, 3),
                    format(lengthValues[1][iter].len, 3)
                );

                iter++;
            }
        }

        for (var i=0;i<2;i++) {
            lengthValues[i] = sortByClosenessToNumber(wall1Lengths[i], lengthValues[i]);
            lengthValues[i].map(function(obj) {
                obj.pitch = shifts[obj.index].pitch;
                obj.roll = shifts[obj.index].roll;
                return obj;
            });
        }

        findClosestRotations(shifts, lengthValues);
    }
    function sortByClosenessToNumber(num, arr) {
        arr.sort(function(a, b) {
            return Math.abs(num-a.len) - Math.abs(num-b.len);
        });
        arr = arr.filter(function(item) { //remove values which are not close to num
            return Math.abs(num - item.len) <= 1;
        });
        return arr;
    }

    /* Find ideal pitch/roll pair
     * @param {object} rotations an object with pitch/roll info
     * @param {object} walls
     */
    function findClosestRotations(rotations, walls) {
        var minSum = Number.MAX_VALUE, minIndexFound, len1, len2, len3, tolerance = 0.2;
        for (var i=0; i<walls[0].length; i++) {
            for (var k=0; k<walls[1].length; k++) {
                if (walls[2] && walls[2].length) { //3 walls defined
                    for (var m=0; m<walls[2].length; m++) {
                        //console.log("pitch diff: " + Math.abs(rotations[walls[0][i].index].pitch - rotations[walls[1][k].index].pitch));
                        //console.log("roll diff: " + Math.abs(rotations[walls[0][i].index].roll - rotations[walls[1][k].index].roll));
                        if (
                            Math.abs(rotations[walls[0][i].index].pitch - rotations[walls[1][k].index].pitch) < tolerance &&
                            Math.abs(rotations[walls[0][i].index].roll - rotations[walls[1][k].index].roll) < tolerance &&

                            Math.abs(rotations[walls[0][i].index].pitch - rotations[walls[2][m].index].pitch) < tolerance &&
                            Math.abs(rotations[walls[0][i].index].roll - rotations[walls[2][m].index].roll) < tolerance &&

                            Math.abs(rotations[walls[1][k].index].pitch - rotations[walls[2][m].index].pitch) < tolerance &&
                            Math.abs(rotations[walls[1][k].index].roll - rotations[walls[2][m].index].roll) < tolerance
                        ) {
                            if (i + k + m < minSum) {
                                minSum = i + k + m;
                                minIndexFound = walls[0][i].index;
                                len1 = walls[0][i].len;
                                len2 = walls[1][k].len;
                                len3 = walls[2][m].len;
                            }
                        }
                    }
                } else { //only 2 walls defined
                    if (Math.abs(rotations[walls[0][i].index].pitch - rotations[walls[1][k].index].pitch) < tolerance &&
                        Math.abs(rotations[walls[0][i].index].roll - rotations[walls[1][k].index].roll) < tolerance) {
                        if (i + k < minSum) {
                            minSum = i + k;
                            minIndexFound = walls[0][i].index;
                            len1 = walls[0][i].len;
                            len2 = walls[1][k].len;
                        }
                    }
                }
            }
        }
        if (minIndexFound) {
            $(".image-settings__input-pitch").val(format(rotations[minIndexFound].pitch)).trigger("change");
            $(".image-settings__input-roll").val(format(rotations[minIndexFound].roll)).trigger("change");
            addApplyResetButtons();
            console.log(len1);
            console.log(len2);
            if (len3) console.log(len3);
        } else {
            alert("pitch/roll pair not found.");
        }
    }

    function addApplyResetButtons() {
        $(sSuggest).hide();
        $(sBlock)
            .append("<button class='js-apply image-settings__button btn btn-success'>Apply</button>")
            .append("<button class='js-reset image-settings__button btn btn-danger'>Once again</button>");
    }

    function applyChanges(pitch, roll) {
        var token, preparedUrl, dfd = new $.Deferred();

        if (token = (new UrlParam()).get("authToken")) {
            preparedUrl = savePitchRollUrl
                .replace("%tour_id%", window.jsonData.tour.id)
                .replace("%panorama_id%", window.jsonData.tour.activeRoom.filename);
            $.ajax({
                url: preparedUrl,
                type: "PUT",
                data: JSON.stringify({pitch: pitch, roll: roll}),
                contentType: "application/json",
                dataType: "json",
                headers: {
                    "Authorization": "Bearer " + token
                }
            }).then(function(data) {
                dfd.resolve();
            });
        }
        return dfd;
    }

    function format(num, decimals) {
        if (!decimals) decimals = 2;
        var divider = Math.pow(10, decimals);
        return Math.round(num * divider) / divider;
    }

    //todo: move out from there
    function loadScript(url, callback) {
        var head = document.getElementsByTagName('head')[0];
        var script = document.createElement('script');
        script.type = 'text/javascript';
        script.src = url;

        script.onreadystatechange = callback;
        script.onload = callback;

        head.appendChild(script);
    }

    //init
    plugin.disablerotate();

    loadScript(window.baseUrl + "/krpano-common/js/threejs/three.js");
    addBlock(); //add block with buttons
    addHandlers();
    addGrid();
    addMagnifier();
    addSuggester();
    krpano.events.onloadcomplete = function() {
        addStitches();
        magnifierChangeScene();
    };
});
/** krpano-content/krpano-wrapper-style/js/floormodel.js **/
'use strict';

/**
 * Representation of a 3d model of a tour
 * @class
 * @param  {object} _krpano
 * @param  {object} _plugin
 * @param  {object} _interaction
 */
const FloorModel = function (_krpano, _plugin, _interaction) {
    /**
     * Transition data
     */
    this.transition = {
        status: false, // false - stopped | true - started
        shader: null,
        object: null,
        innerJump: false, // flag showing if a 3d transition for jump is used. false means a usual krpano transition from xml
        shaderInRender: false,
        lastJumpTimestamp: 0,
        transitionDelay: 100
    };

    /**
     * KRpano data
     */
    this.krpano = {
        clicked: false, // krpano's "currently in action" flag
        object: _krpano,
        device: _krpano.device,
        plugin: _plugin,
        interaction: _interaction,
        panoview: null,
        projection: new Float32Array(16),
        depthBufferScale: 1.0001, // krpano projection matrix
        depthBufferOffset: -0.2, // depthbuffer scaling (use ThreeJS defaults: znear=0.1, zfar=2000)
    };

    /**
     * ThreeJS data
     */
    this.threeJS = {
        splitted: false,
        canvas: null
    }

    /**
     * FloorModel data
     */
    this.data = {
        rooms: {}, // loaded from json in buildScene() below
        arrows: {}, // arrows objects
        ladders: {}, // ladder arrows
        wallsWithoutHoles: [],
        mouse: {
            downX: 0,
            downY: 0
        },
        isTouchDevice: false,
        config: {
            debug: false,
            keepShaderOn: false,
            useProxy: false,
            proxyUrl: 'http://cors-anywhere.herokuapp.com/',
            dollhouse: this.krpano.object.dollhouse === '1',
            // transitions: this.krpano.object.transitions !== '0' && 0,
            useShader: this.krpano.object.useshader !== '0',
            loadTextures: this.krpano.object.loadtextures !== '0',
            textureSize:
                isNaN(parseInt(this.krpano.object.texturesize)) ?
                    256 : // Default
                    parseInt(this.krpano.object.texturesize)
        },
        time: 0,
        animations: [], // animations stack
        fps: {
            value: 0
        },
        currentUrl: null,
        jsonPath: '', // path to json with cameras info
        objPath: '', // path to model
        firstRoom: '' || window.jsonData.tour.activeRoom.filename.slice(0),
        modelAvalible: false,
        renderer: null,
        firstFloor: null,
        lastFloorId: null,
        scriptsLoaded: false,
        krpanoAlternativeLocation: {x: 0, y: 0, z: 0},
        krpanoAlternativeStartLocation: {x: 0, y: 0, z: 0},
        textures: {
            preload: true,
            cache: {},
            loaded: false, // when we don't use preloading, we automatically decide that textures are loaded
            deferred: $.Deferred()
        },
        events: {
            initialLoadImage: null // krpano onloadcomplete action code
        },
        isClickEventBound: false
    };

    /**
     * Main 3D walls
     */
    this.main = {
        object3D: null,
        group: null,
        scene: null,
        camera: null,
        stereoCamera: null,
        raycaster: null,
        krpanoPanoviewEuler: null,
        wrapper: null, // object3D ?
        shaderSphere: null,
        cursor: {
            object: null,
            circleActiveMesh: null,
            arrowActiveMesh: null,
            lastRotation: null,
            worldNormal: null,
            worldPosition: null,
            isFloorHovered: false
        },
        boxes: [], // bounding boxes (for what?)
    }

    /**
     * Small dollhouse in corner
     */
    this.doll = {
        object3D: null,
        group: null,
        scene: null,
        camera: null,
        raycaster: null,
        krpanoPanoviewEuler: null,
        placeInteracted: false,
        viewState: 'small',
        visible: false,
        viewPoint: {
            config: {
                models: {
                    active: null,
                    inactive: null
                },
                materials: {
                    active: null,
                    hovered: null,
                    inactive: null
                }
            },
            active: null,
            activeModels: null,
            inactiveModels: null,
            labels: {}, // labels on hovering view points
            labelsRotation: 0
        },
        coefficents: {
            smallHeight: 0.35,
            smallWidth: 0.35,
            smallTop: 0.55,
            smallLeft: -0.05,
            bigHeight: 0.7,
            bigWidth: 0.7,
            bigTop: 0.20,
            bigLeft: -0.1,
            currentHeight: 0,
            currentWidth: 0,
            currentTop: 0,
            currentLeft: 0,
        }
    };

    /**
     * Initial function
     */
    this.init = function () {
        if (this.isAvailable()) {
            window._floorModelEnabled = true;

            if (window._pano) window._pano.floorModel = this;
            this.data.isTouchDevice = this.isTouchDevice();
            this.resetLoadImageEvent();
            $.when(this.preloadStuff()).then(function () {
                this.setDefaultDollViewPointMaterials();
                this.callLoadImageEvent();
                this.handleChangeFloor();
                this.onSceneChange();
                // $(document).trigger("krpano:loadscene");
            }.bind(this));
            this.setDollViewResponsive();
            this.initFpsMeter();
        } else {
            window._floorModelEnabled = false;
        }
    };

    /**
     * Are all things("use3DPlayer" flag checked, model and cameras json are present) ready to start 3d player mode?
     * @return {boolean}
     */
    this.isAvailable = function() {
        var isLive = ['LIVE_MASTER', 'LIVE_SLAVE', 'LIVE_BACKMONITOR'].indexOf(jsonData.playerMode) !== -1;
        var atLeastOneModelIsAvailable = false;
        for (var key in window.jsonData.tour.maps) {
            if ("model3DUrl" in window.jsonData.tour.maps[key]) atLeastOneModelIsAvailable = true;
            //todo possibly we also need to check if json with cameras is present (camerasInfoUrl)
        }
        return !isLive &&
            (this.krpano.testmodel === "1" || ("use3DPlayer" in window.jsonData.visibility && window.jsonData.visibility.use3DPlayer === true)) &&
            atLeastOneModelIsAvailable;
    }
    
    /**
     * Get touch device
     * @returns {boolean}
     */
    this.isTouchDevice = function () {
        var prefixes = ' -webkit- -moz- -o- -ms- '.split(' ');
        var mq = function(query) {
            return window.matchMedia(query).matches;
        }
        
        if (('ontouchstart' in window) || window.DocumentTouch && document instanceof DocumentTouch) {
            return true;
        }
      
        // include the 'heartz' as a way to have a non matching MQ to help terminate the join
        // https://git.io/vznFH
        var query = ['(', prefixes.join('touch-enabled),('), 'heartz', ')'].join('');
        return !!mq(query);
    }

    /**
     * Set doll visibility
     * @param  {boolean} visible
     */
    this.setDollVisibility = function (visible) {
        this.doll.visible = visible;
        return this;
    }

    this.hideWallsWithoutHoles = function () {
        this.data.wallsWithoutHoles.forEach(function (item) {
            item.without.visible = false;
        });
        return this;
    }

    this.showWallsWithoutHoles = function () {
        return this; // disabled
        this.data.wallsWithoutHoles.forEach(function (item) {
            item.without.visible = true;
        });
        return this;
    }

    /**
     * Change sizes by window size
     */
    this.setDollViewResponsive = function () {
        var gl = this.krpano.object.webGL.context;
        var sw = gl.drawingBufferWidth;
        var sh = gl.drawingBufferHeight;

        this.setDollViewCoefficents({
            height: 0.35,
            width: 0.35,
            top: (1 - 0.35) - (1 / (sh / 70)) ,
            left: (-0.35 / 3) + (1 / (sw / 60)),
        }, {
            height: 0.8,
            width: 0.8,
            top: (1 - (0.8)) - (1 / (sh / 70)),
            left: (-0.8 / 4) + (1 / (sw / 60)),
        });

        return this;
    }

    /**
     * Set coefficents for small and big view of doll
     * @param  {object} small
     * @param  {object} big
     */
    this.setDollViewCoefficents = function (small, big) {
        this.doll.coefficents.smallHeight = small.height;
        this.doll.coefficents.smallWidth = small.width;
        this.doll.coefficents.smallTop = small.top;
        this.doll.coefficents.smallLeft = small.left;

        this.doll.coefficents.bigHeight = big.height;
        this.doll.coefficents.bigWidth = big.width;
        this.doll.coefficents.bigTop = big.top;
        this.doll.coefficents.bigLeft = big.left;

        // current
        this.doll.coefficents.currentHeight = this.doll.coefficents.smallHeight;
        this.doll.coefficents.currentWidth = this.doll.coefficents.smallWidth;
        this.doll.coefficents.currentTop = this.doll.coefficents.smallTop;
        this.doll.coefficents.currentLeft = this.doll.coefficents.smallLeft;

        this.doll.viewState = 'small';

        return this;
    }

    /**
     * Find first floor
     */
    this.findFirstFloor = function () {
        this.data.firstFloor = null;

        Object.keys(jsonData.tour.maps).map(function (i) {
            if (!this.data.firstFloor && parseInt(i) >= 0 && jsonData.tour.maps[i].model3DUrl) {
                this.data.firstFloor = jsonData.tour.maps[i];
            }
        }.bind(this));

        return this.data.firstFloor;
    };

    /**
     * Set default doll view point materials
     */
    this.setDefaultDollViewPointMaterials = function () {
        this.doll.viewPoint.config.materials = {
            active: new THREE.MeshLambertMaterial({color: 0xFEFE0C, transparent: true, opacity: 0.9}),
            hovered: new THREE.MeshLambertMaterial({color: 0x1a6bce, transparent: true, opacity: 0.9}),
            inactive: new THREE.MeshLambertMaterial({color: 0x017BB4, transparent: true, opacity: 0.75})
        };
    }

    /**
     * Set a flag that krpano is requesting an action(click), meaning that we shouldn't handle a click for model
     */
    this.setKrpanoInAction = function () {
        this.krpano.clicked = true;
    }

    /**
     * Alias for setKrpanoInAction method
     */
    this.setkrpanoinaction = function () {
        //return this.setKrpanoInAction(); //this scope is not available :(
    }

    /**
     * Set status of krpano clicked to false
     */
    this.setKrpanoNotInAction = function () {
        this.krpano.clicked = false;
    }

    /**
     * Get status of krpano clicked
     */
    this.isKrpanoInAction = function () {
        return this.krpano.clicked;
    }

    /**
     * Init FPS meter for perfomance look
     */
    this.initFpsMeter = function () {
        this.data.fps.filter = 20;
        this.data.fps.frameTime = 0;
        this.data.fps.lastLoop = new Date;
        this.data.fps.currentLoop = null;

        this.data.fps.loop = function () {
            var currentFrameTime = (this.data.fps.currentLoop = new Date) - this.data.fps.lastLoop;

            this.data.fps.frameTime += (currentFrameTime - this.data.fps.frameTime) / this.data.fps.filter;
            this.data.fps.lastLoop = this.data.fps.currentLoop;

            // compute this.data.fps
            this.data.fps.value = 1000 / this.data.fps.frameTime;

            if (this.data.config.debug) {
                window._fps = this.data.fps.value;
            }

            requestAnimationFrame(this.data.fps.loop);
        }.bind(this);

        this.data.fps.loop();
    }

    /**
     * Create labels on doll for rooms
     */
    this.createLabels = function () {
        var container = document.getElementById('krpanoSWFObject');

        function createLabel (text) {
            var div = document.createElement('div');
            div.className = 'froormodel-doll-label';
            div.innerHTML = text;
            div.style.position = 'absolute';
            div.style.width = 100;
            div.style.height = 100;
            div.style.top = -1000;
            div.style.left = -1000;
            div.style.zIndex = 1000000;
            div.style.pointerEvents = "none";
            div.style.backgroundColor = '#fff';
            div.style.padding = '3px';
            div.style.opacity = "0";
            div.style.transition = "opacity 0.5s ease-in-out"; //todo: move to css styles

            return {
                element: div,
                parent: false,
                position: new THREE.Vector3(0, 0, 0),
                visible: false,
                arrow: null,
                setHTML: function (html) {
                    this.element.innerHTML = html;
                },
                setParent: function (threeJsObject) {
                    this.parent = threeJsObject;
                },
                showArrow: function () {
                    if (this.arrow) {
                        this.doll.scene.remove(this.arrow);
                    }

                    this.arrow = new THREE.ArrowHelper(
                        this.position,
                        new THREE.Vector3(0, 0, 0),
                        0.3
                    );

                    this.doll.scene.add(this.arrow);
                },
                updatePositionByCoordinates: function (x, y) {
                    this.element.style.left = x + 'px';
                    this.element.style.top = y + 'px';
                },
                updatePositionByProjection: function (cam, w, h) {
                    if (parent) {
                        this.position.copy(this.parent.position);
                    }

                    var coords2d = this.get2DCoords(this.position, cam, w, h);

                    this.element.style.left = coords2d.x + 'px';
                    this.element.style.top = coords2d.y + 'px';
                },
                get2DCoords: function (position, cam, w, h) {
                    var vector = position.project(cam);

                    vector.applyAxisAngle(
                        new THREE.Vector3(0, 0, 1),
                        this.doll.viewPoint.labelsRotation // rooms[this.data.currentUrl].rotation
                    );

                    // this.showArrow();

                    vector.x = ((vector.x + 1) / 2) * this.doll.coefficents.currentWidth * w + (w * this.doll.coefficents.currentLeft);
                    vector.y = (-(vector.y - 1) / 2) * this.doll.coefficents.currentHeight * h + (h * this.doll.coefficents.currentTop);

                    return vector;
                },
                show: function () {
                    this.visible = true;
                    this.element.style.opacity = "1";
                },
                hide: function () {
                    this.visible = false;
                    this.element.style.opacity = "0";
                }
            };
        }

        if (this.doll.viewPoint.inactiveModels) {
            for (var index in this.doll.viewPoint.inactiveModels.children) {
                var label = createLabel(this.doll.viewPoint.inactiveModels.children[index].userData.label);
                label.setParent(this.doll.viewPoint.inactiveModels.children[index]);

                container.appendChild(label.element);
                this.doll.viewPoint.labels[this.doll.viewPoint.inactiveModels.children[index].userData.name] = label;
            }
        }
    }

    /**
     * Remove labels on doll for rooms
     */
    this.removeLabels = function () {
        Object.keys(this.doll.viewPoint.labels).map(function (k) {
            if (this.doll.viewPoint.labels[k]) {
                this.doll.viewPoint.labels[k].element.remove();
                this.doll.viewPoint.labels[k] = null;
            }
        }.bind(this));
    }

    /**
     * Update label screen position on doll
     * @param  {string} id
     * @param  {number} x
     * @param  {number} y
     */
    this.updateLabelPositionByMouse = function (id, x, y) {
        if (this.doll.viewPoint.labels[id]) {
            const elHeight = this.doll.viewPoint.labels[id].element.offsetHeight;
            const elWidth = this.doll.viewPoint.labels[id].element.offsetWidth;
            this.doll.viewPoint.labels[id].updatePositionByCoordinates(x - elWidth / 2 + 6, y - elHeight - 3);
        }
    }

    /**
     * Update label screen position on doll by screen size
     * @param  {number} width
     * @param  {number} height
     */
    this.updateLabelsPosition = function (width, height) {
        Object.keys(this.doll.viewPoint.labels).map(function (k) {
            if (this.doll.viewPoint.labels[k].visible) {
                this.doll.viewPoint.labels[k].updatePositionByProjection(this.doll.camera, width, height);
            }
        });
    }

    /**
     * Show label
     * @param  {string} id
     */
    this.showLabel = function (id) {
        if (this.doll.viewPoint.labels[id]) {
            if (!this.doll.viewPoint.labels[id].visible) {
                this.doll.viewPoint.labels[id].show();
            }
        }
    }

    /**
     * Hide all labels
     */
    this.hideAllLabels = function () {
        Object.keys(this.doll.viewPoint.labels).map(function (k) {
            if (this.doll.viewPoint.labels[k] && this.doll.viewPoint.labels[k].visible) {
                this.doll.viewPoint.labels[k].hide();
            }
        }.bind(this));
    }

    /**
     * turn off krpano onloadcomplete action (but save internally)
     */
    this.resetLoadImageEvent = function () {
        var arEvents = this.krpano.object.get("events").getArray();
        this.data.events.initialLoadImage = arEvents[1].onloadcomplete.slice(0);
        arEvents[1].onloadcomplete = "";
    }

    /**
     * restore and call krpano onloadcomplete action
     */
    this.callLoadImageEvent = function () {
        var arEvents = this.krpano.object.get("events").getArray();
        arEvents[1].onloadcomplete = this.data.events.initialLoadImage;
        this.krpano.object.call(this.data.events.initialLoadImage);
    }

    /**
     * Get relative url of asset
     * @param {sring} url
     */
    this.resolveUrlPath = function (url) {
        if (url.charAt(0) !== "." && url.indexOf("://") < 0) {
            // adjust relative url path
            url = window.baseUrl + url;
        }

        return url;
    }

    /**
     * Load scripts for 3d model
     * @param  {array} urls
     * @param  {function} callback
     */
    this.loadScripts = function (urls, callback) {
        if (urls.length > 0) {
            var url = this.resolveUrlPath( urls.splice(0,1)[0] );

            var script = document.createElement("script");
            script.src = url;
            script.addEventListener("load", function(){this.loadScripts(urls,callback);}.bind(this));
            script.addEventListener("error", function(){this.krpano.object.trace(3,"loading file '"+url+"' failed!");});
            document.getElementsByTagName("head")[0].appendChild(script);
        } else {
            // done
            callback();
        }
    }

    /**
     * Start 3D viewer
     */
    this.start = function () {
        // create the ThreeJS WebGL renderer, but use the WebGL context from krpano
        if (this.threeJS.splitted) {
            this.data.renderer = new THREE.WebGLRenderer({
                canvas: this.threeJS.canvas,
                alpha: true,
                // preserveDrawingBuffer: true
            });

            var previousSizes = {
                w: 0,
                h: 0
            };

            var resizeFunction = function () {
                setTimeout(function () {
                    var krpanoCanvas = this.krpano.object.webGL.canvas;
                    var sw = krpanoCanvas.width;
                    var sh = krpanoCanvas.height;

                    // var gl = this.krpano.object.webGL.context;
                    // var sw = gl.drawingBufferWidth;
                    // var sh = gl.drawingBufferHeight;

                    if (previousSizes.w !== sw || previousSizes.h !== sh) {
                        console.log('FloorModel size', sw, sh);

                        previousSizes.w = sw;
                        previousSizes.h = sh;

                        this.data.renderer.setSize(sw, sh, false);
                    }
                }.bind(this), 10);
            }.bind(this);

            setInterval(function () {
                resizeFunction();
            }, 250);

            window.addEventListener('resize', resizeFunction, false);
            this.data.renderer.autoClear = true;
        } else {
            this.data.renderer = new THREE.WebGLRenderer({
                canvas: this.krpano.object.webGL.canvas,
                context: this.krpano.object.webGL.context
            });

            this.data.renderer.autoClear = false;
        }

        this.data.renderer.setPixelRatio(1); // krpano handles the pixel ratio scaling

        // restore the krpano WebGL settings (for correct krpano rendering)
        this.restoreKrpanoWebGLState();

        // use the krpano onviewchanged event as render-frame callback (this event will be directly called after the krpano pano rendering)
        this.krpano.object.set("events[__threejs__].keep", true);
        this.krpano.object.set("events[__threejs__].onviewchange", this.adjustKrpanoRendering.bind(this));	// correct krpano view settings before the rendering
        this.krpano.object.set("events[__threejs__].onviewchanged", this.renderFrame.bind(this));

        // enable continuous rendering (that means render every frame, not just when the view has changed)
        this.krpano.object.view.continuousupdates = true;

        // register mouse and touch events
        if (this.krpano.device.browser.events.mouse && !this.data.isClickEventBound) {
            this.krpano.object.control.layer.addEventListener(
                "mousedown",
                this.handleMouseTouchEvents.bind(this),
                true
            );
            this.data.isClickEventBound = true;
        }
        if (this.krpano.device.browser.events.touch && !this.data.isClickEventBound) {
            this.krpano.object.control.layer.addEventListener(
                this.krpano.device.browser.events.touchstart,
                this.handleMouseTouchEvents.bind(this),
                true
            );
            this.data.isClickEventBound = true;
        }

        this.krpano.plugin.disablerotate();

        // basic ThreeJS objects
        this.main.scene = new THREE.Scene();
        this.main.camera = new THREE.Camera();
        this.main.stereoCamera = new THREE.Camera();
        this.main.raycaster = new THREE.Raycaster();
        this.main.krpanoPanoviewEuler = new THREE.Euler();

        if (this.data.config.dollhouse) {
            this.doll.scene = new THREE.Scene();
            this.doll.camera = new THREE.Camera();
            this.doll.raycaster = new THREE.Raycaster();
            this.doll.krpanoPanoviewEuler = new THREE.Euler();

            var dollLight = new THREE.HemisphereLight(0xffffff, 0x080820, 1);
            this.doll.scene.add(dollLight);

            var dollCameraBackRay = new THREE.Object3D;
            this.doll.camera.add(dollCameraBackRay);
            dollCameraBackRay.position.z = -1;
        }

        this.handleRoomChange();

        // build the ThreeJS scene (start adding custom code there)
        this.buildScene();

        // restore the krpano WebGL settings (for correct krpano rendering)
        this.restoreKrpanoWebGLState();

        this.transition.status = true;
    }

    /* Three.js in krpano stuff. start */

    /**
     * Restore Krpano WebGL State
     */
    this.restoreKrpanoWebGLState = function () {
        if (this.threeJS.splitted) return;

        var gl = this.krpano.object.webGL.context;

        gl.disable(gl.DEPTH_TEST);
        gl.cullFace(gl.FRONT);
        gl.frontFace(gl.CCW);
        gl.enable(gl.BLEND);
        gl.blendFunc(gl.SRC_ALPHA, gl.ONE_MINUS_SRC_ALPHA);
        gl.activeTexture(gl.TEXTURE0);
        gl.pixelStorei(gl.UNPACK_FLIP_Y_WEBGL, false);
        gl.pixelStorei(gl.UNPACK_PREMULTIPLY_ALPHA_WEBGL, false);
        gl.pixelStorei(gl.UNPACK_ALIGNMENT, 4);

        // restore the current krpano WebGL program
        this.krpano.object.webGL.restoreProgram();

        //this.data.renderer.resetGLState();
    }

    /**
     * Restore ThreeJS WebGL State
     */
    this.restoreThreeJSWebGLState = function () {
        if (this.threeJS.splitted) return;

        var gl = this.krpano.object.webGL.context;

        gl.enable(gl.DEPTH_TEST);
        gl.depthFunc(gl.LEQUAL);
        gl.enable(gl.CULL_FACE);
        gl.cullFace(gl.BACK);
        gl.clearDepth(1);
        gl.clear(gl.DEPTH_BUFFER_BIT);

        this.data.renderer.state.reset();
    }

    /**
     * Get krpano projection matrix
     * @param  {number} sw screen width
     * @param  {number} sh screen heigth
     * @param  {number} zoom
     * @param  {number} xoff x offset
     * @param  {number} yoff y offset
     */
    this.krpanoProjectionMatrix = function (sw, sh, zoom, xoff, yoff) {
        var m = this.krpano.projection;

        var pr = window._fm_pr || this.krpano.device.pixelratio;
        var v = window._fm_v || 0.5;
        var z = window._fm_z || 1;
        var y = window._fm_y || 1;
        var x = window._fm_x || 1;

        var swc = (pr / (sw * v));
        var shc = (pr / (sh * v));

        m[0]  = zoom * swc;  m[1]  = 0;               m[2]  = 0;                              m[3]  = 0;
        m[4]  = 0;           m[5]  = -zoom * shc * z; m[6]  = 0;                              m[7]  = 0;
        m[8]  = xoff * x;    m[9]  = -yoff * shc * y; m[10] = this.krpano.depthBufferScale;   m[11] = 1;
        m[12] = 0;           m[13] = 0;               m[14] = this.krpano.depthBufferOffset;  m[15] = 1;

        return m;
    }

    /**
     * Update projection of camera
     * @param  {object} camera
     * @param  {array} customProjection
     */
    this.updateCameraMatrix = function (camera, customProjection) {
        var m = customProjection ? customProjection : this.krpano.projection;
        camera.projectionMatrix.set(
            m[0],
            m[4],
            m[8],
            m[12],
            m[1],
            m[5],
            m[9],
            m[13],
            m[2],
            m[6],
            m[10],
            m[14],
            m[3],
            m[7],
            m[11],
            m[15]
        );
    }

    /**
     * Adjust krpano rendering
     */
    this.adjustKrpanoRendering = function () {
        if (this.krpano.object.view.fisheye != 0.0) {
            // disable the fisheye distortion, ThreeJS objects can't be rendered with it
            this.krpano.object.view.fisheye = 0.0;
        }
    }

    /**
     * Render frame of 3D
     */
    this.renderFrame = function () {
        this.data.time++;

        this.playAnimations();

        var gl = this.krpano.object.webGL.context;
        var vr = this.krpano.object.webVR && this.krpano.object.webVR.enabled ? this.krpano.object.webVR : null;

        var sw = gl.drawingBufferWidth;
        var sh = gl.drawingBufferHeight;

        // this.updateLabelsPosition(sw, sh); // for all labels

        // setup WebGL for ThreeJS
        this.restoreThreeJSWebGLState();

        // set the camera/view rotation
        this.krpano.panoview = this.krpano.object.view.getState(this.krpano.panoview); // the 'this.krpano.panoview' object will be created and cached inside getState()

        // labels rotation
        if (
            this.data.rooms[this.data.currentUrl] &&
            this.data.rooms[this.data.currentUrl].rotation &&
            this.transition.shader &&
            !this.getTransitionShaderVisible() &&
            this.doll.viewState !== 'big'
        ) {
            this.doll.viewPoint.labelsRotation = this.data.rooms[this.data.currentUrl].rotation;
        }

        if (this.data.config.dollhouse) {
            // Alternative roration for doll
            if (this.doll.viewState !== 'small') {
                if (this.krpano.object.mouse.down) {
                    this.doll.krpanoAlternativeLocation = {
                        x: Math.min(150, Math.max(120, this.doll.krpanoAlternativeStartLocation.x + (1 / sh * -(this.krpano.object.mouse.y - this.krpano.object.mouse.downy) * 180))),
                        y: this.doll.krpanoAlternativeStartLocation.y - (1 / sw * (this.krpano.object.mouse.x - this.krpano.object.mouse.downx) * 360),
                        z: this.doll.krpanoAlternativeStartLocation.z + 0,
                    };
                } else {
                    this.doll.krpanoAlternativeStartLocation = {
                        x: this.doll.krpanoPanoviewEuler.x / THREE.Math.DEG2RAD,
                        y: this.doll.krpanoPanoviewEuler.y / THREE.Math.DEG2RAD,
                        z: this.doll.krpanoPanoviewEuler.z / THREE.Math.DEG2RAD
                    }

                    this.doll.krpanoAlternativeLocation = {
                        x: this.doll.krpanoAlternativeStartLocation.x + 0,
                        y: this.doll.krpanoAlternativeStartLocation.y + 0,
                        z: this.doll.krpanoAlternativeStartLocation.z + 0,
                    }
                }

                this.doll.krpanoPanoviewEuler.set(
                    this.doll.krpanoAlternativeLocation.x * THREE.Math.DEG2RAD,
                    this.doll.krpanoAlternativeLocation.y * THREE.Math.DEG2RAD,
                    this.doll.krpanoAlternativeLocation.z * THREE.Math.DEG2RAD,
                    "YXZ"
                );
            } else if (
                !this.data.modelAvalible ||
                (this.data.rooms[this.data.currentUrl] && this.data.rooms[this.data.currentUrl].rotation)
            ) {
                this.doll.krpanoPanoviewEuler.set(
                    Math.min((180 - Math.min(this.krpano.panoview.v + 30, 60)), 150) * THREE.Math.DEG2RAD,
                    -this.krpano.panoview.h * THREE.Math.DEG2RAD,
                    this.krpano.panoview.r * THREE.Math.DEG2RAD,
                    "YXZ"
                );
            } else {
                this.doll.krpanoPanoviewEuler.set(
                    Math.min((180 - Math.min(this.krpano.panoview.v + 30, 60)), 150) * THREE.Math.DEG2RAD,
                    this.doll.krpanoPanoviewEuler.y,
                    this.doll.krpanoPanoviewEuler.z,
                    "YXZ"
                );
            }
        }

        this.main.krpanoPanoviewEuler.set(
            (180 - this.krpano.panoview.v) * THREE.Math.DEG2RAD,
            -this.krpano.panoview.h * THREE.Math.DEG2RAD,
            this.krpano.panoview.r * THREE.Math.DEG2RAD,
            "YXZ"
        );

        if (this.data.config.dollhouse) {
            if (
                this.doll.viewPoint.active &&
                this.data.rooms[this.data.currentUrl] &&
                this.doll.viewPoint.inactiveModels
            ) {
                if (!this.getTransitionShaderVisible()) {
                    var activeRotation  = (
                            -this.krpano.panoview.h *
                            THREE.Math.DEG2RAD -
                            this.data.rooms[this.data.currentUrl].rotation + Math.PI / 2
                        );

                    this.doll.viewPoint.active.rotation.set(
                        0,
                        activeRotation,
                        0,
                        "YXZ"
                    );
                } else {
                    this.doll.viewPoint.active.rotation.set(
                        0,
                        this.doll.krpanoPanoviewEuler.y - this.doll.object3D.rotation.y + Math.PI / 2,
                        0,
                        "YXZ"
                    );
                }

                if (!this.getTransitionShaderVisible() || this.doll.viewState !== 'small') {
                    var inactiveRoration = this.doll.viewState !== 'small' ?
                        this.doll.krpanoPanoviewEuler.y - this.doll.object3D.rotation.y + Math.PI / 2 :
                        this.doll.krpanoPanoviewEuler.y - this.data.rooms[this.data.currentUrl].rotation + Math.PI / 2;

                    this.doll.viewPoint.inactiveModels.children.map(function (v, k) {
                        this.doll.viewPoint.inactiveModels.children[k].rotation.set(
                            0,
                            inactiveRoration,
                            0,
                            "YXZ"
                        )
                    }.bind(this));
                }
            } else if (this.doll.viewPoint.inactiveModels) {
                var inactiveRoration = this.doll.krpanoPanoviewEuler.y - this.doll.object3D.rotation.y + Math.PI / 2;

                this.doll.viewPoint.inactiveModels.children.map(function (v, k) {
                    this.doll.viewPoint.inactiveModels.children[k].rotation.set(
                        0,
                        inactiveRoration,
                        0,
                        "YXZ"
                    )
                }.bind(this));
            }
        }

        this.main.camera.quaternion.setFromEuler(this.main.krpanoPanoviewEuler);
        this.main.camera.updateMatrixWorld(true);

        if (this.data.config.dollhouse) {
            this.doll.camera.quaternion.setFromEuler(this.doll.krpanoPanoviewEuler);
            this.doll.camera.updateMatrixWorld(true);

            var dollCustomProjection = this.krpanoProjectionMatrix(sw, sh, 1000, 0, this.krpano.panoview.yf); // disable zoom by scroll
            this.updateCameraMatrix(this.doll.camera, dollCustomProjection);
        }

        // set the camera/view projection
        this.krpanoProjectionMatrix(sw, sh, this.krpano.panoview.z, 0, this.krpano.panoview.yf);
        this.updateCameraMatrix(this.main.camera);

        // do scene updates
        //update_white_circles();
        this.handleMouseHovering();

        // render the scene
        if (this.krpano.object.display.stereo === false) {
            // normal rendering
            this.data.renderer.setViewport(0, 0, sw, sh);
            //this.data.renderer.setScissor(0, 0, sw, sh);
            //this.data.renderer.setScissorTest(true);
            //todo check scissor rectangle, when viewport resized it should also change

            this.data.renderer.render(this.main.scene, this.main.camera);

            // doll view rendering
            if (this.doll.visible && this.data.config.dollhouse) {
                var dollHeight = sh * this.doll.coefficents.currentHeight;
                var dollWidth = sw * this.doll.coefficents.currentWidth;
                var dollTop = sh * this.doll.coefficents.currentTop;
                var dollLeft = sw * this.doll.coefficents.currentLeft;

                this.data.renderer.setViewport(dollLeft, dollTop, dollWidth, dollHeight);
                this.data.renderer.setScissor(dollLeft, dollTop, dollWidth, dollHeight);
                this.data.renderer.setScissorTest(true);
                this.data.renderer.render(this.doll.scene, this.doll.camera);

                // return view to full rendering
                this.data.renderer.setViewport(0, 0, sw, sh);
                this.data.renderer.setScissor(0, 0, sw, sh);
            } else if (this.data.config.dollhouse) { //Todo: get why render blank screen without doll (start on street)
                this.data.renderer.setViewport(0, 0, 1, 1);
                this.data.renderer.setScissor(0, 0, 1, 1);
                this.data.renderer.setScissorTest(true);
                this.data.renderer.render(this.doll.scene, this.doll.camera);

                // return view to full rendering
                this.data.renderer.setViewport(0, 0, sw, sh);
                this.data.renderer.setScissor(0, 0, sw, sh);
            }
        } else {
            // stereo / VR rendering
            sw *= 0.5;	// use half screen width

            var stereo_scale = 0.05;
            var stereo_offset = Number(this.krpano.object.display.stereooverlap);

            // use a different camera for stereo rendering to keep the normal one for hit-testing
            this.main.stereoCamera.quaternion.copy(this.main.camera.quaternion);
            this.main.stereoCamera.updateMatrixWorld(true);

            // render left eye
            var eye_offset = -0.03;
            this.krpanoProjectionMatrix(sw, sh, this.krpano.panoview.z, stereo_offset, this.krpano.panoview.yf);

            if (vr) {
                eye_offset = vr.eyetranslt(1);						// get the eye offset (from the WebVR API)
                vr.prjmatrix(1, this.krpano.projection);					// replace the projection matrix (with the one from WebVR)
                this.krpano.projection[10] = this.krpano.depthBufferScale;	// adjust the depthbuffer scaling
                this.krpano.projection[14] = this.krpano.depthBufferOffset;
            }

            // add the eye offset
            this.krpano.projection[12] = this.krpano.projection[0] * -eye_offset * stereo_scale;

            this.updateCameraMatrix(this.main.stereoCamera);
            this.data.renderer.setViewport(0,0, sw,sh);
            this.data.renderer.render(this.main.scene, this.main.stereoCamera);

            // render right eye
            eye_offset = +0.03;
            this.krpano.projection[8] = -stereo_offset;	// mod the projection matrix (only change the stereo offset)

            if (vr) {
                eye_offset = vr.eyetranslt(2);						// get the eye offset (from the WebVR API)
                vr.prjmatrix(2, this.krpano.projection);					// replace the projection matrix (with the one from WebVR)
                this.krpano.projection[10] = this.krpano.depthBufferScale;	// adjust the depthbuffer scaling
                this.krpano.projection[14] = this.krpano.depthBufferOffset;
            }

            // add the eye offset
            this.krpano.projection[12] = this.krpano.projection[0] * -eye_offset * stereo_scale;

            this.updateCameraMatrix(this.main.stereoCamera);
            this.data.renderer.setViewport(sw,0, sw,sh);
            this.data.renderer.render(this.main.scene, this.main.stereoCamera);
        }

        // important - restore the krpano WebGL state for correct krpano rendering
        this.restoreKrpanoWebGLState();
    }
    /* Three.js in krpano stuff. end */

    this.getTransitionShaderVisible = function () {
        return !!this.transition.shader.__visible;
    }

    this.setTransitionShaderVisible = function (value) {
        if (this.transition.shader.__visible !== value) {
            this.transition.shader.__visible = value;

            this.transition.shader.visible = value;

            // this.transition.shader.uniforms.opacity.value = value ? 1 : (window._fm_so || 0.5);

            // this.main.group.traverse(function (object) {
            //     if (object.changeScaleOnTransition) {
            //         object.scale.y = value ? 10000 / object.structure.height : 1;
            //     }
            // }.bind(this));
        }
    }

    /**
     * Init shader
     * @param  {string} url1
     * @param  {string} url2
     * @param  {function} callback
     */
    this.initShader = function (url1, url2, callback) {
        // console.log(window.jsonData.tour.rooms[url1].name, window.jsonData.tour.rooms[url2].name);

        const roomsData = jsonData.tour.rooms;

        function getRotationWithFallback(url, rotationName) {
            if (roomsData[url] && roomsData[url][rotationName]) return roomsData[url][rotationName] * THREE.Math.DEG2RAD;
            else return 0;
        }

        if (this.transition.shader == null) {
            if (this.data.config.useShader) {
                this.transition.object = new TransitionShader(this.data.config.debug);
            } else {
                this.transition.object = new TestShader();
            }

            this.transition.shader = new THREE.ShaderMaterial ({
                uniforms : this.transition.object.getUniforms(),
                vertexShader : this.transition.object.getVertexShader(),
                fragmentShader : this.transition.object.getFragmentShader(),
                transparent: true
            });

            this.setTransitionShaderVisible(false);

            this.transition.shader.do = function () {
                try { // TODO: fix bug with fast floors changing
                    var ratio = this.krpano.object.get('ourTransitionProgress');

                    var url1 = this.transition.shader.url1;
                    var url2 = this.transition.shader.url2;
                    var pos = (this.transition.shader.uniforms.placement1.value.set(0, 0, 0))
                            .addScaledVector(this.data.rooms[url1].placement, 1 - ratio)
                            .addScaledVector (this.data.rooms[url2].placement, ratio);
                    var rot = this.data.rooms[url1].rotation * (1 - ratio) + this.data.rooms[url2].rotation * ratio;

                    this.transition.shader.uniforms.pitch1.value = getRotationWithFallback(url1, 'pitch');
                    this.transition.shader.uniforms.roll1.value = getRotationWithFallback(url1, 'roll');
                    this.transition.shader.uniforms.pitch2.value = getRotationWithFallback(url2, 'pitch');
                    this.transition.shader.uniforms.roll2.value = getRotationWithFallback(url2, 'roll');

                    // try to keep the camera properly oriented TODO proper API ?
                    this.krpano.object.view._hlookat -= (rot - this.main.wrapper.rotation.y) / THREE.Math.DEG2RAD;

                    this.main.group.position.copy(pos).multiplyScalar(-1);
                    this.main.wrapper.rotation.y = rot;

                    if (this.doll.viewState === 'small' && this.data.config.dollhouse) {
                        this.doll.object3D.rotation.y = rot;
                    }

                    // the shader expects placements to be proper world space locations, so they need to be re-calculated
                    this.main.wrapper.updateMatrixWorld(true);
                    this.main.group.localToWorld(
                        this.transition.shader.uniforms.placement1.value.copy(this.data.rooms[url1].placement)
                    );
                    this.main.group.localToWorld(
                        this.transition.shader.uniforms.placement2.value.copy(this.data.rooms[url2].placement)
                    );

                    // rotations...
                    this.transition.shader.uniforms.rotation1.value = (rot - this.data.rooms[url1].rotation) / (2 * Math.PI);
                    this.transition.shader.uniforms.rotation2.value = (rot - this.data.rooms[url2].rotation) / (2 * Math.PI);

                    this.transition.shader.uniforms.ratio.value = ratio;

                    this.transition.shaderInRender = true;
                    this.showWallsWithoutHoles();
                    this.hideCursor();

                    if (ratio === 1) {
                        // transition is over - show the actual pano image now
                        // loadxml's onloadcomplete calls the global functions
                        // https://krpano.com/forum/wbb/index.php?page=Thread&postID=41317#post41317
                        //krpano.call('loadxml(\'<krpano><image><sphere url="' + (textures[url2] ? textures[url2].dataUrl : rooms[url2].src) + '"/></image>' +
                        //'<events onloadcomplete="js( __hide_our_shader() );"/></krpano>\', null, KEEPALL)');
                        window.__hide_our_shader = function () {
                            if (!(this.data.config.debug && this.data.config.keepShaderOn)) {
                                this.setTransitionShaderVisible(false);
                            }
                        }.bind(this);

                        // var image = this.data.textures.cache[url2].original.image;
                        if (window.jsonData.tour.useCubic && window.jsonData.tour.activeRoom.cubePatternDesktop) {
                            var krpanoXML = new EJS(
                                {url: window.baseUrl +'/krpano-wrapper-style/hardcoded-content/3d.ejs'}
                            ).render(window.jsonData);

                            this.krpano.object.call("loadxml(" + krpanoXML.replace(/>\s+</g, "><") + ", null, MERGE)"); //remove whitespace between tags
                        } else {
                            //todo same for equirectangular images
                            this.krpano.object.call(
                                'loadxml(\'<krpano><image><sphere url="' +
                                //this.data.textures.cache[url2].original ? this.data.textures.cache[url2].original.image.toDataURL() :
                                this.data.rooms[url2].src +
                                '"/></image>' +
                                '<events onloadcomplete="js(__hide_our_shader());"/></krpano>\', null, MERGE)'
                            );
                        }
                        this.transition.shaderInRender = false;
                        this.hideWallsWithoutHoles();
                        this.showCursor();
                        this.hideAllLabels();
                    } else if (ratio === undefined) { //ratio==undefined when first room is loaded
                        this.transition.shaderInRender = false;
                        this.hideAllLabels();

                        if (!(this.data.config.debug && this.data.config.keepShaderOn)) {
                            this.setTransitionShaderVisible(false);
                            this.hideWallsWithoutHoles();
                            this.showCursor();
                        }
                    }
                } catch (e) {
                    console.warn('Shader error:', e);

                    this.transition.shaderInRender = false;
                    this.hideAllLabels();

                    if (!(this.data.config.debug && this.data.config.keepShaderOn)) {
                        this.setTransitionShaderVisible(false);
                    }
                }
            }.bind(this);
        }

        this.transition.shader.url1 = url1;
        this.transition.shader.url2 = url2;

        if (this.transition.shader.uniforms.map1.value) { this.transition.shader.uniforms.map1.value.dispose(); }
        if (this.transition.shader.uniforms.map2.value) { this.transition.shader.uniforms.map2.value.dispose(); }

        if (this.data.modelAvalible && typeof callback === "function") {
            return callback();
        }

        var loader = new THREE.TextureLoader (), cubeLoader = new THREE.CubeTextureLoader ();
        if (this.data.textures.loaded && this.data.config.loadTextures) {
            var texture, url, cubePattern;
            for (var i = 1; i <= 2; i++) {
                url = this.transition.shader['url' + i];

                if (this.data.fps.value > 50) {
                    texture = this.data.textures.cache[url].original;
                } else if (this.data.fps.value > 25 && this.data.textures.cache[url]['size_0.5']) {
                    texture = this.data.textures.cache[url]['size_0.5'];
                } else if (this.data.textures.cache[url]['size_0.125']) {
                    texture = this.data.textures.cache[url]['size_0.125'];
                } else if (this.data.textures.cache) {
                    texture = this.data.textures.cache[url].original;
                }

                if (texture) {
                    this.transition.shader.uniforms['map' + i].value = texture;
                } else { //when not preloaded
                    cubePattern = window.jsonData.tour.rooms[this.transition.shader['url' + i]].cubePatternDesktop;
                    var cubeUrls = [
                        this.resolveUrlPath(cubePattern.replace('%s', 'r').replace(".JPG", ".jpg")),
                        this.resolveUrlPath(cubePattern.replace('%s', 'l').replace(".JPG", ".jpg")),
                        this.resolveUrlPath(cubePattern.replace('%s', 'u').replace(".JPG", ".jpg")),
                        this.resolveUrlPath(cubePattern.replace('%s', 'd').replace(".JPG", ".jpg")),
                        this.resolveUrlPath(cubePattern.replace('%s', 'f').replace(".JPG", ".jpg")),
                        this.resolveUrlPath(cubePattern.replace('%s', 'b').replace(".JPG", ".jpg"))
                    ];

                    // three.js r79 cannot recover from loading error, so we need to check if the cubemap exists before trying to load it
                    var check = new XMLHttpRequest ();
                    check.open ('HEAD', cubeUrls[0], true);
                    check.onreadystatechange = function () {
                        if (this.readyState === this.DONE) {
                            if (this.status === 200) {
                                // load cubemap
                                this.data.textures.cache[url].original = cubeLoader.load(cubeUrls);
                                this.transition.shader.uniforms['map' + i].value = this.data.textures.cache[url].original;
                            } else {
                                // load equirectangular map
                                this.data.textures.cache[url].original =
                                    loader.load(this.resolveUrlPath(this.data.rooms[url].src)); /*rooms[url].src.replace(/\/Tour\//i, '/Tour/1080-images/')*/

                                this.transition.shader.uniforms['map' + i].value = this.data.textures.cache[url].original;
                                this.transition.shader.uniforms['map' + i].value.wrapS = THREE.RepeatWrapping;
                            }
                        }
                    };
                    check.send ();
                }
            }

            this.transition.shader.fragmentShader = this.transition.object.getFragmentShader(
                this.transition.shader.uniforms.map1.value.images,
                this.transition.shader.uniforms.map2.value.images
            );

            this.transition.shader.needsUpdate = true;
        }

        if (typeof callback === "function") callback();
    }

    /**
     * Set Doll View Point Active
     * @param  {string} url
     */
    this.setDollViewPointActive = function (url) {
        url = !url ? this.data.currentUrl : url;

        if (this.doll.viewPoint.inactiveModels) {
            for (var viewPointIndex in this.doll.viewPoint.inactiveModels.children) {
                var name =
                    this.doll.viewPoint.inactiveModels.children[viewPointIndex].userData ?
                        this.doll.viewPoint.inactiveModels.children[viewPointIndex].userData.name :
                        '';

                if (url && name && ~name.indexOf(url)) {
                    this.doll.viewPoint.active = this.doll.viewPoint.activeModels.children[viewPointIndex];
                    this.doll.viewPoint.activeModels.children[viewPointIndex].visible = true;
                    this.doll.viewPoint.inactiveModels.children[viewPointIndex].visible = false;
                } else if (name) {
                    this.doll.viewPoint.activeModels.children[viewPointIndex].visible = false;
                    this.doll.viewPoint.inactiveModels.children[viewPointIndex].visible = true;
                }
            }
        }
    }

    /**
     * Set Doll View Point Hovered
     * @param  {string} url
     */
    this.setDollViewPointHovered = function (url) {
        if (this.data.config.dollhouse) {
            for (var viewPointIndex in this.doll.viewPoint.inactiveModels.children) {
                var name =
                    this.doll.viewPoint.inactiveModels.children[viewPointIndex].userData ?
                        this.doll.viewPoint.inactiveModels.children[viewPointIndex].userData.name :
                        '';

                if (url && name && ~name.indexOf(url)) {
                    this.doll.viewPoint.inactiveModels.children[viewPointIndex].children.map(function (v, i) {
                        this.doll.viewPoint.inactiveModels.children[viewPointIndex].children[i].material =
                            this.doll.viewPoint.config.materials.hovered;
                    }.bind(this));
                } else {
                    this.doll.viewPoint.inactiveModels.children[viewPointIndex].children.map(function (v, i) {
                        this.doll.viewPoint.inactiveModels.children[viewPointIndex].children[i].material =
                            this.doll.viewPoint.config.materials.inactive;
                    }.bind(this));
                }
            }
        }
    }

    /**
     * Make a jump with transition to room
     * @param {string} url
     */
    this.makeJump = function (url) {
        var ts = + new Date;

        if (this.transition.lastJumpTimestamp > ts - this.transition.transitionDelay) {
            console.warn('Transition spam!'); // Todo: remove old events in handleMouseTouchEvents
            return;
        }

        this.transition.lastJumpTimestamp = ts;

        if (
            this.transition.shader &&
            this.getTransitionShaderVisible() &&
            !(this.data.config.debug && this.data.config.keepShaderOn)
        ) {
            console.warn('transition in progress');
            return;
        }

        if (window._DollHouse) {
            window._DollHouse.setCurrentRoom(url);
        }

        if (this.data.config.debug) {
            console.log('makeJump("' + url + '")');
        }

        if (this.data.currentUrl !== url) {
            this.transition.innerJump = true;
            if (this.data.modelAvalible || jsonData.tour.rooms[url].plan < -10) { // Hint: if floor under -10 has bug with cursor set this number less
                this.initShader(this.data.currentUrl ? this.data.currentUrl : url, url, function () { 
                    this.setTransitionShaderVisible(false);
                    this.unloadPoints();

                    this.renderFrame();

                    if (this.data.currentUrl !== null && url) {
                        this.krpano.plugin.loadscene(url, null, null, jsonData.playerMode);
                        //this.krpano.plugin.afterEnterRoom(this.krpano.interaction.cleanFilename(url), null);
                    }

                    this.setDollViewPointActive(url);
                    this.setDollViewPointHovered();
                    this.setDollViewSmallFast();
                    this.hideAllLabels();
                }.bind(this));
            } else {
                this.initShader(this.data.currentUrl ? this.data.currentUrl : url, url, function () {
                    this.main.shaderSphere.material = this.transition.shader;

                    this.main.group.traverse(function (mesh) {
                        if (mesh.visible && !mesh.isCircle && !mesh.isArrow && !mesh.isLadderArrow && !mesh.noShader && mesh.material) {
                            mesh.material = this.transition.shader;
                        }
                    }.bind(this));

                    //Try to adaptive shader to low end systems
                    if (this.data.config.debug) {
                        console.log('fps:', this.data.fps.value);
                    }

                    if (this.data.fps.value > 50) { // \________/
                        this.transition.shader.uniforms.minRatio.value = 0;
                        this.transition.shader.uniforms.maxRatio.value = 1;
                        this.transition.shader.uniforms.multiplyRatio.value = 1;
                    } else if (this.data.fps.value > 25) { // ----\/----
                        this.transition.shader.uniforms.minRatio.value = 0.4;
                        this.transition.shader.uniforms.maxRatio.value = 0.6;
                        this.transition.shader.uniforms.multiplyRatio.value = 5;
                    } else if (this.data.fps.value > 15) { // --------\/
                        this.transition.shader.uniforms.minRatio.value = 0.90;
                        this.transition.shader.uniforms.maxRatio.value = 1.0;
                        this.transition.shader.uniforms.multiplyRatio.value = 10;
                    } else { //imposible transition ---------/
                        this.transition.shader.uniforms.minRatio.value = 1;
                        this.transition.shader.uniforms.maxRatio.value = 1.1;
                        this.transition.shader.uniforms.multiplyRatio.value = 1;
                    }

                    if (!this.data.currentUrl) {
                        //no transition, just rotate and do other stuff
                        var rot = this.data.rooms[url].rotation;
                        var pos = this.data.rooms[url].placement;

                        this.krpano.object.view._hlookat -= (rot - this.main.wrapper.rotation.y) / THREE.Math.DEG2RAD;

                        this.main.group.position.copy(pos).multiplyScalar(-1);
                        this.main.wrapper.rotation.y = rot;

                        if (this.doll.viewState === 'small' && this.data.config.dollhouse) {
                            this.doll.object3D.rotation.y = rot;
                        }

                        this.hideWallsWithoutHoles();
                        this.showCursor();
                        this.hideAllLabels();
                    } else {
                        //transition
                        if (this.data.textures.loaded) {
                            this.krpano.object.set('ourTransitionProgress', 0);
                            this.krpano.object.actions.tween(
                                'ourTransitionProgress',
                                1,
                                1.2, // = transition time, seconds
                                "easeInOutQuad",
                                null,
                                this.transition.shader.do.bind(this)
                            );

                            if (this.threeJS.splitted) { // we need pass one frame, to remove glitch
                                setTimeout(function () {
                                    this.setTransitionShaderVisible(true);
                                }.bind(this), 10);
                            } else {
                                this.setTransitionShaderVisible(true);
                            }
                        }

                        // force the update at ourTransitionProgress = 0
                        this.transition.shader.do();
                    }


                    // force re-draw
                    this.renderFrame();

                    // activate and deactivate hotspots for current scene
                    this.loadPoints(url);
                    this.setDollViewPointActive(url);

                    //hide current InfoPoints when move
                    if (this.data.currentUrl !== null) {
                        this.deactivateInfoPoints();
                    }

                    this.krpano.interaction.notifySceneChange(url);
                    this.krpano.plugin.afterEnterRoom(this.krpano.interaction.cleanFilename(url), null, url);
                }.bind(this));
            }

            this.data.currentUrl = url;
        }
    }

    /**
     * Build 3D scene
     */
    this.buildScene = function () {
        if (!this.data.modelAvalible) {
          //do not render krpano hotspots
          this.deactivateHotspots();
        }

        $.when(this.loadRoomsInfo(), this.data.textures.deferred).done(function() {
            // load meshes and cameras into system
            this.loadMeshes();
        }.bind(this));
    }

    /**
     * Preload all room images for three js rendering
     */
    this.loadTextures = function () {
        var manager = new THREE.LoadingManager;
        manager.onLoad = function() {
            this.data.textures.loaded = true; // TODO: replace this.data.textures.loaded flag with this.data.textures.deferred.state as it became global
            this.data.textures.deferred.resolve();
            console.log("Textures loaded");
        }.bind(this);

        //also load interface textures
        TextureLib.load("circleTexture", this.resolveUrlPath('/krpano-common/img/circle2.png'));
        TextureLib.load("circleOpaqueTexture", this.resolveUrlPath('/krpano-common/img/circle_center.png'));
        TextureLib.load("circleLockTexture", this.resolveUrlPath('/krpano-common/img/circle_lock.png'));
        TextureLib.load("arrowTexture", this.resolveUrlPath('/krpano-common/img/arrow_nav.png'));
        TextureLib.load("ladderTexture", this.resolveUrlPath('/krpano-common/img/arrow_nav_ladder.png'));

        var loader = new THREE.TextureLoader(manager); // quickly load small texture versions for the transition
        var cubeLoader = new THREE.CubeTextureLoader(manager); // quickly load small texture versions for the transition

        if (this.data.config.loadTextures) {
            var jsonRoom;
            var cubicPattern;
            for (var key in window.jsonData.tour.rooms) {
                jsonRoom = window.jsonData.tour.rooms[key];
                cubicPattern = jsonRoom.cubePatternTransition || jsonRoom.cubePatternDesktop;
                var useCubemap = window.jsonData.tour.useCubic && cubicPattern;

                if (this.data.textures.cache[key] === undefined) {
                    this.data.textures.cache[key] = {
                        original: null,
                    };
                }

                if (useCubemap) {
                    var arUrls = ["r", "l", "u", "d", "f", "b"].map(function(elem) {
                        var replacedUrl = cubicPattern.replace("%s", elem).replace(".JPG", ".jpg");

                        return this.data.config.useProxy ? this.data.config.proxyUrl + replacedUrl : replacedUrl;
                    }.bind(this));

                    this.data.textures.cache[key].original = cubeLoader.load(arUrls, function (t) {
                        var arImages = [];

                        t.image.forEach(function(pict) {
                            pict = this.resizePowerOfTwo(
                                pict,
                                this.data.config.textureSize,
                                this.data.config.textureSize
                            );
                            arImages.push(pict);
                        }.bind(this));

                        t.image = arImages;

                        if (window._DollHouse && this.data.textures.cache[key].original) {
                            // window._DollHouse.setTexture(key, t, useCubemap); // Todo: investigate bug of wrong dollhouse textures more deep
                        }
                    }.bind(this));
                } else { //equirect
                    var textureUrl = this.data.config.useProxy ? this.data.config.proxyUrl + jsonRoom.url : jsonRoom.url;

                    this.data.textures.cache[key].original = loader.load(textureUrl, function (t) {
                        t.image = this.resizePowerOfTwo(
                            t.image,
                            this.data.config.textureSize * 4,
                            this.data.config.textureSize * 2
                        );

                        t.wrapS = THREE.RepeatWrapping;

                        if (window._DollHouse && this.data.textures.cache[key].original) {
                            // window._DollHouse.setTexture(key, t, useCubemap);
                        }
                    }.bind(this));
                }
            }
        } else {
            // Hint: when shader is off, dont need load textures
            manager.onLoad();
        }

        return this.data.textures.deferred.promise();
    }

    /**
     * Get bounding box of object
     * @param  {object} object
     */
    this.getObjectBoundingBox = function (object) {
        return new THREE.Box3().setFromObject(object);
    }

    /**
     * Set pivot to center object
     * @param  {object} object
     */
    this.setPivotToObjectCenter = function (object) {
        this.getObjectBoundingBox(object).getCenter(object.position).multiplyScalar(-1);
        return object.position;
    }

    /**
     * Get max size //Todo: add comment?
     * @param  {object} max
     * @param  {object} min
     */
    this.getMaxSize = function (max, min) {
        var greater = 0;
        Object.keys(max).map(function (key) {
            var result = Math.abs(max[key] - min[key]);
            greater = greater < result ? result : greater;
        });

        return greater;
    }

    this.getCenterPoint = function (mesh) {
        var geometry = mesh.geometry;
        geometry.computeBoundingBox();
        var center = new THREE.Vector3;
        geometry.boundingBox.getCenter(center);
        mesh.localToWorld(center);
        return center;
    }

    /**
     * Load all .obj stuff
     */
    this.loadMeshes = function () {
        var loader = new THREE.OBJLoader;
        
        loader.load (this.resolveUrlPath(this.data.objPath), function (g) {
            // collect the boxes. used in hovering points

            g.traverse(function (object) {
                if (object.name.indexOf ('_Box') == 0) {

                    object.geometry.computeBoundingBox();

                    // patch vertical coordinate temporarily
                    object.geometry.boundingBox.min.y = -1e9;
                    object.geometry.boundingBox.max.y =  1e9;

                    this.main.boxes.push(object.geometry.boundingBox.clone());
                }
            });

            var ssGeometry = new THREE.SphereGeometry(10000, 16, 16);
            var ssMaterial = new THREE.MeshBasicMaterial({color: 0xffff00, opacity: 0});
            ssMaterial.visible = false; // Zero opacity visible - bug

            this.main.shaderSphere = new THREE.Mesh(ssGeometry, ssMaterial);
            this.main.wrapper = new THREE.Object3D();

            this.main.shaderSphere.scale.x = -1;

            if (!this.data.modelAvalible) {
                this.main.group = g;
                this.main.wrapper.add(this.main.group);
            } else {
                this.main.group = new THREE.Group();
            }

            this.main.scene.add(this.main.wrapper);
            this.main.scene.add(this.main.shaderSphere);

            if (this.data.config.dollhouse) {
                this.doll.object3D = new THREE.Object3D();
                this.doll.object3D.add(this.doll.group = g.clone());

                var dollBoundingBox = this.getObjectBoundingBox(this.doll.group);
                var maxSize = this.getMaxSize(dollBoundingBox.max, dollBoundingBox.min);
                var maxSizeDiagonal = Math.sqrt(2) * maxSize;

                var scaleTo = 1500;
                var scaleModifer = 2700000;

                var a = (scaleTo - maxSizeDiagonal) / (scaleTo / 100);
                var b = -(scaleModifer / 100 * a);
                var c = scaleModifer + b;

                var modelScaleСoefficient = scaleTo / c;

                // console.log('Doll size coefficent: ' + modelScaleСoefficient);

                var viewPointScaleCoefficent = modelScaleСoefficient / 0.027;

                this.doll.group.scale.set(
                    modelScaleСoefficient,
                    modelScaleСoefficient,
                    modelScaleСoefficient
                );

                this.setPivotToObjectCenter(this.doll.group);

                this.doll.scene.add(this.doll.object3D);

                if (this.data.config.debug) {
                    this.doll.scene.add(new THREE.AxisHelper(1));
                    this.main.scene.add(new THREE.AxisHelper(1));
                }

                this.doll.viewPoint.activeModels = new THREE.Group();
                this.doll.viewPoint.inactiveModels = new THREE.Group();

                var viewPointScale = modelScaleСoefficient * 16;

                this.doll.group.traverse(function (object) {
                    if (object.name === 'ceiling' || object.name.substr(0, 9) == 'arrow_pos') {
                        object.visible = false;
                    } else if (object.name.substr(0, 10) == 'camera_pos') {
                        object.visible = false;

                        this.setPivotToObjectCenter(object);

                        // ACTIVE
                        var viewPointActive = this.doll.viewPoint.config.models.active.clone();

                        viewPointActive.scale.set(
                            viewPointScale,
                            viewPointScale,
                            viewPointScale
                        );

                        viewPointActive.userData = {name: 'vp_' + object.name};
                        viewPointActive.position.x = this.doll.group.position.x - object.position.x;
                        viewPointActive.position.y = -0.04; //this.doll.group.position.y - object.position.y;
                        viewPointActive.position.z = this.doll.group.position.z - object.position.z;

                        viewPointActive.children.map(function (v, i) {
                            viewPointActive.children[i].material = this.doll.viewPoint.config.materials.active;
                        }.bind(this));

                        viewPointActive.visible = false;

                        this.doll.viewPoint.activeModels.add(viewPointActive);

                        // INACTIVE
                        var viewPointInactive = this.doll.viewPoint.config.models.inactive.clone();

                        viewPointInactive.scale.set(
                            viewPointScale,
                            viewPointScale,
                            viewPointScale
                        );

                        var roomId = object.name.substr(10, object.name.length);
                        var label = jsonData.tour.rooms[roomId] ? jsonData.tour.rooms[roomId].name : 'camera';

                        viewPointInactive.userData = {
                            name: 'vp_' + object.name,
                            label: label
                        };

                        viewPointInactive.position.x = this.doll.group.position.x - object.position.x;
                        viewPointInactive.position.y = -0.04; //this.doll.group.position.y - object.position.y;
                        viewPointInactive.position.z = this.doll.group.position.z - object.position.z;

                        viewPointInactive.children.map(function (v, i) {
                            viewPointInactive.children[i].userData = {
                                name: 'vp_' + object.name,
                                isDollViewPointChild: true
                            };
                            viewPointInactive.children[i].material = this.doll.viewPoint.config.materials.inactive;
                        }.bind(this));

                        this.doll.viewPoint.inactiveModels.add(viewPointInactive);
                    } else if (object.name === 'floor') {
                        object.material = new THREE.MeshLambertMaterial({
                            color: 0x6D6D6D,
                            transparent: true,
                            opacity: 0.90
                        });
                    } else {
                        //make the top edges of the walls darkgray
                        if (object.geometry instanceof THREE.BufferGeometry) {
                            object.geometry = new THREE.Geometry().fromBufferGeometry(object.geometry);
                        }

                        if (object.geometry instanceof THREE.Geometry) {
                            var topVerticesIds = [];
                            var topHeight = 0;

                            object.geometry.vertices.map(function (v, i) {
                                topHeight = topHeight > v.y ? topHeight : v.y;
                            });

                            object.geometry.vertices.map(function (v, i) {
                                if (v.y >= topHeight - 10) {
                                    topVerticesIds.push(i);
                                }
                            });

                            object.geometry.faces.map(function (f, i) {
                                if (
                                    topVerticesIds.indexOf(f.a) >= 0 &&
                                    topVerticesIds.indexOf(f.b) >= 0 &&
                                    topVerticesIds.indexOf(f.c) >= 0
                                ) {
                                    object.geometry.faces[i].materialIndex = 1;
                                } else {
                                    object.geometry.faces[i].materialIndex = 0;
                                }
                            });

                            object.geometry.groupsNeedUpdate = true;

                            object.material = [
                                new THREE.MeshBasicMaterial({color: 0xbebebe, transparent: true, opacity: 0.7}),
                                new THREE.MeshBasicMaterial({color: 0x6D6D6D, transparent: true, opacity: 0.7})
                            ];
                        } else {
                            object.material = new THREE.MeshLambertMaterial({color: 0xffffff, transparent: true, opacity: 0.7});
                        }
                    }
                }.bind(this));

                this.doll.object3D.add(this.doll.viewPoint.activeModels);
                this.doll.object3D.add(this.doll.viewPoint.inactiveModels);

                this.removeLabels();
                this.createLabels();
            }

            var hasCeilings = false;

            this.main.group.traverse(function (object) {
                // hide all the shit for now

                if (object.material) {
                    object.material.visible = false;
                }

                // collect camera positions
                if (object.name.substr (0, 10) == 'camera_pos' || object.name.substr (0, 9) == 'arrow_pos') {
                    object.visible = false;
                    object.updateMatrixWorld (true);
                    object.geometry.computeBoundingSphere ();

                    for (var key in this.data.rooms) {
                        if (this.data.rooms[key].placement == object.name) {
                            this.data.rooms[key].placement = object.geometry.boundingSphere.center;
                        } else if ("arrows" in this.data.rooms[key]) {
                            for (var arrowsKey in this.data.rooms[key].arrows) {
                                if (this.data.rooms[key].arrows[arrowsKey].placement == object.name) {
                                    this.data.rooms[key].arrows[arrowsKey].placement = object.geometry.boundingSphere.center;
                                }
                            }
                        }
                    }
                } else if (~object.name.indexOf('WithoutHoles')) {
                    var withHoles = this.main.scene.getObjectByName(object.name.replace('_WithoutHoles', ''));

                    this.data.wallsWithoutHoles.push({
                        without: object,
                        with: withHoles
                    });

                    object.visible = false;
                } else if (~object.name.indexOf('Door')) {
                    object.visible = false;
                } else if (~object.name.toLowerCase().indexOf("ceiling")) {
                    hasCeilings = true;
                }
            }.bind(this));

            if (!hasCeilings) {
                var mapData = jsonData.tour.maps[jsonData.tour.activeRoom.plan];

                this.main.group.traverse(function (object) {
                    if (~object.name.toLowerCase().indexOf("floor")) {
                        var ceiling = object.clone();
                        var ceilingHeight = 250;
                        var ceilingId = object.name.replace(/^FloorFlat_/, '');

                        ceiling.name = 'Ceiling_' + ceilingId;
                        object.parent.add(ceiling);

                        if (
                            ceilingId &&
                            mapData && 
                            mapData.structure3D &&
                            mapData.structure3D.data
                        ) {
                            if (typeof(mapData.structure3D.data) === 'string') {
                                mapData.structure3D.data = JSON.parse(mapData.structure3D.data);
                            }

                            if (
                                mapData.structure3D.data.floors &&
                                mapData.structure3D.data.floors[ceilingId] &&
                                mapData.structure3D.data.floors[ceilingId].ceilingHeight
                            ) {
                                ceilingHeight = mapData.structure3D.data.floors[ceilingId].ceilingHeight;
                            }
                        }

                        ceiling.position.y += ceilingHeight;
                    }
                }.bind(this));

                this.main.group.traverse(function (object) {
                    if (~object.name.indexOf('Wall_')) {
                        var wallId = object.name.replace(/^Wall_/, '');

                        if (typeof(mapData.structure3D.data) === 'string') {
                            mapData.structure3D.data = JSON.parse(mapData.structure3D.data);
                        }

                        if (
                            mapData.structure3D.data.walls &&
                            mapData.structure3D.data.walls[wallId]
                        ) {
                            var wallStructure = mapData.structure3D.data.walls[wallId];

                            if (wallStructure.outside && false) { // disable for now
                                if (wallStructure.hasHoles) {
                                    var withoutHoles = this.main.group.getObjectByName(object.name + '_WithoutHoles');

                                    if (withoutHoles) {
                                        object.geometry = withoutHoles.geometry;
                                    }
                                }

                                // object.noShader = true;
                                // object.material = new THREE.MeshBasicMaterial({color: 0xFF0000})

                                object.structure = wallStructure;
                                object.changeScaleOnTransition = true;
                            }
                        }
                    }
                }.bind(this));
            }

            if (!this.data.modelAvalible) {
                //cursor
                this.main.cursor.object = new Cursor();
                this.main.scene.add(this.main.cursor.object);
                this.hideCursor();
            } else if (this.main.cursor.object) {
                this.main.cursor.object = null;
            }

            //camera circles
            for (var key in this.data.rooms) {
                var hotspot;
                if (key in jsonData.tour.rooms && jsonData.tour.rooms[key].limitedAccess === true) {
                    hotspot = new LockedHotspot(key, this.data.rooms[key].placement);
                } else {
                    hotspot = new Hotspot(key, this.data.rooms[key].placement);
                }
                this.main.group.add(hotspot);
                this.data.rooms[key].mesh = hotspot;
            }

            //arrows
            this.data.arrows = {};
            this.data.ladders = {};
            for (var key in this.data.rooms) {
                if ("arrows" in this.data.rooms[key]) for (var arrow_key in this.data.rooms[key].arrows) {
                    var arrowItem = this.data.rooms[key].arrows[arrow_key];
                    if (!(arrowItem.name in this.data.arrows)) {
                        var arrowPlane = new Arrow(arrowItem.name, arrowItem.placement, arrowItem.rotation, arrowItem.destination_id);
                        this.main.group.add(arrowPlane);
                        this.data.arrows[arrowItem.name] = arrowPlane;
                    }
                }

                //support ladder hotspots
                if ("ladder_hotspots" in this.data.rooms[key]) for (var ladder_key in this.data.rooms[key].ladder_hotspots) {
                    var ladderArrowItem = this.data.rooms[key].ladder_hotspots[ladder_key];
                    if (!(ladderArrowItem.name in this.data.ladders)) {
                        var ladderArrowPlane = new LadderArrow(ladderArrowItem.name, ladderArrowItem.placement, ladderArrowItem.direction);
                        this.main.group.add(ladderArrowPlane);

                        ladderArrowPlane.destination_id = ladderArrowItem.destination_id;
                        this.data.ladders[ladderArrowItem.name] = ladderArrowPlane;
                    }
                }
            }

            // finally show something
            this.data.currentUrl = null;
            this.makeJump(this.data.firstRoom);
            this.transition.innerJump = false;
        }.bind(this));
    }

    /**
     * Load json with rooms information asynchronously
     */
    this.loadRoomsInfo = function () {
        var dfd = $.Deferred(), postfix = "?v=" + Date.now();
        //also load json
        var request = new XMLHttpRequest();
        request.addEventListener('load', function (result) {
            this.data.rooms = JSON.parse(result.target.responseText);
            dfd.resolve();
        }.bind(this));
        request.open('GET', this.resolveUrlPath(this.data.jsonPath + postfix)); //do not cache cameras info for now
        request.send();

        return dfd.promise();
    }

    /**
     * Deactivate info points
     */
    this.deactivateInfoPoints = function () {
        this.krpano.object.get("hotspot").getArray().forEach(function (hotspot) {
            if (hotspot.name.indexOf("spot") >= 0) {
                hotspot.enabled = false;
                hotspot.visible = false;
            }
        });
    }

    /**
     * Deactivate info hotspots
     */
    this.deactivateHotspots = function () {
        var arHotspots = this.krpano.object.get("hotspot").getArray(), item;
        for (item in arHotspots) {
            if (arHotspots[item].name.indexOf("spot") >= 0 && (arHotspots[item].style !== "hotspot_attachment" && arHotspots[item].style !== "measurement_note")) {
                arHotspots[item].enabled = false;
                arHotspots[item].visible = false;
            }
        }
    }

    /**
     * Do object hit test with mouse coordinates
     * @param  {number} mx
     * @param  {number} my
     */
    this.doObjectHitTest = function (mx, my) {
        var mouse_x = (mx / this.krpano.object.area.pixelwidth)  * 2.0 - 1.0;
        var mouse_y = (my / this.krpano.object.area.pixelheight) * 2.0 - 1.0;

        if (this.krpano.object.display.stereo) {
            mouse_x += (mouse_x < 0.0 ? +1 : -1) * (1.0 - Number(this.krpano.object.display.stereooverlap)) * 0.5;
        }

        this.main.raycaster.ray.direction.set(mouse_x, -mouse_y, 1.0).unproject(this.main.camera).normalize();

        if (this.data.config.dollhouse) {
            // Find doll view camera origin
            var cameraBackPoint = this.doll.camera.children[0].getWorldPosition();

            this.doll.raycaster.ray.origin.set(
                cameraBackPoint.x,
                cameraBackPoint.y,
                cameraBackPoint.z
            );
        }

        var intersects = [];

        if (this.main.group) {
            intersects = this.main.raycaster.intersectObject(this.main.group, true);
        }

        var dollViewPointIntersects = [];
        var dollIntersects = [];

        if (this.doll.visible && this.doll.group && this.doll.viewPoint.inactiveModels && this.data.config.dollhouse) {
            var dollViewMouseX = (((mouse_x + 1) - (this.doll.coefficents.currentLeft * 2)) / this.doll.coefficents.currentWidth) - 1;
            var dollViewMouseY = -((((mouse_y + 1) - (this.doll.coefficents.currentTop * 2)) / this.doll.coefficents.currentHeight) - 1);

            // this.doll.placeInteracted = dollViewMouseX <= 1 && dollViewMouseX >= -1 && dollViewMouseY <= 1 && dollViewMouseY >= -1;
            this.doll.placeInteracted = dollViewMouseX + dollViewMouseY < 0; // diagonal line from (1, -1) to (-1, 1)

            // Project to doll view
            this.doll.raycaster.ray.direction.set(
                dollViewMouseX,
                dollViewMouseY,
                1.0
            ).unproject(this.doll.camera).normalize();

            dollIntersects = this.doll.raycaster.intersectObject(this.doll.group, true);
            dollViewPointIntersects = this.doll.raycaster.intersectObject(this.doll.viewPoint.inactiveModels, true);
        }

        if (dollViewPointIntersects || dollIntersects || intersects) {
            var intersect = dollViewPointIntersects[0] ?
                dollViewPointIntersects[0] : dollIntersects[0] ?
                    dollIntersects[0] : intersects[0];

            if (this.data.config.debug && intersect) {
                if (this.doll.scene) this.doll.scene.add(new THREE.ArrowHelper(
                    this.doll.raycaster.ray.direction,
                    this.doll.raycaster.ray.origin,
                    100,
                    Math.random() * 0xffffff
                ));
                // intersect.object.material.opacity = Math.random(); //new THREE.MeshBasicMaterial({ color: 0x00ff00});
            }

            if (intersect && (dollViewPointIntersects[0] || dollIntersects[0])) {
                intersect.isDoll = true;
            } else if (intersect) {
                intersect.isDoll = false;
            }

            // console.log(intersects);

            return intersect;
        }

        return null;
    }

    /**
     * Handle Mouse Touch Events
     * @param  {object} event
     */
    this.handleMouseTouchEvents = function (event) {
        var type = "";

        if (event.type == "mousedown") {
            type = "ondown";
            this.copyHandler = this.handleMouseTouchEvents.bind(this);
            this.krpano.object.control.layer.addEventListener("mouseup", this.copyHandler, true);
        } else if (event.type == "mouseup") {
            type = "onup";
            this.krpano.object.control.layer.removeEventListener("mouseup", this.copyHandler, true);
        } else if (event.type == this.krpano.device.browser.events.touchstart) {
            type = "ondown";
            this.krpano.object.control.layer.addEventListener(
                this.krpano.device.browser.events.touchend,
                this.handleMouseTouchEvents.bind(this),
                true
            );
        } else if (event.type == this.krpano.device.browser.events.touchend) {
            type = "onup";
            this.krpano.object.control.layer.removeEventListener(
                this.krpano.device.browser.events.touchend,
                this.handleMouseTouchEvents.bind(this),
                true
            );
        }

        // get mouse / touch pos
        var ms = this.krpano.object.control.getMousePos(event.changedTouches ? event.changedTouches[0] : event);
        ms.x /= this.krpano.object.stagescale;
        ms.y /= this.krpano.object.stagescale;

        // is there a object as that pos?
        var hit = this.doObjectHitTest(ms.x, ms.y);

        if (this.data.config.debug) {
            console.log (type, hit);
        }

        if (type == "ondown") {
            this.data.mouse.downX = ms.x;
            this.data.mouse.downY = ms.y;

            if (hit) {
                if (false/*hit.object.properties.capture*/) {
                    this.krpano.object.mouse.down = true;
                    event.stopPropagation();
                }

                event.preventDefault();
            }
        } else if (type == "onup") {
            this.krpano.object.mouse.down = false;

            if (hit) {
                // console.log(this.main.cursor.worldNormal, this.main.cursor.worldPosition, this.data.modelAvalible);
                if (this.isKrpanoInAction()) {
                    this.setKrpanoNotInAction();
                } else if (
                    hit.isDoll &&
                    hit.object &&
                    hit.object.userData &&
                    hit.object.userData.isDollViewPointChild &&
                    hit.object.userData.name.substr(0, 13) == 'vp_camera_pos'
                ) {
                    this.makeJump(hit.object.userData.name.substr(13, hit.object.userData.name.length));
                } else if (!this.data.modelAvalible) {
                   

                    var dpix = Math.abs(this.data.mouse.downX - ms.x) + Math.abs(this.data.mouse.downY - ms.y);
                    if (dpix < 2) {
                        if (hit.object && hit.object.isLadderArrow) {
                            const source = this.krpano.object.screentosphere(this.krpano.object.mouse.x, this.krpano.object.mouse.y);
                            const fakeHotspot = {
                                hlookat: 0, //destination hotspot position
                                ath: source.x, //source hotspot position
                                atv: source.y //source hotspot position
                            };
                            this.krpano.plugin.loadscene(hit.object.destination_id, fakeHotspot, null, jsonData.playerMode);
                        } else {
                            this.main.cursor.isFloorHovered = 
                                hit.object.name && ~hit.object.name.toLowerCase().indexOf("floor");

                            if (this.main.cursor.isFloorHovered) {
                                var key_min, dist_min = 200, externalJump = false, destinationObj,
                                    localPoint = hit.point.clone(); this.main.group.worldToLocal (localPoint);
                                for (var key in this.data.rooms) {
                                    var dist = localPoint.distanceTo(this.data.rooms[key].mesh.position);
                                    if (key !== this.data.currentUrl && dist < dist_min && this.data.rooms[key].mesh.material.visible) {
                                        dist_min = dist;
                                        key_min = key;
                                        destinationObj = this.data.rooms[key].mesh;
                                    }
                                }

                                for (var arrowKey in this.data.arrows) {
                                    var dist = localPoint.distanceTo (this.data.arrows[arrowKey].position);
                                    if (dist < dist_min && this.data.arrows[arrowKey].material.visible) {
                                        dist_min = dist;
                                        key_min = this.data.arrows[arrowKey].destination_id;
                                        if (this.data.arrows[arrowKey].doesLeadToAnotherFloor(this.data.lastFloorId)) {
                                            externalJump = true;
                                        } else {
                                            if (key_min in this.data.rooms) destinationObj = this.data.rooms[ key_min ].mesh;
                                        }
                                    }
                                }

                                if (key_min) { //does any spot clicked?
                                    if (!(key_min in this.data.rooms)) {
                                        this.krpano.plugin.loadscene(key_min, null, null, jsonData.playerMode); //another floor
                                    } else if (externalJump || (destinationObj && destinationObj.isLocked)) {
                                        this.krpano.plugin.loadscene(key_min, null, null, jsonData.playerMode);
                                    } else {
                                        this.makeJump(key_min);
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }

    /**
     * Play stack of animations
     */
    this.playAnimations = function () {
        for (var animationId in this.data.animations) {
            var animation = this.data.animations[animationId];
            if (animation !== null) {
                animation.run(this.data.time - animation.start);
                if (animation.finish <= this.data.time) {
                    animation.afterRun();
                    this.data.animations[animationId] = null;
                }
            }
        }
    }

    /**
     * Put doll view big animation object to animations stack
     */
    this.setDollViewBig = function () {
        var animationTime = 20;
        if (this.doll.viewState === 'small' && !this.getTransitionShaderVisible()) {
            var animation = {
                start: this.data.time,
                finish: this.data.time + animationTime,
                beforeRun: function () {
                    this.doll.viewState = 'in_animation';
                    this.krpano.object.control.usercontrol = 'off';
                }.bind(this),
                run: function (frame) {
                    var step = frame / animationTime;
                    this.doll.coefficents.currentHeight = this.doll.coefficents.smallHeight + ((this.doll.coefficents.bigHeight - this.doll.coefficents.smallHeight) * step);
                    this.doll.coefficents.currentWidth = this.doll.coefficents.smallWidth + ((this.doll.coefficents.bigWidth - this.doll.coefficents.smallWidth) * step);
                    this.doll.coefficents.currentTop = this.doll.coefficents.smallTop + ((this.doll.coefficents.bigTop - this.doll.coefficents.smallTop) * step);
                    this.doll.coefficents.currentLeft = this.doll.coefficents.smallLeft + ((this.doll.coefficents.bigLeft - this.doll.coefficents.smallLeft) * step);
                }.bind(this),
                afterRun: function () {
                    this.doll.viewState = 'big';
                }.bind(this)
            }

            animation.beforeRun();
            this.data.animations.push(animation);
        }
    }

    /**
     * Put doll view small animation object to animations stack
     */
    this.setDollViewSmall = function () {
        var animationTime = 20;
        if (this.doll.viewState === 'big' && (!this.doll.placeInteracted || this.getTransitionShaderVisible())) {
            var animation = {
                start: this.data.time,
                finish: this.data.time + animationTime,
                beforeRun: function () {
                    this.doll.viewState = 'in_animation';
                    this.rotateDollBack();
                }.bind(this),
                run: function (frame) {
                    var step = frame / animationTime;
                    this.doll.coefficents.currentHeight = this.doll.coefficents.bigHeight - ((this.doll.coefficents.bigHeight - this.doll.coefficents.smallHeight) * step);
                    this.doll.coefficents.currentWidth = this.doll.coefficents.bigWidth - ((this.doll.coefficents.bigWidth - this.doll.coefficents.smallWidth) * step);
                    this.doll.coefficents.currentTop = this.doll.coefficents.bigTop - ((this.doll.coefficents.bigTop - this.doll.coefficents.smallTop) * step);
                    this.doll.coefficents.currentLeft = this.doll.coefficents.bigLeft - ((this.doll.coefficents.bigLeft - this.doll.coefficents.smallLeft) * step);
                }.bind(this),
                afterRun: function () {
                    this.doll.viewState = 'small';
                    this.krpano.object.control.usercontrol = 'all';
                }.bind(this)
            }

            animation.beforeRun();
            this.data.animations.push(animation);
        }
    }

    /**
     * Set doll view small without animation
     */
    this.setDollViewSmallFast = function () {
        this.doll.viewState = 'small';
        this.krpano.object.control.usercontrol = 'all';

        this.doll.coefficents.currentHeight = this.doll.coefficents.smallHeight;
        this.doll.coefficents.currentWidth = this.doll.coefficents.smallWidth;
        this.doll.coefficents.currentTop = this.doll.coefficents.smallTop;
        this.doll.coefficents.currentLeft = this.doll.coefficents.smallLeft;
    }

    /**
     * Get Closest Angle Between Radial Angles
     * @param  {number} a
     * @param  {number} b
     */
    this.getClosestDistanceBetweenAngles = function (a, b) {
        function normalize (angle) {
            return angle >= 0 ? angle % (Math.PI * 2) : (Math.PI * 2) - -(angle % (Math.PI * 2));
        }

        a = normalize(a);
        b = normalize(b);

        var distance = 0;
        var forward = true;

        if (a >= b) {
            distance = a - b;

            if (distance > Math.PI) {
                forward = true;
                distance = (Math.PI * 2) - distance;
            } else {
                forward = false;
            }
        } else {
            distance = b - a;

            if (distance > Math.PI) {
                forward = false;
                distance = (Math.PI * 2) - distance;
            } else {
                forward = true;
            }
        }

        return {
            angles: {
                a: a,
                b: b
            },
            distance: distance,
            forward: forward
        };
    }

    /**
     * Put doll rotation animation to current rotation
     */
    this.rotateDollBack = function () {
        var animationTime = 20;

        var dollRotation = null;
        var eulerRotations = {};

        var animation = {
            start: this.data.time,
            finish: this.data.time + animationTime,
            beforeRun: function () {
                if (this.data.rooms[this.data.currentUrl] && this.data.rooms[this.data.currentUrl].rotation) {
                    dollRotation = this.getClosestDistanceBetweenAngles(
                        this.doll.object3D.rotation.y,
                        this.data.rooms[this.data.currentUrl].rotation
                    );

                    eulerRotations = {
                        x: this.getClosestDistanceBetweenAngles(
                            this.doll.krpanoAlternativeLocation.x * THREE.Math.DEG2RAD,
                            Math.min((180 - Math.min(this.krpano.panoview.v + 30, 60)), 150) * THREE.Math.DEG2RAD
                        ),
                        y: this.getClosestDistanceBetweenAngles(
                            this.doll.krpanoAlternativeLocation.y * THREE.Math.DEG2RAD,
                            -this.krpano.panoview.h * THREE.Math.DEG2RAD
                        ),
                        z: this.getClosestDistanceBetweenAngles(
                            this.doll.krpanoAlternativeLocation.z * THREE.Math.DEG2RAD,
                            this.krpano.panoview.r * THREE.Math.DEG2RAD
                        )
                    }
                } else {
                    //Todo: get rotation from street (need data for it)
                    dollRotation = this.getClosestDistanceBetweenAngles(
                        this.doll.object3D.rotation.y,
                        0
                    );

                    eulerRotations = {
                        x: this.getClosestDistanceBetweenAngles(
                            this.doll.krpanoAlternativeLocation.x * THREE.Math.DEG2RAD,
                            Math.min((180 - Math.min(this.krpano.panoview.v + 30, 60)), 150) * THREE.Math.DEG2RAD
                        ),
                        y: this.getClosestDistanceBetweenAngles(
                            this.doll.krpanoAlternativeLocation.y * THREE.Math.DEG2RAD,
                            0
                        ),
                        z: this.getClosestDistanceBetweenAngles(
                            this.doll.krpanoAlternativeLocation.z * THREE.Math.DEG2RAD,
                            0
                        )
                    }
                }
            }.bind(this),
            run: function (frame) {
                var step = frame / animationTime;

                this.doll.object3D.rotation.y = dollRotation.forward ?
                    dollRotation.angles.a + (dollRotation.distance * step) :
                    dollRotation.angles.a - (dollRotation.distance * step);

                this.doll.krpanoPanoviewEuler.set(
                    eulerRotations.x.forward ?
                        eulerRotations.x.angles.a + (eulerRotations.x.distance * step) :
                        eulerRotations.x.angles.a - (eulerRotations.x.distance * step),
                    eulerRotations.y.forward ?
                        eulerRotations.y.angles.a + (eulerRotations.y.distance * step) :
                        eulerRotations.y.angles.a - (eulerRotations.y.distance * step),
                    eulerRotations.z.forward ?
                        eulerRotations.z.angles.a + (eulerRotations.z.distance * step) :
                        eulerRotations.z.angles.a - (eulerRotations.z.distance * step),
                    "YXZ"
                );
            }.bind(this),
            afterRun: function () {}
        }

        animation.beforeRun();
        this.data.animations.push(animation);
    }

    /**
     * Put doll rotation animation to current rotation
     */
    this.rotateDollActiveViewPoint = function () {
        var animationTime = 20;

        var avpRotation = null;

        var animation = {
            start: this.data.time,
            finish: this.data.time + animationTime,
            beforeRun: function () {
                avpRotation = this.getClosestDistanceBetweenAngles(
                    this.doll.viewPoint.inactiveModels.children[0].y,
                    (-this.krpano.panoview.h * THREE.Math.DEG2RAD - this.data.rooms[this.data.currentUrl].rotation + Math.PI / 2)
                );
            },
            run: function (frame) {
                var step = frame / animationTime;

                this.doll.viewPoint.active.rotation.y = avpRotation.forward ?
                    avpRotation.angles.a + (avpRotation.distance * step) :
                    avpRotation.angles.a - (avpRotation.distance * step);
            }.bind(this),
            afterRun: function () {}
        }

        animation.beforeRun();
        this.data.animations.push(animation);
    }

    this.showCursor = function () {        
        if (this.main.cursor.object && !this.data.isTouchDevice) {
            this.main.cursor.object.show();
        } else if (this.main.cursor.object) {
            this.main.cursor.object.hide();
        }
    }
    
    this.hideCursor = function () {
        if (this.main.cursor.object) {
            this.main.cursor.object.hide();
        }
    }

    /**
     * Handle Mouse Hovering
     */
    this.handleMouseHovering = function () {
        // check mouse over state
        if (this.krpano.object.mouse.down == false) { // currently not dragging?
            var hit = this.doObjectHitTest(this.krpano.object.mouse.x, this.krpano.object.mouse.y);
                // var hit = this.doObjectHitTest(krpano.mouse.x, krpano.mouse.y);
            if (hit && hit.isDoll && hit.object) {
                this.hideCursor();

                this.setDollViewBig();

                if (
                    hit.object &&
                    hit.object.userData &&
                    hit.object.userData.isDollViewPointChild &&
                    hit.object.userData.name.substr(0, 13) == 'vp_camera_pos'
                ) {
                    this.setDollViewPointHovered(hit.object.userData.name.substr(13, hit.object.userData.name.length));
                    this.showLabel(hit.object.userData.name);
                    this.updateLabelPositionByMouse(hit.object.userData.name, this.krpano.object.mouse.x, this.krpano.object.mouse.y);
                } else {
                    this.hideAllLabels();
                    this.setDollViewPointHovered();
                }
            } else if (hit && hit.face) {
                this.setDollViewSmall();
                this.setDollViewPointHovered();

                if (this.main.cursor.object && !this.data.modelAvalible) {
                    this.showCursor();

                    this.main.cursor.worldPosition = this.main.cursor.worldPosition || new THREE.Vector3;
                    this.main.cursor.worldPosition.set(0, 0, 0);

                    this.main.cursor.worldNormal = this.main.cursor.worldNormal || new THREE.Vector3;
                    this.main.cursor.worldNormal.copy(hit.face.normal);

                    hit.object.localToWorld(this.main.cursor.worldPosition);
                    hit.object.localToWorld(this.main.cursor.worldNormal);

                    this.main.cursor.worldNormal.sub(this.main.cursor.worldPosition);

                    this.main.cursor.object.position.copy(hit.point);

                    hit.point.add(this.main.cursor.worldNormal);
                    this.main.cursor.object.lookAt(hit.point);

                    // hack to work around transparent sorting issue
                    var hack = 0.85;
                    this.main.camera.localToWorld(
                        this.main.camera.worldToLocal(this.main.cursor.object.position).multiplyScalar(hack)
                    );
                    this.main.cursor.object.scale.set(hack, hack, hack);
                    this.main.cursor.lastRotation = this.main.cursor.object.quaternion.clone();

                    //hovering
                    var found = false, dist_min = 200, localPoint = hit.point.clone(), dist = 0, dist_min_temp = dist_min;
                    var bCircleActive = false, bArrowActive = false;

                    this.main.cursor.isFloorHovered = 
                        hit.object.name && ~hit.object.name.toLowerCase().indexOf("floor");

                    // console.log('cursorHit:', hit.object);

                    if (this.main.cursor.isFloorHovered) {
                        this.main.group.worldToLocal (localPoint);

                        for (var key in this.data.rooms) {
                            if ("mesh" in this.data.rooms[key]) {
                                dist = localPoint.distanceTo (this.data.rooms[key].mesh.position);
                                if (dist < dist_min_temp && key !== this.data.currentUrl && this.data.rooms[key].mesh.material.visible) {
                                    dist_min_temp = dist;
                                    found = true;
                                    bCircleActive = true;
                                    bArrowActive = false;
                                    this.main.cursor.circleActiveMesh = this.data.rooms[key].mesh;
                                    this.main.cursor.object.toCircle();
                                    this.main.cursor.object.toActiveState();
                                }
                            }
                        }

                        for (var keyArrow in this.data.arrows) {
                            dist = localPoint.distanceTo(this.data.arrows[keyArrow].position);
                            if (dist < dist_min_temp && this.data.arrows[keyArrow].material.visible) {
                                dist_min_temp = dist;
                                found = true;
                                bArrowActive = true;
                                bCircleActive = false;
                                this.main.cursor.arrowActiveMesh = this.data.arrows[keyArrow];
                                this.main.cursor.object.toArrow();
                                this.main.cursor.object.toActiveState();
                                if ("rotation" in this.data.arrows[keyArrow]) {
                                    this.main.cursor.object.setRotationFromQuaternion(this.data.arrows[keyArrow].getWorldQuaternion());
                                }
                            }
                        }
                        if (bCircleActive) {
                            this.main.cursor.circleActiveMesh.material.opacity = 1;
                            this.showCursor();
                        }
                        else if (bArrowActive) {
                            this.main.cursor.arrowActiveMesh.material.opacity = 1;
                            this.showCursor();
                        }
                        else {
                            this.main.cursor.object.toDot();
                            this.main.cursor.object.resetState();
                            this.main.cursor.object.changeOpacityByDistance(hit.distance);
                        }
                    } else {
                        if (hit.object.isLadderArrow) {
                            this.main.cursor.object.toLadderArrow();
                            this.main.cursor.object.setRotationFromQuaternion(hit.object.quaternion);
                            this.main.cursor.object.toActiveState();
                        } else if (this.main.cursor.object.isActive()) { //todo: we must determine material another way.
                            if (this.main.cursor.object.isArrow()) {
                                this.main.cursor.object.toDot();
                                this.main.cursor.object.setRotationFromQuaternion(this.main.cursor.lastRotation); //recover rotation when cursor is on the wall
                            }
                            this.main.cursor.object.resetState();
                            this.main.cursor.object.changeOpacityByDistance(hit.distance);
                        } else {
                            this.main.cursor.object.changeOpacityByDistance(hit.distance);
                        }
                    }
                }
            }
        }
    }

    /**
     * Resize image to make its dimensions be a multiple of 2 (eg. 16x16, 512x1024)
     * @param {HTMLImageElement}
     * @param {number} maxW max width
     * @param {number} maxH max height
     * @return {HTMLImageElement} resized or source image
     */
    this.resizePowerOfTwo = function (image, maxW, maxH) {
        if (maxW === undefined) maxW = Infinity;
        if (maxH === undefined) maxH = Infinity;

        return this.resizeImage(
            image,
            Math.min(THREE.Math.floorPowerOfTwo(image.width), maxW),
            Math.min(THREE.Math.floorPowerOfTwo(image.height), maxH)
        );
    }

    /**
     * Resize image
     * @param {HTMLImageElement}
     * @return {HTMLImageElement} resized or source image
     */
    this.resizeImage = function (image, new_width, new_height) {
        if (
            (image instanceof HTMLImageElement || image instanceof HTMLCanvasElement || image instanceof ImageBitmap) &&
            (image.width !== new_width || image.height !== new_height)
        ) {
            console.log(
                '%cImage resize: ',
                'color: orange; font-weight: bold;',
                'W:[' +
                image.width +
                ' -> ' +
                new_width +
                '] H:[' +
                image.height +
                ' -> ' +
                new_height +
                ']'
            );

            var canvas = document.createElementNS('http://www.w3.org/1999/xhtml', 'canvas');
            canvas.width = new_width;
            canvas.height = new_height;

            var context = canvas.getContext('2d');
            context.drawImage(image, 0, 0, canvas.width, canvas.height);

            return canvas;
        }

        return image;
    }

    /**
     * Rotate model when room is changed externally
     */
    this.handleRoomChange = function () {
        $(document).on("krpano:loadscene", function() {
            if (
                this.transition.shader && 
                !this.transition.innerJump && 
                !this.data.modelAvalible &&
                jsonData.tour.activeRoom.filename in this.data.rooms
            ) {
                this.deactivateHotspots();

                //rotate hotspots when a jump is made from floorplan or carousel
                var pos = (this.transition.shader.uniforms.placement1.value.set (0, 0, 0)).addScaledVector(
                    this.data.rooms[jsonData.tour.activeRoom.filename].placement,
                    1
                );
                var rot = this.data.rooms[jsonData.tour.activeRoom.filename].rotation;

                // try to keep the camera properly oriented TODO proper API ?
                this.krpano.object.view._hlookat -= (rot - this.main.wrapper.rotation.y) / THREE.Math.DEG2RAD;

                this.main.group.position.copy(pos).multiplyScalar(-1);
                this.main.wrapper.rotation.y = rot;


                if (this.doll.viewState === 'small' && this.data.config.dollhouse) {
                    this.doll.object3D.rotation.y = rot;
                }

                // the shader expects placements to be proper world space locations, so they need to be re-calculated
                this.main.wrapper.updateMatrixWorld (true);
                this.main.group.localToWorld(
                    this.transition.shader.uniforms.placement2.value.copy(
                        this.data.rooms[jsonData.tour.activeRoom.filename].placement
                    )
                );

                this.loadPoints(jsonData.tour.activeRoom.filename);
                this.setDollViewPointActive(jsonData.tour.activeRoom.filename);
                this.data.currentUrl = jsonData.tour.activeRoom.filename;
            } else {
                this.setDollViewPointActive(jsonData.tour.activeRoom.filename);
                this.data.currentUrl = jsonData.tour.activeRoom.filename;
            }
            this.transition.innerJump = false;
        }.bind(this));
    }

    /**
     * Event on scene change
     */
    this.onSceneChange = function () {
        //update model
        if (this.data.lastFloorId !== jsonData.tour.activeRoom.plan) {
            this.data.lastFloorId = jsonData.tour.activeRoom.plan;

            if ("model3DUrl" in jsonData.tour.maps[jsonData.tour.activeRoom.plan]) {
                this.data.modelAvalible = false;
                this.data.objPath = jsonData.tour.maps[jsonData.tour.activeRoom.plan].model3DUrl;

                if (this.krpano.object.camerasinfourl) { //temp hack to take cameras json from url when some environmental error occur
                    this.data.jsonPath = this.krpano.object.camerasinfourl;
                } else if ("camerasInfoUrl" in jsonData.tour.maps[jsonData.tour.activeRoom.plan]) {
                    this.data.jsonPath = jsonData.tour.maps[jsonData.tour.activeRoom.plan].camerasInfoUrl;
                }

                this.data.firstRoom = window.jsonData.tour.activeRoom.filename.slice(0);

                this.start();
            } else if (jsonData.tour.activeRoom.plan < 0 && this.findFirstFloor()) {
                this.data.modelAvalible = true;
                this.data.jsonPath = this.data.firstFloor.camerasInfoUrl;
                this.data.firstRoom = window.jsonData.tour.activeRoom.filename.slice(0);
                this.data.objPath = this.data.firstFloor.model3DUrl;

                if (!this.transition.status) {
                    this.start();
                } else {
                    this.unloadPoints();
                    this.resetScene();
                    this.buildScene();
                }
            } else {
                this.data.modelAvalible = true;

                this.unloadPoints();
                this.resetScene();
            }
        }
    }

    /**
     * Handle change floor
     */
    this.handleChangeFloor = function () {
        $(document).on("krpano:loadscene", this.onSceneChange.bind(this));
    }

    /**
     * Preload scripts and textures
     */
    this.preloadStuff = function () {
        var deferred = new $.Deferred();
        this.loadScripts([
                "/krpano-common/js/threejs/three.js",
                "/krpano-common/js/threejs/OBJLoader.js",
                "/krpano-wrapper-style/js/3d/texture_lib.js",
                "/krpano-wrapper-style/js/3d/cursor.js",
                "/krpano-wrapper-style/js/3d/hotspot.js",
                "/krpano-wrapper-style/js/3d/arrow.js",
                "/krpano-wrapper-style/js/3d/ladder_arrow.js",
                "/krpano-wrapper-style/js/3d/locked_hotspot.js",
                "/krpano-wrapper-style/js/3d/transition_shader.js",
                "/krpano-wrapper-style/js/3d/test_shader.js",
            ],
            function () {
                this.splitContexts();

                this.data.scriptsLoaded = true;

                if (this.data.textures.preload) {
                    $.when(this.loadTextures()).done(function () {
                        deferred.resolve();
                    });
                } else {
                    deferred.resolve();
                }
            }.bind(this)
        );

        return deferred.promise();
    }

    this.splitContexts = function () {
        this.threeJS.splitted = true;

        this.threeJS.canvas = document.createElement('canvas');
        this.threeJS.canvas.id = 'floorModel3D';

        var playerConatiner = document.querySelector('#playerContainer')

        playerConatiner.append(this.threeJS.canvas);
    }

    /**
     * Every change of scene show only circle hotspots available for this scene.
     */
    this.loadPoints = function (sceneName) {
        if (
            // this.data.rooms[sceneName] && 
            this.data.rooms[sceneName].hotspots && 
            this.data.rooms[sceneName].hotspots.length > 0
        ) {
            this.main.group.traverse(function (child) {
                if (child.name !== "" && child.isCircle === true) {
                    if (this.data.rooms[sceneName].hotspots.indexOf(child.name) >= 0) {
                        child.material.visible = true;
                    } else {
                        child.material.visible = false;
                    }
                }
            }.bind(this));
        } else {
            this.main.group.traverse(function (child) {
                if (child.name !== "" && child.isCircle === true) {
                    child.material.visible = false;
                }
            });
        }

        //arrows
        if (
            // this.data.rooms[sceneName] && 
            "arrows" in this.data.rooms[sceneName] && 
            this.data.rooms[sceneName].arrows.length > 0
        ) {
            for (var key in this.data.arrows) {
                this.data.arrows[key].hide();
            }
            this.main.group.traverse(function (child) {
                if (child.name !== "" && child.isArrow === true) {
                    this.data.rooms[sceneName].arrows.forEach(function(arrowObj) {
                        if (arrowObj.name === child.name) {
                            child.material.visible = true;
                            child.visible = true;
                        }
                    });
                }
            }.bind(this));
        } else {
            this.main.group.traverse(function (child) {
                if (child.name !== "" && child.isArrow === true) {
                    child.material.visible = false;
                }
            });
        }

        //ladder_arrows
        if (
            // this.data.rooms[sceneName] && 
            "ladder_hotspots" in this.data.rooms[sceneName] && 
            this.data.rooms[sceneName].ladder_hotspots.length > 0
        ) {
            for (key in this.data.ladders) {
                this.data.ladders[key].hide();
            }
            this.main.group.traverse(function (child) {
                if (child.name !== "" && child.isLadderArrow === true) {
                    this.data.rooms[sceneName].ladder_hotspots.forEach(function(arrowObj) {
                        if (arrowObj.name === child.name) {
                            child.show();
                        }
                    });
                }
            }.bind(this));
        } else {
            this.main.group.traverse(function (child) {
                if (child.name !== "" && child.isLadderArrow === true) {
                    child.hide();
                }
            });
        }
    }

    /**
     * Remove all kinds of hotspots in three js scene
     */
    this.unloadPoints = function () {
        if (this.main.group) {
            this.main.group.traverse(function (child) {
                if (child.name !== "" && (child.isCircle === true || child.isArrow === true || child.isLadderArrow === true)) {
                    child.material.visible = false;
                }
            });
            this.data.rooms = {};
            this.data.arrows = {};
            this.data.ladders = {};
        }
    }

    /**
     * Reset three js scene to default state, remove meshes, dispose materials etc.
     */
    this.resetScene = function () {
        function disposeGroup (parent) {
            for (var i = parent.children.length - 1; i >= 0; i--) {
                var obj = parent.children[i];
                disposeItem(obj, parent);
            }
        }

        function disposeItem (item, parent) {
            parent.remove(item);

            if (item.geometry) {
                item.geometry.dispose();
            }
            if (item.material) {
                if (Array.isArray(item.material)) {
                    item.material.map(function (v, i) {
                        item.material[i].dispose();
                    });
                } else {
                    item.material.dispose();
                }
            }
            if (item.mesh) {
                item.mesh.dispose();
            }
            if (item.texture) {
                item.texture.dispose();
            }
        }

        if (this.main.cursor.object) {
            this.main.cursor.object.position.copy(new THREE.Vector3(99999, 99999, 99999)); //cursor. dirty hack to prevent krpano fading
        }

        if (this.main.scene) {
            disposeGroup(this.main.group);
        }

        if (this.doll.scene) {
            disposeGroup(this.doll.group);
            disposeGroup(this.doll.viewPoint.activeModels);
            disposeGroup(this.doll.viewPoint.inactiveModels);
        }

        if (this.main.scene || this.doll.scene) {
            this.renderFrame();

            this.data.firstRoom = "";
            this.data.currentUrl = "";
        }
    }
};


/** krpano-content/krpano-common/js/sly/sly.js **/
/*!
 * sly 1.6.1 - 8th Aug 2015
 * https://github.com/darsain/sly
 *
 * Licensed under the MIT license.
 * http://opensource.org/licenses/MIT
 */

;(function ($, w, undefined) {
    'use strict';

    var pluginName = 'sly';
    var className  = 'Sly';
    var namespace  = pluginName;

    // Local WindowAnimationTiming interface
    var cAF = w.cancelAnimationFrame || w.cancelRequestAnimationFrame;
    var rAF = w.requestAnimationFrame;

    // Support indicators
    var transform, gpuAcceleration;

    // Other global values
    var $doc = $(document);
    var dragInitEvents = 'touchstart.' + namespace + ' mousedown.' + namespace;
    var dragMouseEvents = 'mousemove.' + namespace + ' mouseup.' + namespace;
    var dragTouchEvents = 'touchmove.' + namespace + ' touchend.' + namespace;
    var wheelEvent = (document.implementation.hasFeature('Event.wheel', '3.0') ? 'wheel.' : 'mousewheel.') + namespace;
    var clickEvent = 'click.' + namespace;
    var mouseDownEvent = 'mousedown.' + namespace;
    var interactiveElements = ['INPUT', 'SELECT', 'BUTTON', 'TEXTAREA'];
    var tmpArray = [];
    var time;

    // Math shorthands
    var abs = Math.abs;
    var sqrt = Math.sqrt;
    var pow = Math.pow;
    var round = Math.round;
    var max = Math.max;
    var min = Math.min;

    // Keep track of last fired global wheel event
    var lastGlobalWheel = 0;
    $doc.on(wheelEvent, function (event) {
        var sly = event.originalEvent[namespace];
        var time = +new Date();
        // Update last global wheel time, but only when event didn't originate
        // in Sly frame, or the origin was less than scrollHijack time ago
        if (!sly || sly.options.scrollHijack < time - lastGlobalWheel) lastGlobalWheel = time;
    });

    /**
     * Sly.
     *
     * @class
     *
     * @param {Element} frame       DOM element of sly container.
     * @param {Object}  options     Object with options.
     * @param {Object}  callbackMap Callbacks map.
     */
    function Sly(frame, options, callbackMap) {
        if (!(this instanceof Sly)) return new Sly(frame, options, callbackMap);

        // Extend options
        var o = $.extend({}, Sly.defaults, options);

        // Private variables
        var self = this;
        var parallax = isNumber(frame);

        // Frame
        var $frame = $(frame);
        var $slidee = o.slidee ? $(o.slidee).eq(0) : $frame.children().eq(0);
        var frameSize = 0;
        var slideeSize = 0;
        var pos = {
            start: 0,
            center: 0,
            end: 0,
            cur: 0,
            dest: 0
        };

        // Scrollbar
        var $sb = $(o.scrollBar).eq(0);
        var $handle = $sb.children().eq(0);
        var sbSize = 0;
        var handleSize = 0;
        var hPos = {
            start: 0,
            end: 0,
            cur: 0
        };

        // Pagesbar
        var $pb = $(o.pagesBar);
        var $pages = 0;
        var pages = [];

        // Items
        var $items = 0;
        var items = [];
        var rel = {
            firstItem: 0,
            lastItem: 0,
            centerItem: 0,
            activeItem: null,
            activePage: 0
        };

        // Styles
        var frameStyles = new StyleRestorer($frame[0]);
        var slideeStyles = new StyleRestorer($slidee[0]);
        var sbStyles = new StyleRestorer($sb[0]);
        var handleStyles = new StyleRestorer($handle[0]);

        // Navigation type booleans
        var basicNav = o.itemNav === 'basic';
        var forceCenteredNav = o.itemNav === 'forceCentered';
        var centeredNav = o.itemNav === 'centered' || forceCenteredNav;
        var itemNav = !parallax && (basicNav || centeredNav || forceCenteredNav);

        // Miscellaneous
        var $scrollSource = o.scrollSource ? $(o.scrollSource) : $frame;
        var $dragSource = o.dragSource ? $(o.dragSource) : $frame;
        var $forwardButton = $(o.forward);
        var $backwardButton = $(o.backward);
        var $prevButton = $(o.prev);
        var $nextButton = $(o.next);
        var $prevPageButton = $(o.prevPage);
        var $nextPageButton = $(o.nextPage);
        var callbacks = {};
        var last = {};
        var animation = {};
        var move = {};
        var dragging = {
            released: 1
        };
        var scrolling = {
            last: 0,
            delta: 0,
            resetTime: 200
        };
        var renderID = 0;
        var historyID = 0;
        var cycleID = 0;
        var continuousID = 0;
        var i, l;

        // Normalizing frame
        if (!parallax) {
            frame = $frame[0];
        }

        // Expose properties
        self.initialized = 0;
        self.frame = frame;
        self.slidee = $slidee[0];
        self.pos = pos;
        self.rel = rel;
        self.items = items;
        self.pages = pages;
        self.isPaused = 0;
        self.options = o;
        self.dragging = dragging;

        /**
         * Loading function.
         *
         * Populate arrays, set sizes, bind events, ...
         *
         * @param {Boolean} [isInit] Whether load is called from within self.init().
         * @return {Void}
         */
        function load(isInit) {
            // Local variables
            var lastItemsCount = 0;
            var lastPagesCount = pages.length;

            // Save old position
            pos.old = $.extend({}, pos);

            // Reset global variables
            frameSize = parallax ? 0 : $frame[o.horizontal ? 'width' : 'height']();
            sbSize = $sb[o.horizontal ? 'width' : 'height']();
            slideeSize = parallax ? frame : $slidee[o.horizontal ? 'outerWidth' : 'outerHeight']();
            pages.length = 0;

            // Set position limits & relatives
            pos.start = 0;
            pos.end = max(slideeSize - frameSize, 0);

            // Sizes & offsets for item based navigations
            if (itemNav) {
                // Save the number of current items
                lastItemsCount = items.length;

                // Reset itemNav related variables
                $items = $slidee.children(o.itemSelector);
                items.length = 0;

                // Needed variables
                var paddingStart = getPx($slidee, o.horizontal ? 'paddingLeft' : 'paddingTop');
                var paddingEnd = getPx($slidee, o.horizontal ? 'paddingRight' : 'paddingBottom');
                var borderBox = $($items).css('boxSizing') === 'border-box';
                var areFloated = $items.css('float') !== 'none';
                var ignoredMargin = 0;
                var lastItemIndex = $items.length - 1;
                var lastItem;

                // Reset slideeSize
                slideeSize = 0;

                // Iterate through items
                $items.each(function (i, element) {
                    // Item
                    var $item = $(element);
                    var rect = element.getBoundingClientRect();
                    var itemSize = round(o.horizontal ? rect.width || rect.right - rect.left : rect.height || rect.bottom - rect.top);
                    var itemMarginStart = getPx($item, o.horizontal ? 'marginLeft' : 'marginTop');
                    var itemMarginEnd = getPx($item, o.horizontal ? 'marginRight' : 'marginBottom');
                    var itemSizeFull = itemSize + itemMarginStart + itemMarginEnd;
                    var singleSpaced = !itemMarginStart || !itemMarginEnd;
                    var item = {};
                    item.el = element;
                    item.size = singleSpaced ? itemSize : itemSizeFull;
                    item.half = item.size / 2;
                    item.start = slideeSize + (singleSpaced ? itemMarginStart : 0);
                    item.center = item.start - round(frameSize / 2 - item.size / 2);
                    item.end = item.start - frameSize + item.size;

                    // Account for slidee padding
                    if (!i) {
                        slideeSize += paddingStart;
                    }

                    // Increment slidee size for size of the active element
                    slideeSize += itemSizeFull;

                    // Try to account for vertical margin collapsing in vertical mode
                    // It's not bulletproof, but should work in 99% of cases
                    if (!o.horizontal && !areFloated) {
                        // Subtract smaller margin, but only when top margin is not 0, and this is not the first element
                        if (itemMarginEnd && itemMarginStart && i > 0) {
                            slideeSize -= min(itemMarginStart, itemMarginEnd);
                        }
                    }

                    // Things to be done on last item
                    if (i === lastItemIndex) {
                        item.end += paddingEnd;
                        slideeSize += paddingEnd;
                        ignoredMargin = singleSpaced ? itemMarginEnd : 0;
                    }

                    // Add item object to items array
                    items.push(item);
                    lastItem = item;
                });

                // Resize SLIDEE to fit all items
                $slidee[0].style[o.horizontal ? 'width' : 'height'] = (borderBox ? slideeSize: slideeSize - paddingStart - paddingEnd) + 'px';

                // Adjust internal SLIDEE size for last margin
                slideeSize -= ignoredMargin;

                // Set limits
                if (items.length) {
                    pos.start =  items[0][forceCenteredNav ? 'center' : 'start'];
                    pos.end = forceCenteredNav ? lastItem.center : frameSize < slideeSize ? lastItem.end : pos.start;
                } else {
                    pos.start = pos.end = 0;
                }
            }

            // Calculate SLIDEE center position
            pos.center = round(pos.end / 2 + pos.start / 2);

            // Update relative positions
            updateRelatives();

            // Scrollbar
            if ($handle.length && sbSize > 0) {
                // Stretch scrollbar handle to represent the visible area
                if (o.dynamicHandle) {
                    handleSize = pos.start === pos.end ? sbSize : round(sbSize * frameSize / slideeSize);
                    handleSize = within(handleSize, o.minHandleSize, sbSize);
                    $handle[0].style[o.horizontal ? 'width' : 'height'] = handleSize + 'px';
                } else {
                    handleSize = $handle[o.horizontal ? 'outerWidth' : 'outerHeight']();
                }

                hPos.end = sbSize - handleSize;

                if (!renderID) {
                    syncScrollbar();
                }
            }

            // Pages
            if (!parallax && frameSize > 0) {
                var tempPagePos = pos.start;
                var pagesHtml = '';

                // Populate pages array
                if (itemNav) {
                    $.each(items, function (i, item) {
                        if (forceCenteredNav) {
                            pages.push(item.center);
                        } else if (item.start + item.size > tempPagePos && tempPagePos <= pos.end) {
                            tempPagePos = item.start;
                            pages.push(tempPagePos);
                            tempPagePos += frameSize;
                            if (tempPagePos > pos.end && tempPagePos < pos.end + frameSize) {
                                pages.push(pos.end);
                            }
                        }
                    });
                } else {
                    while (tempPagePos - frameSize < pos.end) {
                        pages.push(tempPagePos);
                        tempPagePos += frameSize;
                    }
                }

                // Pages bar
                if ($pb[0] && lastPagesCount !== pages.length) {
                    for (var i = 0; i < pages.length; i++) {
                        pagesHtml += o.pageBuilder.call(self, i);
                    }
                    $pages = $pb.html(pagesHtml).children();
                    $pages.eq(rel.activePage).addClass(o.activeClass);
                }
            }

            // Extend relative variables object with some useful info
            rel.slideeSize = slideeSize;
            rel.frameSize = frameSize;
            rel.sbSize = sbSize;
            rel.handleSize = handleSize;

            // Activate requested position
            if (itemNav) {
                if (isInit && o.startAt != null) {
                    activate(o.startAt);
                    self[centeredNav ? 'toCenter' : 'toStart'](o.startAt);
                }
                // Fix possible overflowing
                var activeItem = items[rel.activeItem];
                slideTo(centeredNav && activeItem ? activeItem.center : within(pos.dest, pos.start, pos.end));
            } else {
                if (isInit) {
                    if (o.startAt != null) slideTo(o.startAt, 1);
                } else {
                    // Fix possible overflowing
                    slideTo(within(pos.dest, pos.start, pos.end));
                }
            }

            // Trigger load event
            trigger('load');
        }
        self.reload = function () { load(); };

        /**
         * Animate to a position.
         *
         * @param {Int}  newPos    New position.
         * @param {Bool} immediate Reposition immediately without an animation.
         * @param {Bool} dontAlign Do not align items, use the raw position passed in first argument.
         *
         * @return {Void}
         */
        function slideTo(newPos, immediate, dontAlign) {
            // Align items
            if (itemNav && dragging.released && !dontAlign) {
                var tempRel = getRelatives(newPos);
                var isNotBordering = newPos > pos.start && newPos < pos.end;

                if (centeredNav) {
                    if (isNotBordering) {
                        newPos = items[tempRel.centerItem].center;
                    }
                    if (forceCenteredNav && o.activateMiddle) {
                        activate(tempRel.centerItem);
                    }
                } else if (isNotBordering) {
                    newPos = items[tempRel.firstItem].start;
                }
            }

            // Handle overflowing position limits
            if (dragging.init && dragging.slidee && o.elasticBounds) {
                if (newPos > pos.end) {
                    newPos = pos.end + (newPos - pos.end) / 6;
                } else if (newPos < pos.start) {
                    newPos = pos.start + (newPos - pos.start) / 6;
                }
            } else {
                newPos = within(newPos, pos.start, pos.end);
            }

            // Update the animation object
            animation.start = +new Date();
            animation.time = 0;
            animation.from = pos.cur;
            animation.to = newPos;
            animation.delta = newPos - pos.cur;
            animation.tweesing = dragging.tweese || dragging.init && !dragging.slidee;
            animation.immediate = !animation.tweesing && (immediate || dragging.init && dragging.slidee || !o.speed);

            // Reset dragging tweesing request
            dragging.tweese = 0;

            // Start animation rendering
            if (newPos !== pos.dest) {
                pos.dest = newPos;
                trigger('change');
                if (!renderID) {
                    render();
                }
            }

            // Reset next cycle timeout
            resetCycle();

            // Synchronize states
            updateRelatives();
            updateButtonsState();
            syncPagesbar();
        }

        /**
         * Render animation frame.
         *
         * @return {Void}
         */
        function render() {
            if (!self.initialized) {
                return;
            }

            // If first render call, wait for next animationFrame
            if (!renderID) {
                renderID = rAF(render);
                if (dragging.released) {
                    trigger('moveStart');
                }
                return;
            }

            // If immediate repositioning is requested, don't animate.
            if (animation.immediate) {
                pos.cur = animation.to;
            }
            // Use tweesing for animations without known end point
            else if (animation.tweesing) {
                animation.tweeseDelta = animation.to - pos.cur;
                // Fuck Zeno's paradox
                if (abs(animation.tweeseDelta) < 0.1) {
                    pos.cur = animation.to;
                } else {
                    pos.cur += animation.tweeseDelta * (dragging.released ? o.swingSpeed : o.syncSpeed);
                }
            }
            // Use tweening for basic animations with known end point
            else {
                animation.time = min(+new Date() - animation.start, o.speed);
                pos.cur = animation.from + animation.delta * $.easing[o.easing](animation.time/o.speed, animation.time, 0, 1, o.speed);
            }

            // If there is nothing more to render break the rendering loop, otherwise request new animation frame.
            if (animation.to === pos.cur) {
                pos.cur = animation.to;
                dragging.tweese = renderID = 0;
            } else {
                renderID = rAF(render);
            }

            trigger('move');

            // Update SLIDEE position
            if (!parallax) {
                if (transform) {
                    $slidee[0].style[transform] = gpuAcceleration + (o.horizontal ? 'translateX' : 'translateY') + '(' + (-pos.cur) + 'px)';
                } else {
                    $slidee[0].style[o.horizontal ? 'left' : 'top'] = -round(pos.cur) + 'px';
                }
            }

            // When animation reached the end, and dragging is not active, trigger moveEnd
            if (!renderID && dragging.released) {
                trigger('moveEnd');
            }

            syncScrollbar();
        }

        /**
         * Synchronizes scrollbar with the SLIDEE.
         *
         * @return {Void}
         */
        function syncScrollbar() {
            if ($handle.length) {
                hPos.cur = pos.start === pos.end ? 0 : (((dragging.init && !dragging.slidee) ? pos.dest : pos.cur) - pos.start) / (pos.end - pos.start) * hPos.end;
                hPos.cur = within(round(hPos.cur), hPos.start, hPos.end);
                if (last.hPos !== hPos.cur) {
                    last.hPos = hPos.cur;
                    if (transform) {
                        $handle[0].style[transform] = gpuAcceleration + (o.horizontal ? 'translateX' : 'translateY') + '(' + hPos.cur + 'px)';
                    } else {
                        $handle[0].style[o.horizontal ? 'left' : 'top'] = hPos.cur + 'px';
                    }
                }
            }
        }

        /**
         * Synchronizes pagesbar with SLIDEE.
         *
         * @return {Void}
         */
        function syncPagesbar() {
            if ($pages[0] && last.page !== rel.activePage) {
                last.page = rel.activePage;
                $pages.removeClass(o.activeClass).eq(rel.activePage).addClass(o.activeClass);
                trigger('activePage', last.page);
            }
        }

        /**
         * Returns the position object.
         *
         * @param {Mixed} item
         *
         * @return {Object}
         */
        self.getPos = function (item) {
            if (itemNav) {
                var index = getIndex(item);
                return index !== -1 ? items[index] : false;
            } else {
                var $item = $slidee.find(item).eq(0);

                if ($item[0]) {
                    var offset = o.horizontal ? $item.offset().left - $slidee.offset().left : $item.offset().top - $slidee.offset().top;
                    var size = $item[o.horizontal ? 'outerWidth' : 'outerHeight']();

                    return {
                        start: offset,
                        center: offset - frameSize / 2 + size / 2,
                        end: offset - frameSize + size,
                        size: size
                    };
                } else {
                    return false;
                }
            }
        };

        /**
         * Continuous move in a specified direction.
         *
         * @param  {Bool} forward True for forward movement, otherwise it'll go backwards.
         * @param  {Int}  speed   Movement speed in pixels per frame. Overrides options.moveBy value.
         *
         * @return {Void}
         */
        self.moveBy = function (speed) {
            move.speed = speed;
            // If already initiated, or there is nowhere to move, abort
            if (dragging.init || !move.speed || pos.cur === (move.speed > 0 ? pos.end : pos.start)) {
                return;
            }
            // Initiate move object
            move.lastTime = +new Date();
            move.startPos = pos.cur;
            // Set dragging as initiated
            continuousInit('button');
            dragging.init = 1;
            // Start movement
            trigger('moveStart');
            cAF(continuousID);
            moveLoop();
        };

        /**
         * Continuous movement loop.
         *
         * @return {Void}
         */
        function moveLoop() {
            // If there is nowhere to move anymore, stop
            if (!move.speed || pos.cur === (move.speed > 0 ? pos.end : pos.start)) {
                self.stop();
            }
            // Request new move loop if it hasn't been stopped
            continuousID = dragging.init ? rAF(moveLoop) : 0;
            // Update move object
            move.now = +new Date();
            move.pos = pos.cur + (move.now - move.lastTime) / 1000 * move.speed;
            // Slide
            slideTo(dragging.init ? move.pos : round(move.pos));
            // Normally, this is triggered in render(), but if there
            // is nothing to render, we have to do it manually here.
            if (!dragging.init && pos.cur === pos.dest) {
                trigger('moveEnd');
            }
            // Update times for future iteration
            move.lastTime = move.now;
        }

        /**
         * Stops continuous movement.
         *
         * @return {Void}
         */
        self.stop = function () {
            if (dragging.source === 'button') {
                dragging.init = 0;
                dragging.released = 1;
            }
        };

        /**
         * Activate previous item.
         *
         * @return {Void}
         */
        self.prev = function () {
            self.activate(rel.activeItem == null ? 0 : rel.activeItem - 1);
        };

        /**
         * Activate next item.
         *
         * @return {Void}
         */
        self.next = function () {
            self.activate(rel.activeItem == null ? 0 : rel.activeItem + 1);
        };

        /**
         * Activate previous page.
         *
         * @return {Void}
         */
        self.prevPage = function () {
            self.activatePage(rel.activePage - 1);
        };

        /**
         * Activate next page.
         *
         * @return {Void}
         */
        self.nextPage = function () {
            self.activatePage(rel.activePage + 1);
        };

        /**
         * Slide SLIDEE by amount of pixels.
         *
         * @param {Int}  delta     Pixels/Items. Positive means forward, negative means backward.
         * @param {Bool} immediate Reposition immediately without an animation.
         *
         * @return {Void}
         */
        self.slideBy = function (delta, immediate) {
            if (!delta) {
                return;
            }
            if (itemNav) {
                self[centeredNav ? 'toCenter' : 'toStart'](
                    within((centeredNav ? rel.centerItem : rel.firstItem) + o.scrollBy * delta, 0, items.length)
                );
            } else {
                slideTo(pos.dest + delta, immediate);
            }
        };

        /**
         * Animate SLIDEE to a specific position.
         *
         * @param {Int}  pos       New position.
         * @param {Bool} immediate Reposition immediately without an animation.
         *
         * @return {Void}
         */
        self.slideTo = function (pos, immediate) {
            slideTo(pos, immediate);
        };

        /**
         * Core method for handling `toLocation` methods.
         *
         * @param  {String} location
         * @param  {Mixed}  item
         * @param  {Bool}   immediate
         *
         * @return {Void}
         */
        function to(location, item, immediate) {
            // Optional arguments logic
            if (type(item) === 'boolean') {
                immediate = item;
                item = undefined;
            }

            if (item === undefined) {
                slideTo(pos[location], immediate);
            } else {
                // You can't align items to sides of the frame
                // when centered navigation type is enabled
                if (centeredNav && location !== 'center') {
                    return;
                }

                var itemPos = self.getPos(item);
                if (itemPos) {
                    slideTo(itemPos[location], immediate, !centeredNav);
                }
            }
        }

        /**
         * Animate element or the whole SLIDEE to the start of the frame.
         *
         * @param {Mixed} item      Item DOM element, or index starting at 0. Omitting will animate SLIDEE.
         * @param {Bool}  immediate Reposition immediately without an animation.
         *
         * @return {Void}
         */
        self.toStart = function (item, immediate) {
            to('start', item, immediate);
        };

        /**
         * Animate element or the whole SLIDEE to the end of the frame.
         *
         * @param {Mixed} item      Item DOM element, or index starting at 0. Omitting will animate SLIDEE.
         * @param {Bool}  immediate Reposition immediately without an animation.
         *
         * @return {Void}
         */
        self.toEnd = function (item, immediate) {
            to('end', item, immediate);
        };

        /**
         * Animate element or the whole SLIDEE to the center of the frame.
         *
         * @param {Mixed} item      Item DOM element, or index starting at 0. Omitting will animate SLIDEE.
         * @param {Bool}  immediate Reposition immediately without an animation.
         *
         * @return {Void}
         */
        self.toCenter = function (item, immediate) {
            to('center', item, immediate);
        };

        /**
         * Get the index of an item in SLIDEE.
         *
         * @param {Mixed} item     Item DOM element.
         *
         * @return {Int}  Item index, or -1 if not found.
         */
        function getIndex(item) {
            return item != null ?
                isNumber(item) ?
                    item >= 0 && item < items.length ? item : -1 :
                    $items.index(item) :
                -1;
        }
        // Expose getIndex without lowering the compressibility of it,
        // as it is used quite often throughout Sly.
        self.getIndex = getIndex;

        /**
         * Get index of an item in SLIDEE based on a variety of input types.
         *
         * @param  {Mixed} item DOM element, positive or negative integer.
         *
         * @return {Int}   Item index, or -1 if not found.
         */
        function getRelativeIndex(item) {
            return getIndex(isNumber(item) && item < 0 ? item + items.length : item);
        }

        /**
         * Activates an item.
         *
         * @param  {Mixed} item Item DOM element, or index starting at 0.
         *
         * @return {Mixed} Activated item index or false on fail.
         */
        function activate(item, force) {
            var index = getIndex(item);

            if (!itemNav || index < 0) {
                return false;
            }

            // Update classes, last active index, and trigger active event only when there
            // has been a change. Otherwise just return the current active index.
            if (last.active !== index || force) {
                // Update classes
                $items.eq(rel.activeItem).removeClass(o.activeClass);
                $items.eq(index).addClass(o.activeClass);

                last.active = rel.activeItem = index;

                updateButtonsState();
                trigger('active', index);
            }

            return index;
        }

        /**
         * Activates an item and helps with further navigation when o.smart is enabled.
         *
         * @param {Mixed} item      Item DOM element, or index starting at 0.
         * @param {Bool}  immediate Whether to reposition immediately in smart navigation.
         *
         * @return {Void}
         */
        self.activate = function (item, immediate) {
            var index = activate(item);

            // Smart navigation
            if (o.smart && index !== false) {
                // When centeredNav is enabled, center the element.
                // Otherwise, determine where to position the element based on its current position.
                // If the element is currently on the far end side of the frame, assume that user is
                // moving forward and animate it to the start of the visible frame, and vice versa.
                if (centeredNav) {
                    self.toCenter(index, immediate);
                } else if (index >= rel.lastItem) {
                    self.toStart(index, immediate);
                } else if (index <= rel.firstItem) {
                    self.toEnd(index, immediate);
                } else {
                    resetCycle();
                }
            }
        };

        /**
         * Activates a page.
         *
         * @param {Int}  index     Page index, starting from 0.
         * @param {Bool} immediate Whether to reposition immediately without animation.
         *
         * @return {Void}
         */
        self.activatePage = function (index, immediate) {
            if (isNumber(index)) {
                slideTo(pages[within(index, 0, pages.length - 1)], immediate);
            }
        };

        /**
         * Return relative positions of items based on their visibility within FRAME.
         *
         * @param {Int} slideePos Position of SLIDEE.
         *
         * @return {Void}
         */
        function getRelatives(slideePos) {
            slideePos = within(isNumber(slideePos) ? slideePos : pos.dest, pos.start, pos.end);

            var relatives = {};
            var centerOffset = forceCenteredNav ? 0 : frameSize / 2;

            // Determine active page
            if (!parallax) {
                for (var p = 0, pl = pages.length; p < pl; p++) {
                    if (slideePos >= pos.end || p === pages.length - 1) {
                        relatives.activePage = pages.length - 1;
                        break;
                    }

                    if (slideePos <= pages[p] + centerOffset) {
                        relatives.activePage = p;
                        break;
                    }
                }
            }

            // Relative item indexes
            if (itemNav) {
                var first = false;
                var last = false;
                var center = false;

                // From start
                for (var i = 0, il = items.length; i < il; i++) {
                    // First item
                    if (first === false && slideePos <= items[i].start + items[i].half) {
                        first = i;
                    }

                    // Center item
                    if (center === false && slideePos <= items[i].center + items[i].half) {
                        center = i;
                    }

                    // Last item
                    if (i === il - 1 || slideePos <= items[i].end + items[i].half) {
                        last = i;
                        break;
                    }
                }

                // Safe assignment, just to be sure the false won't be returned
                relatives.firstItem = isNumber(first) ? first : 0;
                relatives.centerItem = isNumber(center) ? center : relatives.firstItem;
                relatives.lastItem = isNumber(last) ? last : relatives.centerItem;
            }

            return relatives;
        }

        /**
         * Update object with relative positions.
         *
         * @param {Int} newPos
         *
         * @return {Void}
         */
        function updateRelatives(newPos) {
            $.extend(rel, getRelatives(newPos));
        }

        /**
         * Disable navigation buttons when needed.
         *
         * Adds disabledClass, and when the button is <button> or <input>, activates :disabled state.
         *
         * @return {Void}
         */
        function updateButtonsState() {
            var isStart = pos.dest <= pos.start;
            var isEnd = pos.dest >= pos.end;
            var slideePosState = (isStart ? 1 : 0) | (isEnd ? 2 : 0);

            // Update paging buttons only if there has been a change in SLIDEE position
            if (last.slideePosState !== slideePosState) {
                last.slideePosState = slideePosState;

                if ($prevPageButton.is('button,input')) {
                    $prevPageButton.prop('disabled', isStart);
                }

                if ($nextPageButton.is('button,input')) {
                    $nextPageButton.prop('disabled', isEnd);
                }

                $prevPageButton.add($backwardButton)[isStart ? 'addClass' : 'removeClass'](o.disabledClass);
                $nextPageButton.add($forwardButton)[isEnd ? 'addClass' : 'removeClass'](o.disabledClass);
            }

            // Forward & Backward buttons need a separate state caching because we cannot "property disable"
            // them while they are being used, as disabled buttons stop emitting mouse events.
            if (last.fwdbwdState !== slideePosState && dragging.released) {
                last.fwdbwdState = slideePosState;

                if ($backwardButton.is('button,input')) {
                    $backwardButton.prop('disabled', isStart);
                }

                if ($forwardButton.is('button,input')) {
                    $forwardButton.prop('disabled', isEnd);
                }
            }

            // Item navigation
            if (itemNav && rel.activeItem != null) {
                var isFirst = rel.activeItem === 0;
                var isLast = rel.activeItem >= items.length - 1;
                var itemsButtonState = (isFirst ? 1 : 0) | (isLast ? 2 : 0);

                if (last.itemsButtonState !== itemsButtonState) {
                    last.itemsButtonState = itemsButtonState;

                    if ($prevButton.is('button,input')) {
                        $prevButton.prop('disabled', isFirst);
                    }

                    if ($nextButton.is('button,input')) {
                        $nextButton.prop('disabled', isLast);
                    }

                    $prevButton[isFirst ? 'addClass' : 'removeClass'](o.disabledClass);
                    $nextButton[isLast ? 'addClass' : 'removeClass'](o.disabledClass);
                }
            }
        }

        /**
         * Resume cycling.
         *
         * @param {Int} priority Resume pause with priority lower or equal than this. Used internally for pauseOnHover.
         *
         * @return {Void}
         */
        self.resume = function (priority) {
            if (!o.cycleBy || !o.cycleInterval || o.cycleBy === 'items' && (!items[0] || rel.activeItem == null) || priority < self.isPaused) {
                return;
            }

            self.isPaused = 0;

            if (cycleID) {
                cycleID = clearTimeout(cycleID);
            } else {
                trigger('resume');
            }

            cycleID = setTimeout(function () {
                trigger('cycle');
                switch (o.cycleBy) {
                    case 'items':
                        self.activate(rel.activeItem >= items.length - 1 ? 0 : rel.activeItem + 1);
                        break;

                    case 'pages':
                        self.activatePage(rel.activePage >= pages.length - 1 ? 0 : rel.activePage + 1);
                        break;
                }
            }, o.cycleInterval);
        };

        /**
         * Pause cycling.
         *
         * @param {Int} priority Pause priority. 100 is default. Used internally for pauseOnHover.
         *
         * @return {Void}
         */
        self.pause = function (priority) {
            if (priority < self.isPaused) {
                return;
            }

            self.isPaused = priority || 100;

            if (cycleID) {
                cycleID = clearTimeout(cycleID);
                trigger('pause');
            }
        };

        /**
         * Toggle cycling.
         *
         * @return {Void}
         */
        self.toggle = function () {
            self[cycleID ? 'pause' : 'resume']();
        };

        /**
         * Updates a signle or multiple option values.
         *
         * @param {Mixed} name  Name of the option that should be updated, or object that will extend the options.
         * @param {Mixed} value New option value.
         *
         * @return {Void}
         */
        self.set = function (name, value) {
            if ($.isPlainObject(name)) {
                $.extend(o, name);
            } else if (o.hasOwnProperty(name)) {
                o[name] = value;
            }
        };

        /**
         * Add one or multiple items to the SLIDEE end, or a specified position index.
         *
         * @param {Mixed} element Node element, or HTML string.
         * @param {Int}   index   Index of a new item position. By default item is appended at the end.
         *
         * @return {Void}
         */
        self.add = function (element, index) {
            var $element = $(element);

            if (itemNav) {
                // Insert the element(s)
                if (index == null || !items[0] || index >= items.length) {
                    $element.appendTo($slidee);
                } else if (items.length) {
                    $element.insertBefore(items[index].el);
                }

                // Adjust the activeItem index
                if (rel.activeItem != null && index <= rel.activeItem) {
                    last.active = rel.activeItem += $element.length;
                }
            } else {
                $slidee.append($element);
            }

            // Reload
            load();
        };

        /**
         * Remove an item from SLIDEE.
         *
         * @param {Mixed} element Item index, or DOM element.
         * @param {Int}   index   Index of a new item position. By default item is appended at the end.
         *
         * @return {Void}
         */
        self.remove = function (element) {
            if (itemNav) {
                var index = getRelativeIndex(element);

                if (index > -1) {
                    // Remove the element
                    $items.eq(index).remove();

                    // If the current item is being removed, activate new one after reload
                    var reactivate = index === rel.activeItem;

                    // Adjust the activeItem index
                    if (rel.activeItem != null && index < rel.activeItem) {
                        last.active = --rel.activeItem;
                    }

                    // Reload
                    load();

                    // Activate new item at the removed position
                    if (reactivate) {
                        last.active = null;
                        self.activate(rel.activeItem);
                    }
                }
            } else {
                $(element).remove();
                load();
            }
        };

        /**
         * Helps re-arranging items.
         *
         * @param  {Mixed} item     Item DOM element, or index starting at 0. Use negative numbers to select items from the end.
         * @param  {Mixed} position Item insertion anchor. Accepts same input types as item argument.
         * @param  {Bool}  after    Insert after instead of before the anchor.
         *
         * @return {Void}
         */
        function moveItem(item, position, after) {
            item = getRelativeIndex(item);
            position = getRelativeIndex(position);

            // Move only if there is an actual change requested
            if (item > -1 && position > -1 && item !== position && (!after || position !== item - 1) && (after || position !== item + 1)) {
                $items.eq(item)[after ? 'insertAfter' : 'insertBefore'](items[position].el);

                var shiftStart = item < position ? item : (after ? position : position - 1);
                var shiftEnd = item > position ? item : (after ? position + 1 : position);
                var shiftsUp = item > position;

                // Update activeItem index
                if (rel.activeItem != null) {
                    if (item === rel.activeItem) {
                        last.active = rel.activeItem = after ? (shiftsUp ? position + 1 : position) : (shiftsUp ? position : position - 1);
                    } else if (rel.activeItem > shiftStart && rel.activeItem < shiftEnd) {
                        last.active = rel.activeItem += shiftsUp ? 1 : -1;
                    }
                }

                // Reload
                load();
            }
        }

        /**
         * Move item after the target anchor.
         *
         * @param  {Mixed} item     Item to be moved. Can be DOM element or item index.
         * @param  {Mixed} position Target position anchor. Can be DOM element or item index.
         *
         * @return {Void}
         */
        self.moveAfter = function (item, position) {
            moveItem(item, position, 1);
        };

        /**
         * Move item before the target anchor.
         *
         * @param  {Mixed} item     Item to be moved. Can be DOM element or item index.
         * @param  {Mixed} position Target position anchor. Can be DOM element or item index.
         *
         * @return {Void}
         */
        self.moveBefore = function (item, position) {
            moveItem(item, position);
        };

        /**
         * Registers callbacks.
         *
         * @param  {Mixed} name  Event name, or callbacks map.
         * @param  {Mixed} fn    Callback, or an array of callback functions.
         *
         * @return {Void}
         */
        self.on = function (name, fn) {
            // Callbacks map
            if (type(name) === 'object') {
                for (var key in name) {
                    if (name.hasOwnProperty(key)) {
                        self.on(key, name[key]);
                    }
                }
                // Callback
            } else if (type(fn) === 'function') {
                var names = name.split(' ');
                for (var n = 0, nl = names.length; n < nl; n++) {
                    callbacks[names[n]] = callbacks[names[n]] || [];
                    if (callbackIndex(names[n], fn) === -1) {
                        callbacks[names[n]].push(fn);
                    }
                }
                // Callbacks array
            } else if (type(fn) === 'array') {
                for (var f = 0, fl = fn.length; f < fl; f++) {
                    self.on(name, fn[f]);
                }
            }
        };

        /**
         * Registers callbacks to be executed only once.
         *
         * @param  {Mixed} name  Event name, or callbacks map.
         * @param  {Mixed} fn    Callback, or an array of callback functions.
         *
         * @return {Void}
         */
        self.one = function (name, fn) {
            function proxy() {
                fn.apply(self, arguments);
                self.off(name, proxy);
            }
            self.on(name, proxy);
        };

        /**
         * Remove one or all callbacks.
         *
         * @param  {String} name Event name.
         * @param  {Mixed}  fn   Callback, or an array of callback functions. Omit to remove all callbacks.
         *
         * @return {Void}
         */
        self.off = function (name, fn) {
            if (fn instanceof Array) {
                for (var f = 0, fl = fn.length; f < fl; f++) {
                    self.off(name, fn[f]);
                }
            } else {
                var names = name.split(' ');
                for (var n = 0, nl = names.length; n < nl; n++) {
                    callbacks[names[n]] = callbacks[names[n]] || [];
                    if (fn == null) {
                        callbacks[names[n]].length = 0;
                    } else {
                        var index = callbackIndex(names[n], fn);
                        if (index !== -1) {
                            callbacks[names[n]].splice(index, 1);
                        }
                    }
                }
            }
        };

        /**
         * Returns callback array index.
         *
         * @param  {String}   name Event name.
         * @param  {Function} fn   Function
         *
         * @return {Int} Callback array index, or -1 if isn't registered.
         */
        function callbackIndex(name, fn) {
            for (var i = 0, l = callbacks[name].length; i < l; i++) {
                if (callbacks[name][i] === fn) {
                    return i;
                }
            }
            return -1;
        }

        /**
         * Reset next cycle timeout.
         *
         * @return {Void}
         */
        function resetCycle() {
            if (dragging.released && !self.isPaused) {
                self.resume();
            }
        }

        /**
         * Calculate SLIDEE representation of handle position.
         *
         * @param  {Int} handlePos
         *
         * @return {Int}
         */
        function handleToSlidee(handlePos) {
            return round(within(handlePos, hPos.start, hPos.end) / hPos.end * (pos.end - pos.start)) + pos.start;
        }

        /**
         * Keeps track of a dragging delta history.
         *
         * @return {Void}
         */
        function draggingHistoryTick() {
            // Looking at this, I know what you're thinking :) But as we need only 4 history states, doing it this way
            // as opposed to a proper loop is ~25 bytes smaller (when minified with GCC), a lot faster, and doesn't
            // generate garbage. The loop version would create 2 new variables on every tick. Unexaptable!
            dragging.history[0] = dragging.history[1];
            dragging.history[1] = dragging.history[2];
            dragging.history[2] = dragging.history[3];
            dragging.history[3] = dragging.delta;
        }

        /**
         * Initialize continuous movement.
         *
         * @return {Void}
         */
        function continuousInit(source) {
            dragging.released = 0;
            dragging.source = source;
            dragging.slidee = source === 'slidee';
        }

        /**
         * Dragging initiator.
         *
         * @param  {Event} event
         *
         * @return {Void}
         */
        function dragInit(event) {
            var isTouch = event.type === 'touchstart';
            var source = event.data.source;
            var isSlidee = source === 'slidee';

            // Ignore when already in progress, or interactive element in non-touch navivagion
            if (dragging.init || !isTouch && isInteractive(event.target)) {
                return;
            }

            // Handle dragging conditions
            if (source === 'handle' && (!o.dragHandle || hPos.start === hPos.end)) {
                return;
            }

            // SLIDEE dragging conditions
            if (isSlidee && !(isTouch ? o.touchDragging : o.mouseDragging && event.which < 2)) {
                return;
            }

            if (!isTouch) {
                // prevents native image dragging in Firefox
                stopDefault(event);
            }

            // Reset dragging object
            continuousInit(source);

            // Properties used in dragHandler
            dragging.init = 0;
            dragging.$source = $(event.target);
            dragging.touch = isTouch;
            dragging.pointer = isTouch ? event.originalEvent.touches[0] : event;
            dragging.initX = dragging.pointer.pageX;
            dragging.initY = dragging.pointer.pageY;
            dragging.initPos = isSlidee ? pos.cur : hPos.cur;
            dragging.start = +new Date();
            dragging.time = 0;
            dragging.path = 0;
            dragging.delta = 0;
            dragging.locked = 0;
            dragging.history = [0, 0, 0, 0];
            dragging.pathToLock = isSlidee ? isTouch ? 30 : 10 : 0;

            // Bind dragging events
            $doc.on(isTouch ? dragTouchEvents : dragMouseEvents, dragHandler);

            // Pause ongoing cycle
            self.pause(1);

            // Add dragging class
            (isSlidee ? $slidee : $handle).addClass(o.draggedClass);

            // Trigger moveStart event
            trigger('moveStart');

            // Keep track of a dragging path history. This is later used in the
            // dragging release swing calculation when dragging SLIDEE.
            if (isSlidee) {
                historyID = setInterval(draggingHistoryTick, 10);
            }
        }

        /**
         * Handler for dragging scrollbar handle or SLIDEE.
         *
         * @param  {Event} event
         *
         * @return {Void}
         */
        function dragHandler(event) {
            dragging.released = event.type === 'mouseup' || event.type === 'touchend';
            dragging.pointer = dragging.touch ? event.originalEvent[dragging.released ? 'changedTouches' : 'touches'][0] : event;
            dragging.pathX = dragging.pointer.pageX - dragging.initX;
            dragging.pathY = dragging.pointer.pageY - dragging.initY;
            dragging.path = sqrt(pow(dragging.pathX, 2) + pow(dragging.pathY, 2));
            dragging.delta = o.horizontal ? dragging.pathX : dragging.pathY;

            if (!dragging.released && dragging.path < 1) return;

            // We haven't decided whether this is a drag or not...
            if (!dragging.init) {
                // If the drag path was very short, maybe it's not a drag?
                if (dragging.path < o.dragThreshold) {
                    // If the pointer was released, the path will not become longer and it's
                    // definitely not a drag. If not released yet, decide on next iteration
                    return dragging.released ? dragEnd() : undefined;
                }
                else {
                    // If dragging path is sufficiently long we can confidently start a drag
                    // if drag is in different direction than scroll, ignore it
                    if (o.horizontal ? abs(dragging.pathX) > abs(dragging.pathY) : abs(dragging.pathX) < abs(dragging.pathY)) {
                        dragging.init = 1;
                    } else {
                        return dragEnd();
                    }
                }
            }

            stopDefault(event);

            // Disable click on a source element, as it is unwelcome when dragging
            if (!dragging.locked && dragging.path > dragging.pathToLock && dragging.slidee) {
                dragging.locked = 1;
                dragging.$source.on(clickEvent, disableOneEvent);
            }

            // Cancel dragging on release
            if (dragging.released) {
                dragEnd();

                // Adjust path with a swing on mouse release
                if (o.releaseSwing && dragging.slidee) {
                    dragging.swing = (dragging.delta - dragging.history[0]) / 40 * 300;
                    dragging.delta += dragging.swing;
                    dragging.tweese = abs(dragging.swing) > 10;
                }
            }

            slideTo(dragging.slidee ? round(dragging.initPos - dragging.delta) : handleToSlidee(dragging.initPos + dragging.delta));
        }

        /**
         * Stops dragging and cleans up after it.
         *
         * @return {Void}
         */
        function dragEnd() {
            clearInterval(historyID);
            dragging.released = true;
            $doc.off(dragging.touch ? dragTouchEvents : dragMouseEvents, dragHandler);
            (dragging.slidee ? $slidee : $handle).removeClass(o.draggedClass);

            // Make sure that disableOneEvent is not active in next tick.
            setTimeout(function () {
                dragging.$source.off(clickEvent, disableOneEvent);
            });

            // Normally, this is triggered in render(), but if there
            // is nothing to render, we have to do it manually here.
            if (pos.cur === pos.dest && dragging.init) {
                trigger('moveEnd');
            }

            // Resume ongoing cycle
            self.resume(1);

            dragging.init = 0;
        }

        /**
         * Check whether element is interactive.
         *
         * @return {Boolean}
         */
        function isInteractive(element) {
            return ~$.inArray(element.nodeName, interactiveElements) || $(element).is(o.interactive);
        }

        /**
         * Continuous movement cleanup on mouseup.
         *
         * @return {Void}
         */
        function movementReleaseHandler() {
            self.stop();
            $doc.off('mouseup', movementReleaseHandler);
        }

        /**
         * Buttons navigation handler.
         *
         * @param  {Event} event
         *
         * @return {Void}
         */
        function buttonsHandler(event) {
            /*jshint validthis:true */
            stopDefault(event);
            switch (this) {
                case $forwardButton[0]:
                case $backwardButton[0]:
                    self.moveBy($forwardButton.is(this) ? o.moveBy : -o.moveBy);
                    $doc.on('mouseup', movementReleaseHandler);
                    break;

                case $prevButton[0]:
                    self.prev();
                    break;

                case $nextButton[0]:
                    self.next();
                    break;

                case $prevPageButton[0]:
                    self.prevPage();
                    break;

                case $nextPageButton[0]:
                    self.nextPage();
                    break;
            }
        }

        /**
         * Mouse wheel delta normalization.
         *
         * @param  {Event} event
         *
         * @return {Int}
         */
        function normalizeWheelDelta(event) {
            // wheelDelta needed only for IE8-
            scrolling.curDelta = ((o.horizontal ? event.deltaY || event.deltaX : event.deltaY) || -event.wheelDelta);
            scrolling.curDelta /= event.deltaMode === 1 ? 3 : 100;
            if (!itemNav) {
                return scrolling.curDelta;
            }
            time = +new Date();
            if (scrolling.last < time - scrolling.resetTime) {
                scrolling.delta = 0;
            }
            scrolling.last = time;
            scrolling.delta += scrolling.curDelta;
            if (abs(scrolling.delta) < 1) {
                scrolling.finalDelta = 0;
            } else {
                scrolling.finalDelta = round(scrolling.delta / 1);
                scrolling.delta %= 1;
            }
            return scrolling.finalDelta;
        }

        /**
         * Mouse scrolling handler.
         *
         * @param  {Event} event
         *
         * @return {Void}
         */
        function scrollHandler(event) {
            // Mark event as originating in a Sly instance
            event.originalEvent[namespace] = self;
            // Don't hijack global scrolling
            var time = +new Date();
            if (lastGlobalWheel + o.scrollHijack > time && $scrollSource[0] !== document && $scrollSource[0] !== window) {
                lastGlobalWheel = time;
                return;
            }
            // Ignore if there is no scrolling to be done
            if (!o.scrollBy || pos.start === pos.end) {
                return;
            }
            var delta = normalizeWheelDelta(event.originalEvent);
            // Trap scrolling only when necessary and/or requested
            if (o.scrollTrap || delta > 0 && pos.dest < pos.end || delta < 0 && pos.dest > pos.start) {
                stopDefault(event, 1);
            }
            self.slideBy(o.scrollBy * delta);
        }

        /**
         * Scrollbar click handler.
         *
         * @param  {Event} event
         *
         * @return {Void}
         */
        function scrollbarHandler(event) {
            // Only clicks on scroll bar. Ignore the handle.
            if (o.clickBar && event.target === $sb[0]) {
                stopDefault(event);
                // Calculate new handle position and sync SLIDEE to it
                slideTo(handleToSlidee((o.horizontal ? event.pageX - $sb.offset().left : event.pageY - $sb.offset().top) - handleSize / 2));
            }
        }

        /**
         * Keyboard input handler.
         *
         * @param  {Event} event
         *
         * @return {Void}
         */
        function keyboardHandler(event) {
            if (!o.keyboardNavBy) {
                return;
            }

            switch (event.which) {
                // Left or Up
                case o.horizontal ? 37 : 38:
                    stopDefault(event);
                    self[o.keyboardNavBy === 'pages' ? 'prevPage' : 'prev']();
                    break;

                // Right or Down
                case o.horizontal ? 39 : 40:
                    stopDefault(event);
                    self[o.keyboardNavBy === 'pages' ? 'nextPage' : 'next']();
                    break;
            }
        }

        /**
         * Click on item activation handler.
         *
         * @param  {Event} event
         *
         * @return {Void}
         */
        function activateHandler(event) {
            /*jshint validthis:true */

            // Ignore clicks on interactive elements.
            if (isInteractive(this)) {
                event.originalEvent[namespace + 'ignore'] = true;
                return;
            }

            // Ignore events that:
            // - are not originating from direct SLIDEE children
            // - originated from interactive elements
            if (this.parentNode !== $slidee[0] || event.originalEvent[namespace + 'ignore']) return;

            self.activate(this);
        }

        /**
         * Click on page button handler.
         *
         * @param {Event} event
         *
         * @return {Void}
         */
        function activatePageHandler() {
            /*jshint validthis:true */
            // Accept only events from direct pages bar children.
            if (this.parentNode === $pb[0]) {
                self.activatePage($pages.index(this));
            }
        }

        /**
         * Pause on hover handler.
         *
         * @param  {Event} event
         *
         * @return {Void}
         */
        function pauseOnHoverHandler(event) {
            if (o.pauseOnHover) {
                self[event.type === 'mouseenter' ? 'pause' : 'resume'](2);
            }
        }

        /**
         * Trigger callbacks for event.
         *
         * @param  {String} name Event name.
         * @param  {Mixed}  argX Arguments passed to callbacks.
         *
         * @return {Void}
         */
        function trigger(name, arg1) {
            if (callbacks[name]) {
                l = callbacks[name].length;
                // Callbacks will be stored and executed from a temporary array to not
                // break the execution queue when one of the callbacks unbinds itself.
                tmpArray.length = 0;
                for (i = 0; i < l; i++) {
                    tmpArray.push(callbacks[name][i]);
                }
                // Execute the callbacks
                for (i = 0; i < l; i++) {
                    tmpArray[i].call(self, name, arg1);
                }
            }
        }

        /**
         * Destroys instance and everything it created.
         *
         * @return {Void}
         */
        self.destroy = function () {
            // Remove the reference to itself
            Sly.removeInstance(frame);

            // Unbind all events
            $scrollSource
                .add($handle)
                .add($sb)
                .add($pb)
                .add($forwardButton)
                .add($backwardButton)
                .add($prevButton)
                .add($nextButton)
                .add($prevPageButton)
                .add($nextPageButton)
                .off('.' + namespace);

            // Unbinding specifically as to not nuke out other instances
            $doc.off('keydown', keyboardHandler);

            // Remove classes
            $prevButton
                .add($nextButton)
                .add($prevPageButton)
                .add($nextPageButton)
                .removeClass(o.disabledClass);

            if ($items && rel.activeItem != null) {
                $items.eq(rel.activeItem).removeClass(o.activeClass);
            }

            // Remove page items
            $pb.empty();

            if (!parallax) {
                // Unbind events from frame
                $frame.off('.' + namespace);
                // Restore original styles
                frameStyles.restore();
                slideeStyles.restore();
                sbStyles.restore();
                handleStyles.restore();
                // Remove the instance from element data storage
                $.removeData(frame, namespace);
            }

            // Clean up collections
            items.length = pages.length = 0;
            last = {};

            // Reset initialized status and return the instance
            self.initialized = 0;
            return self;
        };

        /**
         * Initialize.
         *
         * @return {Object}
         */
        self.init = function () {
            if (self.initialized) {
                return;
            }

            // Disallow multiple instances on the same element
            if (Sly.getInstance(frame)) throw new Error('There is already a Sly instance on this element');

            // Store the reference to itself
            Sly.storeInstance(frame, self);

            // Register callbacks map
            self.on(callbackMap);

            // Save styles
            var holderProps = ['overflow', 'position'];
            var movableProps = ['position', 'webkitTransform', 'msTransform', 'transform', 'left', 'top', 'width', 'height'];
            frameStyles.save.apply(frameStyles, holderProps);
            sbStyles.save.apply(sbStyles, holderProps);
            slideeStyles.save.apply(slideeStyles, movableProps);
            handleStyles.save.apply(handleStyles, movableProps);

            // Set required styles
            var $movables = $handle;
            if (!parallax) {
                $movables = $movables.add($slidee);
                $frame.css('overflow', 'hidden');
                if (!transform && $frame.css('position') === 'static') {
                    $frame.css('position', 'relative');
                }
            }
            if (transform) {
                if (gpuAcceleration) {
                    $movables.css(transform, gpuAcceleration);
                }
            } else {
                if ($sb.css('position') === 'static') {
                    $sb.css('position', 'relative');
                }
                $movables.css({ position: 'absolute' });
            }

            // Navigation buttons
            if (o.forward) {
                $forwardButton.on(mouseDownEvent, buttonsHandler);
            }
            if (o.backward) {
                $backwardButton.on(mouseDownEvent, buttonsHandler);
            }
            if (o.prev) {
                $prevButton.on(clickEvent, buttonsHandler);
            }
            if (o.next) {
                $nextButton.on(clickEvent, buttonsHandler);
            }
            if (o.prevPage) {
                $prevPageButton.on(clickEvent, buttonsHandler);
            }
            if (o.nextPage) {
                $nextPageButton.on(clickEvent, buttonsHandler);
            }

            // Scrolling navigation
            $scrollSource.on(wheelEvent, scrollHandler);

            // Clicking on scrollbar navigation
            if ($sb[0]) {
                $sb.on(clickEvent, scrollbarHandler);
            }

            // Click on items navigation
            if (itemNav && o.activateOn) {
                $frame.on(o.activateOn + '.' + namespace, '*', activateHandler);
            }

            // Pages navigation
            if ($pb[0] && o.activatePageOn) {
                $pb.on(o.activatePageOn + '.' + namespace, '*', activatePageHandler);
            }

            // Dragging navigation
            $dragSource.on(dragInitEvents, { source: 'slidee' }, dragInit);

            // Scrollbar dragging navigation
            if ($handle) {
                $handle.on(dragInitEvents, { source: 'handle' }, dragInit);
            }

            // Keyboard navigation
            $doc.on('keydown', keyboardHandler);

            if (!parallax) {
                // Pause on hover
                $frame.on('mouseenter.' + namespace + ' mouseleave.' + namespace, pauseOnHoverHandler);
                // Reset native FRAME element scroll
                $frame.on('scroll.' + namespace, resetScroll);
            }

            // Mark instance as initialized
            self.initialized = 1;

            // Load
            load(true);

            // Initiate automatic cycling
            if (o.cycleBy && !parallax) {
                self[o.startPaused ? 'pause' : 'resume']();
            }

            // Return instance
            return self;
        };
    }

    Sly.getInstance = function (element) {
        return $.data(element, namespace);
    };

    Sly.storeInstance = function (element, sly) {
        return $.data(element, namespace, sly);
    };

    Sly.removeInstance = function (element) {
        return $.removeData(element, namespace);
    };

    /**
     * Return type of the value.
     *
     * @param  {Mixed} value
     *
     * @return {String}
     */
    function type(value) {
        if (value == null) {
            return String(value);
        }

        if (typeof value === 'object' || typeof value === 'function') {
            return Object.prototype.toString.call(value).match(/\s([a-z]+)/i)[1].toLowerCase() || 'object';
        }

        return typeof value;
    }

    /**
     * Event preventDefault & stopPropagation helper.
     *
     * @param {Event} event     Event object.
     * @param {Bool}  noBubbles Cancel event bubbling.
     *
     * @return {Void}
     */
    function stopDefault(event, noBubbles) {
        event.preventDefault();
        if (noBubbles) {
            event.stopPropagation();
        }
    }

    /**
     * Disables an event it was triggered on and unbinds itself.
     *
     * @param  {Event} event
     *
     * @return {Void}
     */
    function disableOneEvent(event) {
        /*jshint validthis:true */
        stopDefault(event, 1);
        $(this).off(event.type, disableOneEvent);
    }

    /**
     * Resets native element scroll values to 0.
     *
     * @return {Void}
     */
    function resetScroll() {
        /*jshint validthis:true */
        this.scrollLeft = 0;
        this.scrollTop = 0;
    }

    /**
     * Check if variable is a number.
     *
     * @param {Mixed} value
     *
     * @return {Boolean}
     */
    function isNumber(value) {
        return !isNaN(parseFloat(value)) && isFinite(value);
    }

    /**
     * Parse style to pixels.
     *
     * @param {Object}   $item    jQuery object with element.
     * @param {Property} property CSS property to get the pixels from.
     *
     * @return {Int}
     */
    function getPx($item, property) {
        return 0 | round(String($item.css(property)).replace(/[^\-0-9.]/g, ''));
    }

    /**
     * Make sure that number is within the limits.
     *
     * @param {Number} number
     * @param {Number} min
     * @param {Number} max
     *
     * @return {Number}
     */
    function within(number, min, max) {
        return number < min ? min : number > max ? max : number;
    }

    /**
     * Saves element styles for later restoration.
     *
     * Example:
     *   var styles = new StyleRestorer(frame);
     *   styles.save('position');
     *   element.style.position = 'absolute';
     *   styles.restore(); // restores to state before the assignment above
     *
     * @param {Element} element
     */
    function StyleRestorer(element) {
        var self = {};
        self.style = {};
        self.save = function () {
            if (!element || !element.nodeType) return;
            for (var i = 0; i < arguments.length; i++) {
                self.style[arguments[i]] = element.style[arguments[i]];
            }
            return self;
        };
        self.restore = function () {
            if (!element || !element.nodeType) return;
            for (var prop in self.style) {
                if (self.style.hasOwnProperty(prop)) element.style[prop] = self.style[prop];
            }
            return self;
        };
        return self;
    }

    // Local WindowAnimationTiming interface polyfill
    (function (w) {
        rAF = w.requestAnimationFrame
            || w.webkitRequestAnimationFrame
            || fallback;

        /**
         * Fallback implementation.
         */
        var prev = new Date().getTime();
        function fallback(fn) {
            var curr = new Date().getTime();
            var ms = Math.max(0, 16 - (curr - prev));
            var req = setTimeout(fn, ms);
            prev = curr;
            return req;
        }

        /**
         * Cancel.
         */
        var cancel = w.cancelAnimationFrame
            || w.webkitCancelAnimationFrame
            || w.clearTimeout;

        cAF = function(id){
            cancel.call(w, id);
        };
    }(window));

    // Feature detects
    (function () {
        var prefixes = ['', 'Webkit', 'Moz', 'ms', 'O'];
        var el = document.createElement('div');

        function testProp(prop) {
            for (var p = 0, pl = prefixes.length; p < pl; p++) {
                var prefixedProp = prefixes[p] ? prefixes[p] + prop.charAt(0).toUpperCase() + prop.slice(1) : prop;
                if (el.style[prefixedProp] != null) {
                    return prefixedProp;
                }
            }
        }

        // Global support indicators
        transform = testProp('transform');
        gpuAcceleration = testProp('perspective') ? 'translateZ(0) ' : '';
    }());

    // Expose class globally
    w[className] = Sly;

    // jQuery proxy
    $.fn[pluginName] = function (options, callbackMap) {
        var method, methodArgs;

        // Attributes logic
        if (!$.isPlainObject(options)) {
            if (type(options) === 'string' || options === false) {
                method = options === false ? 'destroy' : options;
                methodArgs = Array.prototype.slice.call(arguments, 1);
            }
            options = {};
        }

        // Apply to all elements
        return this.each(function (i, element) {
            // Call with prevention against multiple instantiations
            var plugin = Sly.getInstance(element);

            if (!plugin && !method) {
                // Create a new object if it doesn't exist yet
                plugin = new Sly(element, options, callbackMap).init();
            } else if (plugin && method) {
                // Call method
                if (plugin[method]) {
                    plugin[method].apply(plugin, methodArgs);
                }
            }
        });
    };

    // Default options
    Sly.defaults = {
        slidee:     null,  // Selector, DOM element, or jQuery object with DOM element representing SLIDEE.
        horizontal: false, // Switch to horizontal mode.

        // Item based navigation
        itemNav:        null,  // Item navigation type. Can be: 'basic', 'centered', 'forceCentered'.
        itemSelector:   null,  // Select only items that match this selector.
        smart:          false, // Repositions the activated item to help with further navigation.
        activateOn:     null,  // Activate an item on this event. Can be: 'click', 'mouseenter', ...
        activateMiddle: false, // Always activate the item in the middle of the FRAME. forceCentered only.

        // Scrolling
        scrollSource: null,  // Element for catching the mouse wheel scrolling. Default is FRAME.
        scrollBy:     0,     // Pixels or items to move per one mouse scroll. 0 to disable scrolling.
        scrollHijack: 300,   // Milliseconds since last wheel event after which it is acceptable to hijack global scroll.
        scrollTrap:   false, // Don't bubble scrolling when hitting scrolling limits.

        // Dragging
        dragSource:    null,  // Selector or DOM element for catching dragging events. Default is FRAME.
        mouseDragging: false, // Enable navigation by dragging the SLIDEE with mouse cursor.
        touchDragging: false, // Enable navigation by dragging the SLIDEE with touch events.
        releaseSwing:  false, // Ease out on dragging swing release.
        swingSpeed:    0.2,   // Swing synchronization speed, where: 1 = instant, 0 = infinite.
        elasticBounds: false, // Stretch SLIDEE position limits when dragging past FRAME boundaries.
        dragThreshold: 3,     // Distance in pixels before Sly recognizes dragging.
        interactive:   null,  // Selector for special interactive elements.

        // Scrollbar
        scrollBar:     null,  // Selector or DOM element for scrollbar container.
        dragHandle:    false, // Whether the scrollbar handle should be draggable.
        dynamicHandle: false, // Scrollbar handle represents the ratio between hidden and visible content.
        minHandleSize: 50,    // Minimal height or width (depends on sly direction) of a handle in pixels.
        clickBar:      false, // Enable navigation by clicking on scrollbar.
        syncSpeed:     0.5,   // Handle => SLIDEE synchronization speed, where: 1 = instant, 0 = infinite.

        // Pagesbar
        pagesBar:       null, // Selector or DOM element for pages bar container.
        activatePageOn: null, // Event used to activate page. Can be: click, mouseenter, ...
        pageBuilder:          // Page item generator.
            function (index) {
                return '<li>' + (index + 1) + '</li>';
            },

        // Navigation buttons
        forward:  null, // Selector or DOM element for "forward movement" button.
        backward: null, // Selector or DOM element for "backward movement" button.
        prev:     null, // Selector or DOM element for "previous item" button.
        next:     null, // Selector or DOM element for "next item" button.
        prevPage: null, // Selector or DOM element for "previous page" button.
        nextPage: null, // Selector or DOM element for "next page" button.

        // Automated cycling
        cycleBy:       null,  // Enable automatic cycling by 'items' or 'pages'.
        cycleInterval: 5000,  // Delay between cycles in milliseconds.
        pauseOnHover:  false, // Pause cycling when mouse hovers over the FRAME.
        startPaused:   false, // Whether to start in paused sate.

        // Mixed options
        moveBy:        300,     // Speed in pixels per second used by forward and backward buttons.
        speed:         0,       // Animations speed in milliseconds. 0 to disable animations.
        easing:        'swing', // Easing for duration based (tweening) animations.
        startAt:       null,    // Starting offset in pixels or items.
        keyboardNavBy: null,    // Enable keyboard navigation by 'items' or 'pages'.

        // Classes
        draggedClass:  'dragged', // Class for dragged elements (like SLIDEE or scrollbar handle).
        activeClass:   'active',  // Class for active items and pages.
        disabledClass: 'disabled' // Class for disabled navigation elements.
    };
}(jQuery, window));
/** krpano-content/krpano-wrapper-style/js/diffplayer/rooms_collection.js **/
'use strict';

/**
 * An object-like collection of rooms.
 * @param rooms
 * @see window.tour.jsonData.activeRoom object
 * @constructor
 */
function RoomsCollection(rooms) {
    this.rooms = rooms || {};
}

RoomsCollection.prototype.add = function(room) {
    this.rooms[room.id] = room;
};

RoomsCollection.prototype.remove = function(id) {
    delete this.rooms[id];
};

RoomsCollection.prototype.getById = function(id) {
    return this.rooms[id];
};

RoomsCollection.prototype.getByFilename = function(filename) {
    for (var k in this.rooms) {
        if (this.rooms[k].filename === filename) return this.rooms[k];
    }
    return null;
};

/** krpano-content/krpano-wrapper-style/js/diffplayer/diff_controller.js **/
'use strict';

/**
 * Diffplayer controller. Handles all interactions between ui and user.
 * @param diffModel
 * @constructor
 */
function DiffController(diffModel) {
    this.diffModel = diffModel;
    this.detectedEvent = diffModel.detectedEvent;
    this.galleryOpened = true;

    this.krpanoLeft = null;
    this.krpanoRight = null;

    /* selectors */
    this.sContent = ".krpano-content";
    this.sHeader = ".tour-changer";
    this.sCompare = ".js-compare";

    this.sTimeline = ".timeline";
    this.sTimelineAsTable = ".timeline_table";
    this.sTimelineFrame = ".timeline__frame";
    this.sTimelineList = ".timeline__list";
    this.sTimelineItem = ".timeline__item";
    this.sTimelineLink = ".timeline__link";
    this.sTimelineItemActive = ".timeline__item_active";
    this.sTimelineTriangle = ".timeline__triangle";

    this.sModalContainer = ".krpano-content-modal";
    this.sModal = ".js-popup";
    this.sModalContent = ".modal-content";
    this.sModalHeader = ".diff-modal__header";
    this.sModalTimeline = ".diff-modal__timeline";
    this.sModalFooter = ".diff-modal__footer";

    this.sPlayerContainer = ".js-diffcontainer";
    this.sPlayer = ".js-diffplayer";
    this.sPlayerLeft = ".js-diffplayer-left";
    this.sPlayerRight = ".js-diffplayer-right";
    this.sArrow = ".timeline__arrow";

    this.sSyncBtn = ".js-sync";
    this.sModeBtn = ".js-mode";

    this.sGalleryIcon = ".js-gallery-btn";
    this.sGalleryIconMobile = "#gallery-switcher .panel-button";

    this.namespace = ".diffplayer";

    var _this = this, loadedFirstTime = false;
    $(window).on("templatesLoaded", function() { //load event
        if (!loadedFirstTime) {
            loadedFirstTime = true;
            _this.initPopup();
        }
        _this.handleRoomChange();
        _this.handleCompareButton();
        _this.controlGallery();
    });
    $(window).on("resize", function() {
        _this.optimizeUI();
    });
}

DiffController.prototype.addHeader = function() {
    var _this = this, nActive = -1;
    var header = new EJS({url: window.baseUrl +'/krpano-wrapper-style/html/diff_player/header.ejs'}).render(window.jsonData);
    $(this.sHeader).remove();
    $(this.sContent).append(header);

    var $frame = $(".timeline__frame", this.sHeader);
    $frame.sly({
        horizontal: 1,
        itemNav: 'basic',
        smart: 1,
        activateOn: 'click',
        mouseDragging: 1,
        touchDragging: 1,
        releaseSwing: 1,
        startAt: null,
        scrollBar: $(".scrollbar", this.sHeader),
        scrollBy: 1,
        activatePageOn: 'click',
        speed: 300,
        elasticBounds: 1,
        easing: 'easeOutExpo',
        dragHandle: 1,
        dynamicHandle: 1,
        clickBar: 1,
        activeClass: "timeline__item_active"
    });
    if ((nActive = this.diffModel.getActiveRoomIndexInTimeline()) > -1) {
        $frame.sly('activate', nActive);
    }

    $(window)
        .off("resize.timelineRender")
        .on("resize.timelineRender", function() {
            _this.updateTriangles();
            _this.updateScrolling();
            $frame.sly("reload");
        })
        .trigger("resize.timelineRender");

    this.handleTourSelecting();
};

DiffController.prototype.handleRoomChange = function() {
    var _this = this;
    $(document)
        .off("krpano:loadscene" + this.namespace)
        .on("krpano:loadscene" + this.namespace, function() {
            _this.diffModel.setActiveRoom(window.jsonData.tour.activeRoom);
            _this.addHeader();
            _this.optimizeUI();
            _this.updateTriangles();
            if ("timelineRooms" in window.jsonData.tour.activeRoom)
                _this.diffModel.setFirstDiffRoom(window.jsonData.tour.activeRoom, true); //find and copy room
            _this.updatePopupTimeline();
        });
};

DiffController.prototype.initPopup = function() {
    //init container for popup outside of .krpano-content, which won't be removed every scene reload
    $(this.sContent).after("<div class='" + this.sModalContainer.substring(1) + "'></div>");

    var popup = new EJS({url: window.baseUrl +'/krpano-wrapper-style/html/diff_player/popup.ejs'}).render(window.jsonData);
    $(this.sModalContainer).append(popup);

    this.updatePopupTimeline();
    this.updatePopupFooter();

    this.embedDiffPano();
};

DiffController.prototype.handleCompareButton = function() {
    var _this = this;
    $(this.sContent)
        .off(this.detectedEvent, this.sCompare)
        .on(this.detectedEvent, this.sCompare, function() {
            _this.updateStereoImages();
            _this.updatePopupFooter();
            $(_this.sModal).modal('show');
        });

    $(this.sModalContainer)
        .off('shown.bs.modal' + this.namespace, this.sModal)
        .on('shown.bs.modal' + this.namespace, this.sModal, function () {
            _this.updateTriangles();
            _this.updatePanoHeight();

            var $frame = $(".timeline__frame", _this.sModal);
            $frame.sly({
                horizontal: 1,
                itemNav: 'basic',
                smart: 1,
                activateOn: 'click',
                mouseDragging: 1,
                touchDragging: 1,
                releaseSwing: 1,
                startAt: null,
                scrollBar: $(".scrollbar", _this.sModal),
                scrollBy: 1,
                activatePageOn: 'click',
                speed: 300,
                elasticBounds: 1,
                easing: 'easeOutExpo',
                dragHandle: 1,
                dynamicHandle: 1,
                clickBar: 1,
                activeClass: "timeline__item_active"
            });

            $(window)
                .off("resize.modalRender")
                .on("resize.modalRender", function() {
                    _this.updatePanoHeight();
                    $frame.sly("reload");
                });
        });
};

/**
 * do show gallery or not.
 */
DiffController.prototype.controlGallery = function() {
    var _this = this;
    if (_this.galleryOpened) {
        _this.diffModel.interaction.showGallery();
        $(_this.sGalleryIcon).addClass("active");
    } else {
        _this.diffModel.interaction.hideGallery();
        $(_this.sGalleryIcon).removeClass("active");
    }
    $(_this.sGalleryIcon + ", " + _this.sGalleryIconMobile)
        .off(_this.detectedEvent + _this.namespace)
        .on(_this.detectedEvent + _this.namespace, function() {
            _this.galleryOpened = !_this.galleryOpened;
        });
};

/**
 * Handle header timeline
 */
DiffController.prototype.handleTourSelecting = function() {
    var _this = this;
    $(this.sHeader)
        .off(this.detectedEvent, this.sTimelineItem)
        .on(this.detectedEvent, this.sTimelineItem, function() {
            $(_this.sTimelineItem, _this.sHeader).removeClass(_this.sTimelineItemActive.substr(1));
            $(this).addClass(_this.sTimelineItemActive.substr(1));
            var filename = $(this).find(_this.sTimelineLink).data("filename");
            if (filename) {
                _this.diffModel.setActiveRoom(_this.diffModel.arRooms.getByFilename(filename));
                _this.diffModel.setSecondDiffRoom(_this.diffModel.arRooms.getByFilename(filename), true);  //find and copy room
                _this.diffModel.switchRoomAndTour(filename);
            }
        });

    /* functions to iterate siblings circularly */
    $.fn.loopNext = function(selector){
        selector = selector || '';
        return this.next(selector).length ? this.next(selector)[0] : this.siblings(selector).addBack(selector).first();
    };
    $.fn.loopPrev = function(selector){
        selector = selector || '';
        return this.prev(selector).length ? this.prev(selector)[0] : this.siblings(selector).addBack(selector).last();
    };

    $(this.sHeader)
        .off(this.detectedEvent, this.sArrow)
        .on(this.detectedEvent, this.sArrow, function() {
            var method = $(this).data("side") === "left" ? "loopPrev" : "loopNext";
            var curItem = $(this).parent().find(_this.sTimelineItemActive);
            var nextOrPrev = curItem[method].apply(curItem, curItem);

            var filename = $(_this.sTimelineLink, nextOrPrev).data("filename");
            if (filename) {
                _this.diffModel.setActiveRoom(_this.diffModel.arRooms.getByFilename(filename));
                _this.diffModel.setSecondDiffRoom(_this.diffModel.arRooms.getByFilename(filename), true);
                _this.diffModel.switchRoomAndTour(filename);
            }
        });
};

DiffController.prototype.handlePopupTimeline = function() {
    var _this = this;

    $(this.sModal)
        .off(this.detectedEvent, this.sTimelineItem)
        .on(this.detectedEvent, this.sTimelineItem, function() {
            var $pTimeline = $(this).parent().parent().parent();
            $(_this.sTimelineItem, $pTimeline).removeClass(_this.sTimelineItemActive.substr(1));
            $(this).addClass(_this.sTimelineItemActive.substr(1));
            var filename = $(this).find(_this.sTimelineLink).data("filename");
            var playerSideId = $pTimeline.data("id");
            if (filename) {
                if (playerSideId === 0) _this.diffModel.setFirstDiffRoom(_this.diffModel.arRooms.getByFilename(filename));
                else _this.diffModel.setSecondDiffRoom(_this.diffModel.arRooms.getByFilename(filename));

                _this.updateStereoImages();
            }
        });

    $(this.sModal)
        .off(this.detectedEvent, this.sArrow)
        .on(this.detectedEvent, this.sArrow, function() {
            var $pTimeline = $(this).parent();

            var method = $(this).data("side") === "left" ? "loopPrev" : "loopNext";
            var curItem = $(this).parent().find(_this.sTimelineItemActive);
            var nextOrPrev = curItem[method].apply(curItem, curItem);
            $(_this.sTimelineItem, $pTimeline).removeClass(_this.sTimelineItemActive.substr(1));
            $(nextOrPrev).addClass(_this.sTimelineItemActive.substr(1));

            var filename = $(_this.sTimelineLink, nextOrPrev).data("filename");
            var playerSideId = $pTimeline.data("id");
            if (filename) {
                if (playerSideId === 0) _this.diffModel.setFirstDiffRoom(_this.diffModel.arRooms.getByFilename(filename));
                else _this.diffModel.setSecondDiffRoom(_this.diffModel.arRooms.getByFilename(filename));

                _this.updateStereoImages();
            }
        });
};

/**
 * Initialize both parts of panorama
 */
DiffController.prototype.embedDiffPano = function() {
    var _this = this;
    var scriptUrl = "../krpano-common/js/" + ((window.jsonData.fallback) ? "krpano-pr3/krpano.js" : "krpano/krpano.js");
    var swfUrl = "../krpano-common/js/" + ((window.jsonData.fallback) ? "krpano-pr3/krpano.swf" : "krpano/krpano.swf");

    $(this.sPlayerContainer).html("" +
        "<div class='container-fluid'>" +
            "<div class='row row-no-padding'>" +
                "<div class='col-xs-6 " + this.sPlayerLeft.substring(1) + "' id='diffplayer_left'></div>" +
                "<div class='col-xs-6 " + this.sPlayerRight.substring(1) + "' id='diffplayer_right'></div>" +
            "</div>" +
        "</div>"
    );
    var arSides = ['diffplayer_left', 'diffplayer_right'];
    for (var key in arSides) {
        embedpano({
            swf: swfUrl,
            js: scriptUrl,
            xml: null,
            target: arSides[key],
            id: "krpanoSWFObject_diff" + key,
            html5: "prefer",
            webglsettings: {depth:true},
            passQueryParameters: true,
            wmode: "opaque",
            onready: function(krpanoObj) {
                var xml = new EJS({url: window.baseUrl +'/krpano-wrapper-style/hardcoded-content/diffplayer_dual.ejs'})
                    .render($.extend({}, window.jsonData, {side: key}));
                krpanoObj.call("loadxml(" + xml + ")");

                krpanoObj.set("onmousewheel", "events.onmousedown();");
                if (arSides[key] === 'diffplayer_left') {
                    _this.krpanoLeft = krpanoObj;
                } else {
                    _this.krpanoRight = krpanoObj;
                    krpanoObj.set("view.hlookat", _this.diffModel.getDiffAngles(
                        _this.diffModel.diffRooms[1].id, _this.diffModel.diffRooms[0].id));
                    _this.toggleSync();
                }
            },
            onerror: function() {
                console.error("error occured.");
            },
            consolelog: true
        });
    }
};

/**
 * Update only images in popup player
 */
DiffController.prototype.updateStereoImages = function() {
    //todo: update images separately to optimize if needed
    var xml = new EJS({url: window.baseUrl +'/krpano-wrapper-style/hardcoded-content/diffplayer_dual.ejs'})
        .render($.extend({}, window.jsonData, {side: "0"}));
    this.krpanoLeft.call("loadxml(" + xml + ", null, IMAGEONLY)");

    xml = new EJS({url: window.baseUrl +'/krpano-wrapper-style/hardcoded-content/diffplayer_dual.ejs'})
        .render($.extend({}, window.jsonData, {side: "1"}));
    this.krpanoRight.call("loadxml(" + xml + ", null, IMAGEONLY)");

    if (this.diffModel.splitMode === this.diffModel.splitModes.SIDE_BY_SIDE) this.setSideBySideMode(false);
    else this.setSplitMode(false);
};

DiffController.prototype.updatePopupTimeline = function() {
    var popupTimeline = new EJS({url: window.baseUrl +'/krpano-wrapper-style/html/diff_player/popup-timeline.ejs'}).render(window.jsonData);
    $(this.sModal).find(this.sModalTimeline).html(popupTimeline);

    this.handlePopupTimeline();
};

/**
 * Update popup html piece with buttons
 */
DiffController.prototype.updatePopupFooter = function() {
    var popupFooter = new EJS({url: window.baseUrl +'/krpano-wrapper-style/html/diff_player/popup-footer.ejs'}).render(window.jsonData);
    $(this.sModal).find(this.sModalFooter).html(popupFooter);

    this.handleModeBtn();
    this.handleSyncBtn();
};

DiffController.prototype.handleModeBtn = function() {
    var _this = this, name;
    $(this.sModeBtn)
        .off(this.detectedEvent + this.namespace)
        .on(this.detectedEvent + this.namespace, function() {
            if (_this.diffModel.splitMode === _this.diffModel.splitModes.SPLIT) {
                _this.setSideBySideMode(true);
            } else {
                _this.setSplitMode(true);
            }
        });
};

/**
 * use split mode
 * @param doUseAnimation {boolean} indicating whether to use animation or not
 */
DiffController.prototype.setSideBySideMode = function(doUseAnimation) {
    this.diffModel.splitMode = this.diffModel.splitModes.SIDE_BY_SIDE;
    var name = this.diffModel.splitModeNames.SPLIT;
    if (doUseAnimation) {
        this.krpanoLeft.call("tween(area.x, 0%, 0.5, default, updatescreen(), updatescreen());");
        this.krpanoLeft.call("tween(area.width, 100%, 0.5, default, updatescreen(), updatescreen());");
        this.krpanoRight.call("tween(area.x, 0%, 0.5, default, updatescreen(), updatescreen());");
        this.krpanoRight.call("tween(area.width, 100%, 0.5, default, updatescreen(), updatescreen());");
    } else {
        this.krpanoLeft.set("area.x", "0%");
        this.krpanoLeft.set("area.width", "100%");
        this.krpanoRight.set("area.x", "0%");
        this.krpanoRight.set("area.width", "100%");
    }
    $(this.sModeBtn).text(name);
};

/**
 * use side-by-side mode
 * @param doUseAnimation {boolean} indicating whether to use animation or not
 */
DiffController.prototype.setSplitMode = function(doUseAnimation) {
    this.diffModel.splitMode = this.diffModel.splitModes.SPLIT;
    var name = this.diffModel.splitModeNames.SIDE_BY_SIDE;

    if (doUseAnimation) {
        this.krpanoLeft.call("tween(area.x, 0%, 0.5, default, updatescreen(), updatescreen());");
        this.krpanoLeft.call("tween(area.width, 200%, 0.5, default, updatescreen(), updatescreen());");
        this.krpanoRight.call("tween(area.x, -100%, 0.5, default, updatescreen(), updatescreen());");
        this.krpanoRight.call("tween(area.width, 200%, 0.5, default, updatescreen(), updatescreen());");
    } else {
        this.krpanoLeft.set("area.x", "0%");
        this.krpanoLeft.set("area.width", "200%");
        this.krpanoRight.set("area.x", "-100%");
        this.krpanoRight.set("area.width", "200%");
    }
    $(this.sModeBtn).text(name);
};

DiffController.prototype.handleSyncBtn = function() {
    var _this = this;

    //reset to default state: synced
    this.diffModel.isSynced = true;
    if (this.krpanoLeft) _this.toggleSync();

    $(this.sSyncBtn)
        .off("changeState")
        .on("changeState", function() {
            if ($(this).hasClass('active')) {
                $(_this.sSyncBtn).removeClass("btn-primary");
                _this.diffModel.isSynced = false;
            } else {
                $(_this.sSyncBtn).addClass("btn-primary");
                _this.diffModel.isSynced = true;
            }
        });
    $(this.sSyncBtn)
        .off(this.detectedEvent)
        .on(this.detectedEvent, function () {
            $(_this.sSyncBtn).trigger("changeState");
            _this.toggleSync();
        });
};

/**
 * Change diff player behaviour: make it synchronized or unsynchronized
 */
DiffController.prototype.toggleSync = function() {
    //sync one of the screens, when first changed
    var _this = this;
    function sync(krpano1, krpano2) {
        var diff;
        if (krpano2 === _this.krpanoRight)
            diff = _this.diffModel.getDiffAngles(_this.diffModel.diffRooms[1].id, _this.diffModel.diffRooms[0].id);
        else
            diff = _this.diffModel.getDiffAngles(_this.diffModel.diffRooms[0].id, _this.diffModel.diffRooms[1].id);
        krpano1.set("syncother",    false);
        krpano1.set("view.hlookat", krpano2.get("view.hlookat") - diff);
        krpano1.set("view.vlookat", krpano2.get("view.vlookat"));
        krpano1.set("view.fov",     krpano2.get("view.fov"));
    }

    if (this.diffModel.isSynced) {
        this.diffModel.setDiffAngles(this.krpanoLeft.get("view.hlookat"), _this.krpanoRight.get("view.hlookat"));
        [this.krpanoLeft, this.krpanoRight].forEach(function(krpanoObj) {
            var krpanoOpposite = (krpanoObj === _this.krpanoLeft ? _this.krpanoRight : _this.krpanoLeft);
            krpanoObj.set("events.onviewchange", function() {
                if (krpanoObj.get("syncother") === "true") {
                    sync(krpanoOpposite, krpanoObj);
                }
            });
            krpanoObj.set("events.onmousedown", function() {
                krpanoObj.set("syncother", true);
                krpanoObj.call("events.onviewchange();");
            });
        });
    } else {
        [this.krpanoLeft, this.krpanoRight].forEach(function(krpanoObj) {
            krpanoObj.set("events.onviewchange", function() {});
            krpanoObj.set("events.onmousedown", function() {});
        });
    }
};

/**
 * Reload fully all ui stuff in div.krpano-content
 * @todo possibly move to krpano_plugin.js of somewhere else
 */
DiffController.prototype.reloadKrpanoContent = function() {
    $(this.sContent).krpanoConfig(window.jsonData); //redraw all stuff
};

/**
 * Optimize other ui stuff when diffplayer is active
 */
DiffController.prototype.optimizeUI = function() {
    var marginTop = $(this.sHeader).outerHeight();
    $(this.sContent).find(".immo-logo").css("margin-top", marginTop);
    $(this.sContent).find("#side-menu-btn").css("margin-top", marginTop);
    $(this.sContent).find("#infopoint-btn").css("margin-top", marginTop+10); //+default margin
    $(this.sContent).find(".infopoints-expose-section").css("margin-top", marginTop);
};

/**
 * Helper method to resize triangles height in timelines.
 */
DiffController.prototype.updateTriangles = function() {
    var _this = this;
    $(this.sTimeline).each(function(index, item) {
        var h = $(_this.sTimelineTriangle, item).parent().outerHeight();
        $(_this.sTimelineTriangle, item).css("border-top-width", h/2).css("border-bottom-width", h/2);
    });
};
/**
 * Controls rendering of timeline blocks.
 * On small screens -> just show current timeline item with arrows
 * On tablet/desktop screens -> show timeline 1. as a table or 2. as a scrolling slider, depending on how many items it includes.
 */
DiffController.prototype.updateScrolling = function() {
    var _this = this, breakpoint = 768;
    $(this.sTimeline).each(function(index, item) {
        var frameWidth = $(_this.sTimelineFrame, item).width();
        var listWidth = $(_this.sTimelineList, item).width();
        if ($(window).width() < breakpoint) {
            $(item).removeClass(_this.sTimelineAsTable.substring(1));
            $(item).parent().find(".scrollbar").addClass("hide");
        } else if (listWidth > frameWidth) { //make it scrollable
            $(item).removeClass(_this.sTimelineAsTable.substring(1));
            $(item).parent().find(".scrollbar").removeClass("hide");
        } else { //non-scrollable
            $(item).addClass(_this.sTimelineAsTable.substring(1));
            $(item).parent().find(".scrollbar").addClass("hide");
        }
    });
};
DiffController.prototype.updatePanoHeight = function() {
    var hHeader = $(this.sModalHeader).outerHeight();
    var hTimeline = $(this.sModalTimeline).outerHeight();
    var h = ($(window).outerHeight() - hHeader - hTimeline) * 90 / 100;
    $(this.sPlayer).height(h);
    $(this.sPlayerLeft).height(h);
    $(this.sPlayerRight).height(h);
};
/** krpano-content/krpano-wrapper-style/js/diffplayer/diff_player.js **/
'use strict';

function DiffPlayer(interaction, plugin, detectedEvent) {
    this.detectedEvent = detectedEvent;
    this.interaction = interaction;
    this.plugin = plugin;

    /**
     * all rooms from all tours
     * @type {RoomsCollection}
     */
    this.arRooms = new RoomsCollection();

    /**
     * All Timeline tours
     * @type {Array}
     */
    this.arTours = window.jsonData.timelineTours;

    /**
     * 2 rooms chosen and shown in diff player.
     * [0: {roomObj}, 1: {roomObj2}]
     * @type {Array}
     */
    this.diffRooms = window.jsonData.timelineDiffRooms = [];


    /**
     * diff player modes
     * @type {{SPLIT: number, SIDE_BY_SIDE: number}}
     */
    this.splitModes = {
        SPLIT: 0,
        SIDE_BY_SIDE: 1
    };

    /**
     * player modes names
     * @type {{SPLIT: string, SIDE_BY_SIDE: string}}
     */
    this.splitModeNames = {
        SPLIT: "Split mode",
        SIDE_BY_SIDE: "Side-by-side mode"
    };

    /**
     * player mode.
     * @type {number}
     */
    this.splitMode = this.splitModes.SIDE_BY_SIDE;

    /**
     * Flag indicating if player is in synchronized mode or not
     * @type {boolean}
     */
    Object.defineProperty(this, 'isSynced', {
        get: function() { return window.jsonData.timelineIsSynced; },
        set: function(newValue) { window.jsonData.timelineIsSynced = newValue; }
    });
    this.isSynced = true;

    /**
     * Object containing pairs of timeline rooms and difference angle between them.
     * Looks like a 2 dimensional array:
     * {
     *     "aa696df3029e18df901793aa": {"bb696df3029e18df901793bb": 30},
     *     "bb696df3029e18df901793bb": {"aa696df3029e18df901793aa": -30}
     * }
     * @type {{}}
     */
    this.oDiffAngles = {};
    this.anglesStoreTime = 30; //days

    this.init();
    this.controller = new DiffController(this);
}

/**
 * Do use a diffplayer functionality or not.
 * @static
 * @return {boolean}
 */
DiffPlayer.isEnabled = function() {
    var isDocusketch = jsonData.userRoles && jsonData.userRoles.length && jsonData.userRoles.indexOf('DOCUSKETCH') !== -1;
    return jsonData.playerMode === 'DEFAULT' && isDocusketch && "timelineTours" in window.jsonData && Object.keys(window.jsonData.timelineTours).length > 0;
};

DiffPlayer.prototype.init = function() {
    var savedAngles;
    this.setActiveRoom(window.jsonData.tour.activeRoom);
    this.replaceIdsWIthObjects();
    this.setFirstDiffRoom(window.jsonData.tour.activeRoom, true);
    if (savedAngles = Cookies.getJSON("timelineRoomsAngles", {path: document.location.pathname})) {
        this.oDiffAngles = savedAngles;
    }
};

/**
 * Replaces room id with full room objects from timelineTours
 */
DiffPlayer.prototype.replaceIdsWIthObjects = function() {
    this.arTours[window.jsonData.tour.id] = window.jsonData.tour;

    for (var m in this.arTours) {
        for (var n in this.arTours[m].rooms) {
            var r = this.arTours[m].rooms[n];
            this.arRooms.add(r);
        }
    }

    for (var tt in this.arTours) {
        for (var key in this.arTours[tt].rooms) {
            var roomObjects = [this.arTours[tt].rooms[key]];
            if ("timelineRooms" in this.arTours[tt].rooms[key]) {
                this.arTours[tt].rooms[key].timelineRooms.forEach(function (item, k, arItems) {
                    var roomToAdd = this.arRooms.getById(item);
                    if (roomToAdd) {
                        roomObjects.push(roomToAdd);
                    }
                }.bind(this));
            }
            this.arTours[tt].rooms[key].timelineRooms = roomObjects.sort(function(a, b) {
                return a.created - b.created;
            });
        }
    }

    window.jsonData.tour.activeRoom = window.jsonData.tour.rooms[window.jsonData.tour.activeRoom.filename];
};

/**
 * Replace tour with the one from jsonData.timelineTours, load room and set it as active.
 * @param filename
 */
DiffPlayer.prototype.switchRoomAndTour = function (filename) {
    window.jsonData.visibility.showWelcomeWindowNew = false;

    //switch tour
    for (var t in this.arTours) {
        for (var r in this.arTours[t].rooms) {
            if (this.arTours[t].rooms[r].filename === filename) {
                window.jsonData.tour = this.arTours[t];
                window.jsonData.tour.activeRoom = this.arTours[t].rooms[r];
                this.controller.reloadKrpanoContent(); //redraw all stuff
            }
        }
    }

    //switch room
    this.plugin.loadscene(filename);
};

/**
 * Set an active room object.
 * we use a separate property for it (not tour.activeRoom) because of timelineRooms property
 * @param room {object} jsonData room node
 */
DiffPlayer.prototype.setActiveRoom = function(room) {
    window.jsonData.timelineActiveRoom = room;
};
DiffPlayer.prototype.getActiveRoom = function(room) {
    return window.jsonData.timelineActiveRoom;
};
DiffPlayer.prototype.getActiveRoomIndexInTimeline = function(room) {
    return window.jsonData.tour.activeRoom.timelineRooms.indexOf(window.jsonData.timelineActiveRoom);
};

DiffPlayer.prototype.setFirstDiffRoom = function(room, doSetSecond) {
    this.diffRooms[0] = room;

    if (doSetSecond) {
        var arKeys = Object.keys(room.timelineRooms);
        var firstKey = arKeys[0];
        var secondKey = arKeys[1];
        if (arKeys.length === 1) {
            this.diffRooms[1] = room.timelineRooms[firstKey];
        } else {
            this.diffRooms[1] = room.timelineRooms[firstKey] !== this.diffRooms[0] ?
                room.timelineRooms[firstKey] : room.timelineRooms[secondKey];
        }
    }
};

DiffPlayer.prototype.setSecondDiffRoom = function(room, doSetFirst) {
    this.diffRooms[1] = room;

    //also set the first room
    if (doSetFirst) {
        var firstKey = Object.keys(room.timelineRooms)[0];
        var secondKey = Object.keys(room.timelineRooms)[1];
        this.diffRooms[0] = room.timelineRooms[firstKey] !== this.diffRooms[1] ?
            room.timelineRooms[firstKey] : room.timelineRooms[secondKey];
    }
};

DiffPlayer.prototype.setDiffAngles = function(leftVal, rightVal) {
    var leftId = this.diffRooms[0].id;
    var rightId = this.diffRooms[1].id;
    if (!(leftId in this.oDiffAngles)) this.oDiffAngles[leftId] = {};
    if (!(rightId in this.oDiffAngles)) this.oDiffAngles[rightId] = {};
    this.oDiffAngles[leftId][rightId] = leftVal - rightVal;
    this.oDiffAngles[rightId][leftId] = rightVal - leftVal;
    Cookies.set("timelineRoomsAngles", this.oDiffAngles, { expires: this.anglesStoreTime, path: document.location.pathname });
};

DiffPlayer.prototype.getDiffAngles = function(leftId, rightId) {
    var value = 0;
    if (leftId in this.oDiffAngles && rightId in this.oDiffAngles[leftId]) value = this.oDiffAngles[leftId][rightId];
    return value;
};
/** krpano-content/krpano-wrapper-style/js/visualization/visualization_common.js **/
'use strict';

function VisualizationCommon(krpanoEmbeded) {
    CommonPlayer.call(this, krpanoEmbeded);
}

VisualizationCommon.isEnabled = function() {
    var isDocusketch = jsonData.userRoles && jsonData.userRoles.length && jsonData.userRoles.indexOf('DOCUSKETCH') !== -1;
    return !isDocusketch;
};

VisualizationCommon.prototype = Object.create(CommonPlayer.prototype);

VisualizationCommon.prototype.findTimelineRoom = function() {
    if (!Object.keys(jsonData.timelineTours).length || !jsonData.tour.activeRoom.timelineRooms) {
        return false;
    }

    var timelineTourId = Object.keys(jsonData.timelineTours)[0];
    var timelineTour = jsonData.timelineTours[timelineTourId];

    for (var name in timelineTour.rooms) {
        if (timelineTour.rooms[name].id === jsonData.tour.activeRoom.timelineRooms[0]) {
            return timelineTour.rooms[name];
        }
    }

    return null;
};

VisualizationCommon.prototype.findTimelineTourAndRoom = function() {
    if (!Object.keys(jsonData.timelineTours).length) {
        return false;
    }

    var timelineTourId = Object.keys(jsonData.timelineTours)[0];
    var timelineTour = jsonData.timelineTours[timelineTourId];

    for (var name in timelineTour.rooms) {
        if (timelineTour.rooms[name].id === jsonData.tour.activeRoom.timelineRooms[0]) {
            return { tour: timelineTour, room: timelineTour.rooms[name] };
        }
    }

    return { tour: timelineTour };
};

VisualizationCommon.prototype.getTimelineTour = function() {
    if (!Object.keys(jsonData.timelineTours).length) {
        return false;
    }

    var timelineTourId = Object.keys(jsonData.timelineTours)[0];

    return jsonData.timelineTours[timelineTourId];
};

VisualizationCommon.prototype.getTimelineRoomByID = function(id) {
    if (!id) {
        return null;
    }

    var tour = jsonData.tour; // this.getTimelineTour();

    for (var name in tour.rooms) {
        if (tour.rooms.hasOwnProperty(name) && tour.rooms[name].id === id && tour.rooms[name].isTimeline) {
            return tour.rooms[name];
        }
    }
    return null;
};

VisualizationCommon.prototype.getTimelineRoomByName = function(name) {
    if (!name) {
        return null;
    }

    var tour = this.getTimelineTour();

    if (tour.rooms[name]) {
        return tour.rooms[name];
    }

    return null;
};

VisualizationCommon.prototype.showInfo = function(sentence, hideAfterMs) {
    this.showMessage('success', sentence, hideAfterMs)
}
VisualizationCommon.prototype.showError = function(sentence, hideAfterMs) {
    this.showMessage('error', sentence, hideAfterMs)
}
VisualizationCommon.prototype.showWarning = function(sentence, hideAfterMs) {
    this.showMessage('warning', sentence, hideAfterMs)
}
/** krpano-content/krpano-wrapper-style/js/visualization/visualization_player.js **/
'use strict';

function VisualizationPlayer(interaction, krpano, detectedEvent, visualizationCommon, plugin) {
    this.detectedEvent = detectedEvent;
    this.interaction = interaction;
    this.krpano = krpano;
    this.visualizationCommon = visualizationCommon;
    this.plugin = plugin;

    this.defaultPlayerInBubble;
    this.krpanoVizualization;

    this.pathXML = window.baseUrl + '/krpano-wrapper-style/hardcoded-content/visualization.ejs';
    this.pathEJS = window.baseUrl + '/krpano-wrapper-style/html/visualization/visualizationBubble.ejs';
    this.galleryEJS = window.baseUrl + '/krpano-wrapper-style/html/gallerySlider.ejs';

    this.timelineActiveRoom;
    this.originalActiveRoom;

    this.init();
}

VisualizationPlayer.isEnabled = function() {
    var timelineTour = 'timelineTours' in jsonData && Object.keys(jsonData.timelineTours).length > 0;
    return VisualizationCommon.isEnabled() && jsonData.playerMode === 'DEFAULT' && timelineTour;
};

VisualizationPlayer.prototype.init = function() {
    var visualizationBubble = new EJS({ url: this.pathEJS }).render(jsonData);
    $(visualizationBubble).appendTo($('#playerContainer'));

    this.initSecondPlayer();
    this.handleRoomChange();
    this.toggleBubble();
    this.reconfigureTourData();
    this.showSplitGallery();
};

VisualizationPlayer.prototype.setActiveRooms = function() {
    this.originalActiveRoom = jsonData.tour.activeRoom;

    if (this.originalActiveRoom.isTimeline) {
        this.swapActiveRooms();
    } else {
        var id = jsonData.tour.activeRoom.timelineRooms && jsonData.tour.activeRoom.timelineRooms[0];
        this.timelineActiveRoom = this.visualizationCommon.getTimelineRoomByID(id);
    }
};

VisualizationPlayer.prototype.toggleVisualizationMark = function() {
    if (!this.defaultPlayerInBubble || !jsonData.tour.activeRoom.timelineRooms) {
        $('.visualization-title').hide();
    } else {
        $('.visualization-title').show();
    }
};

VisualizationPlayer.prototype.handleRoomChange = function() {
    var _this = this;

    $(document).on('krpano:loadscene', function() {
        $('.visualization-bubble-wrapper').removeClass('fade-in').addClass('fade-out');
        _this.toggleVisualizationMark();
        _this.setActiveRooms();
        _this.showTourInBubble();
    });
};

VisualizationPlayer.prototype.initSecondPlayer = function() {
    var _this = this;

    embedpano({
        swf: '../krpano-common/js/' + (jsonData.fallback ? 'krpano-pr3/krpano.swf' : 'krpano/krpano.swf'),
        js: '../krpano-common/js/' + (jsonData.fallback ? 'krpano-pr3/krpano.js' : 'krpano/krpano.js'),
        xml: null,
        target: 'visualization-container',
        id: 'krpanoSWFObject_visualization',
        html5: 'prefer',
        webglsettings: { depth: true },
        passQueryParameters: true,
        wmode: 'opaque',
        onready: function(krpano) {
            _this.krpanoVizualization = krpano;
            _this.setEventListeners();
        },
        onerror: function() {
            console.log('Error on init visualization krpano player', error);
        },
        consolelog: true
    });
};

VisualizationPlayer.prototype.enableAutorotation = function() {
    this.krpanoVizualization.set('autorotate.enabled', true);
};

VisualizationPlayer.prototype.disableAutorotation = function() {
    this.krpanoVizualization.set('autorotate.enabled', false);
};

VisualizationPlayer.prototype.toggleBubble = function() {
    $('.visualization-bubble-wrapper .close-btn').on(this.detectedEvent, function() {
        var bubble = $('.visualization-bubble-wrapper');
        bubble.toggleClass('fade-in fade-out');

        setTimeout(function() {
            bubble.toggleClass('big-bubble small-bubble');
            bubble.toggleClass('fade-in fade-out');
        }, 500);
    });
};

VisualizationPlayer.prototype.updateImage = function(tourData) {
    if (tourData && tourData.room && tourData.tour) {
        $('.visualization-bubble-wrapper').removeClass('fade-out').addClass('fade-in');

        var xml = new EJS({ url: this.pathXML }).render({
            activeRoom: tourData.room,
            useCubic: tourData.tour.useCubic,
            visibility: jsonData.visibility
        });

        this.krpanoVizualization.call('loadxml(' + xml + ', null, IMAGEONLY)');
    }
};

VisualizationPlayer.prototype.getLookAt = function(player) {
    if (!player) return;

    return {
        hlookat: player.get('view.hlookat'),
        vlookat: player.get('view.vlookat'),
        fov: player.get('view.fov')
    };
};

VisualizationPlayer.prototype.showImageInDefaultPlayer = function() {
    var filename = this.defaultPlayerInBubble ? this.timelineActiveRoom.filename : this.originalActiveRoom.filename;
    this.loadScene(filename);
};

VisualizationPlayer.prototype.loadScene = function(filename) {
    var filenameClean = this.visualizationCommon.cleanFilename(filename);
    var position = this.getLookAt(this.defaultPlayerInBubble ? this.krpano : this.krpanoVizualization);

    this.krpano.call('loadscene(scene' + filenameClean + ', null, MERGE, OPENBLEND(2.6, -0.5, 0.5, 0, linear));' +
        'lookat(' + position.hlookat + ',' + position.vlookat + ',' + position.fov + ');wait(LOAD);');
};

VisualizationPlayer.prototype.setEventListeners = function() {
    var _this = this;

    this.krpano.set('syncother', true);

    if (this.krpano.events.onviewchange) {
        var prevFunc = this.krpano.events.onviewchange;
        var nextFunc = function() {
            _this.syncPlayers();
        }
        var combined = function () {
            prevFunc();
            nextFunc();
        }
        this.krpano.set('events.onviewchange', combined);
    } else {
        this.krpano.set('events.onviewchange', function() {
            _this.syncPlayers();
        });
    }

    this.krpano.set('events.onmousedown', function() {
        if (!_this.defaultPlayerInBubble) {
            _this.krpano.call('events.onviewchange();');
        }
    });

    this.krpanoVizualization.set('events.onmousedown', function() {
        _this.swapPlayers();
    });

    this.visualizationCommon.krpanoEmbeded.on(this.detectedEvent, '.slider-gallery-section .slider-element img', function() {
        var isOriginal = !$(this).hasClass('visualization');
        _this.defaultPlayerInBubble = !isOriginal;

        if (isOriginal) {
            _this.interaction.initGallerySliderCarouselElementSelect($(this).parent());
        } else {
            if (!_this.touchMove) {
                var activeCarouselId = $(this).parents('.owl-carousel').attr('id');
                var timelineFilenameClean = $(this).parent().attr('data-visualization-filename');
                var roomFileName = _this.interaction.originalFilename(timelineFilenameClean);
                _this.timelineActiveRoom = _this.visualizationCommon.getTimelineRoomByName(roomFileName);

                jsonData.tour.activeRoom = _this.timelineActiveRoom;

                _this.plugin.loadThiScene(roomFileName, null, activeCarouselId, jsonData.playerMode);
            }
        }
    });

    this.krpano.set('events.onautorotatestart', function() {
        _this.enableAutorotation();
    });

    this.krpano.set('events.onautorotatestop', function() {
        _this.disableAutorotation();
    });

    this.visualizationCommon.krpanoEmbeded.on('mouseenter', '.slider-gallery-section .slider-element img', function() {
        if ($(this).hasClass('visualization')) {
            $(this).parent().addClass('right');
        } else {
            $(this).parent().addClass('left');
        }
    }).on('mouseleave', '.slider-gallery-section .slider-element img', function() {
        $(this).parent().removeClass('left').removeClass('right');
    });
};

VisualizationPlayer.prototype.swapActiveRooms = function() {
    this.timelineActiveRoom = jsonData.tour.activeRoom;
    var roomID = jsonData.tour.activeRoom.timelineRooms[0];

    for (var filename in jsonData.tour.rooms) {
        if (jsonData.tour.rooms.hasOwnProperty(filename) && jsonData.tour.rooms[filename].id === roomID) {
            this.originalActiveRoom = jsonData.tour.rooms[filename];
            break;
        }
    }
};

VisualizationPlayer.prototype.syncPlayers = function() {
    if (this.krpano.get('syncother') === true) {
        this.krpanoVizualization.set('syncother', false);
        this.krpanoVizualization.set('view.hlookat', this.krpano.get('view.hlookat'));
        this.krpanoVizualization.set('view.vlookat', this.krpano.get('view.vlookat'));
        this.krpanoVizualization.set('view.fov', this.krpano.get('view.fov'));
    }
};

VisualizationPlayer.prototype.showTourInBubble = function() {
    var defaultTourData = { tour: jsonData.tour, room: this.originalActiveRoom };
    var timelineTourData = { tour: this.visualizationCommon.getTimelineTour(), room: this.timelineActiveRoom };

    if (this.originalActiveRoom && this.timelineActiveRoom) {
        this.updateImage(this.defaultPlayerInBubble ? defaultTourData : timelineTourData);
    }
};

VisualizationPlayer.prototype.swapPlayers = function() {
    this.defaultPlayerInBubble = !this.defaultPlayerInBubble;
    this.showImageInDefaultPlayer();
};

VisualizationPlayer.prototype.reconfigureTourData = function() {
    var timelineTourData = this.visualizationCommon.findTimelineTourAndRoom();

    for (var filename in timelineTourData.tour.rooms) {
        timelineTourData.tour.rooms[filename].isTimeline = true;

        jsonData.tour.rooms[filename] = timelineTourData.tour.rooms[filename];

        this.addPanoElement(filename);

        for (var infopoint in jsonData.tour.rooms[filename].attachments) {
            var infopoint = timelineTourData.tour.rooms[filename].attachments[infopoint];

            if (infopoint.content && infopoint.content[0] && infopoint.content[0].type !== 'HOTSPOT') {
                jsonData.tour.infopoints[infopoint.content[0].ID] = infopoint;
            }
        }
    }
};

VisualizationPlayer.prototype.addPanoElement = function(filename) {
    var input = this.visualizationCommon.krpanoEmbeded.find('.tour-data input:last').clone();
    input.val('plan-' + jsonData.tour.rooms[filename].plan);
    input.prop('id', 'pano-' + this.visualizationCommon.cleanFilename(filename));
    this.visualizationCommon.krpanoEmbeded.find('.tour-data').append(input);
};

VisualizationPlayer.prototype.appendVisualizationImage = function(sliderElement) {
    var filenameClean = sliderElement.attr('data-filename');
    var filenameOriginal = this.visualizationCommon.originalFilename(filenameClean);

    if (jsonData.tour.rooms[filenameOriginal] && jsonData.tour.rooms[filenameOriginal].timelineRooms &&
        jsonData.tour.rooms[filenameOriginal].timelineRooms.length) {

        var timelineRoomID = jsonData.tour.rooms[filenameOriginal].timelineRooms[0];
        var timelineRoom = this.visualizationCommon.getTimelineRoomByID(timelineRoomID);
        var timelineFilenameClean = this.visualizationCommon.cleanFilename(timelineRoom.filename);
        var img = $('<img class="img-responsive visualization" src="' + timelineRoom.thumbnail + '">');

        sliderElement.addClass('has-visualization')
            .attr('data-visualization-filename', timelineFilenameClean)
            .append('<span class="visualization-hint before">' + window.lang.translate('generic.before') + '</span>')
            .append('<span class="slider-line-wrapper"><i class="fa fa-chevron-left"></i><span class="comparison-line"></span><i class="fa fa-chevron-right"></i></span>')
            .append(img)
            .append('<span class="visualization-hint after">' + window.lang.translate('generic.after') + '</span>');
    }
};

VisualizationPlayer.prototype.showSplitGallery = function() {
    var _this = this;

    $('.slider-gallery-section .slider-element').each(function(index) {
        _this.appendVisualizationImage($(this));
    });
};

VisualizationPlayer.prototype.enableShowOnlySplitGallery = function() {
    var _this = this;

    $('.slider-gallery-section .owl-carousel').each(function(key, owl) {
        $(owl).find('.slider-element')
            .toArray()
            .map(function(element, index) {
                return { element: element, index: index };
            })
            .reverse()
            .forEach(function(pair) {
                if (!$(pair.element).hasClass('has-visualization')) {
                    $(owl).trigger('remove.owl.carousel', pair.index);
                    $(owl).trigger('refresh.owl.carousel');
                }
            });

        _this.hideFloorWithNoRooms(owl);
    });

    this.interaction.showGallery();
};

VisualizationPlayer.prototype.hideFloorWithNoRooms = function(owl) {
    if (!$(owl).find('.slider-element').length) {
        var floor = $(owl).data('floor');
        $('.slider-gallery-actions .dropdown-floor-wrapper .floor-item[data-floor="' + floor + '"]').addClass('hidden');
    }
};

VisualizationPlayer.prototype.disableShowOnlySplitGallery = function() {
    $('.slider-gallery-section .slider-element').each(function(elem) {
        $(this).removeClass('hidden');
    });

    this.removeAllGalleryImages();

    var _this = this;
    var filename = jsonData.tour.activeRoom.filename;

    if (jsonData.tour.activeRoom.isTimeline) {
        filename = this.interaction.getFilenameByID(jsonData.tour.activeRoom.timelineRooms[0]);
    }

    var filenameClean = this.visualizationCommon.cleanFilename(filename);

    $('.slider-gallery-section .owl-carousel').each(function(key, owl) {
        for (var filename in jsonData.tour.rooms) {
            var room = jsonData.tour.rooms[filename];

            if (room.plan === $(owl).data('floor') && !room.isTimeline) {
                _this.addImageToGallery(room, owl);
            }
        }

    });

    $('.slider-gallery-actions .dropdown-floor-wrapper .floor-item').removeClass('hidden');

    this.showSplitGallery();
    this.interaction.showGallery();
    this.interaction.syncCarouselPosition(filenameClean);

    setTimeout(function() {
        _this.visualizationCommon.krpanoEmbeded.find('.plans__nav.js-gallery-btn').addClass('active');
    });
};

VisualizationPlayer.prototype.removeAllGalleryImages = function() {
    $('.slider-gallery-section .owl-carousel').each(function(key, owl) {
        $(owl).find('.slider-element')
            .toArray()
            .map(function(element, index) {
                return { element: element, index: index };
            })
            .reverse()
            .forEach(function(pair) {
                $(owl).trigger('remove.owl.carousel', pair.index);
                $(owl).trigger('refresh.owl.carousel');
            });
    });
};

VisualizationPlayer.prototype.addImageToGallery = function(room, owl) {
    var cleanFilename = this.visualizationCommon.cleanFilename(room.filename);
    var newElement = $('<div id="thumb-' + cleanFilename + '" ' + 'class="slider-element ' + room.plan + '" ' +
        'data-filename="' + cleanFilename + '" data-access="' + room.limitedAccess + '">' +
        '<img class="img-responsive" src="' + room.thumbnail + '">' +
        '<span class="slider-description">' + room.name || '' + '</span></div>');

    $(owl).trigger('add.owl.carousel', [newElement]);
    $(owl).trigger('refresh.owl.carousel');
};

/** krpano-content/krpano-wrapper-style/js/visualization/visualization_editor.js **/
'use strict';

function VisualizationEditor(krpanoEmbeded, krpano, detectedEvent, api, promiseApi, editor, visualizationCommon) {
    this.detectedEvent = detectedEvent;
    this.krpanoEmbeded = krpanoEmbeded;
    this.krpano = krpano;
    this.api = api;
    this.promiseApi = promiseApi;
    this.editor = editor;
    this.visualizationCommon = visualizationCommon;

    this.modal = krpanoEmbeded.find('#add-visualization-modal');

    this.init();
}

VisualizationEditor.isEnabled = function() {
    var showVisualizationEditor = jsonData.userRoles && jsonData.userRoles.indexOf('VISUALIZATION_EDITOR_ROLE') !== -1;
    return VisualizationCommon.isEnabled() && jsonData.playerMode === 'EDIT_ATTACHMENTS' && showVisualizationEditor;
};

VisualizationEditor.prototype.init = function() {
    this.showUploadModal();
    this.addVisualization();
    this.deleteVisualization();
};

VisualizationEditor.prototype.showUploadModal = function() {
    var _this = this;

    this.krpanoEmbeded.find('#add_visualization').on(this.detectedEvent, function(e) {
        _this.modal.modal('show');
        _this.hideFileElements(_this.modal.find('.upload-wrapper-room .upload-btn'));
        _this.showCurrentPano();
    });
};

VisualizationEditor.prototype.hideFileElements = function(button) {
    var wrapper = button.parent();
    wrapper.find('.selected-block').hide();
    wrapper.find('.preview-image').hide();
    wrapper.removeClass('has-error');
    wrapper.find('.help-block').hide();
    wrapper.find('.delete-pano').hide();
    button.find('i').removeClass('fa-check').addClass('fa-upload');
    this.modal.find('button[type="submit"]').attr('disabled', true);
};

VisualizationEditor.prototype.showCurrentPano = function() {
    var timelineActiveRoom = this.visualizationCommon.findTimelineRoom();

    // after uploading a new pano, image would not be shown since there is no image url in request response
    if (timelineActiveRoom) {
        this.modal.find('.preview-current-image').attr('src', timelineActiveRoom.thumbnail);
        this.modal.find('.preview-filename').text(timelineActiveRoom.filename);
        this.modal.find('.preview-current-image-wrapper').css('display', 'block');
    } else {
        this.modal.find('.preview-current-image-wrapper').hide();
        this.modal.find('.preview-current-image').attr('src', '');
        this.modal.find('.preview-filename').text('');
    }
};

VisualizationEditor.prototype.addVisualization = function() {
    var _this = this;

    this.modal.find('.btn[type="submit"]').on(this.detectedEvent, function(e) {
        e.preventDefault();

        _this.uploadVisualization($(this));
    });
};

VisualizationEditor.prototype.uploadVisualization = function(button) {
    var _this = this;
    var originalPanoID = jsonData.tour.activeRoom.id;
    var payload = JSON.stringify({
        timelineOf: originalPanoID,
        source: 'EDITOR',
        plan: jsonData.tour.activeRoom.plan,
        name: jsonData.tour.activeRoom.name
    });

    var dataRequest = new FormData();
    var file = this.modal.find('input[type=file]')[0];
    var currentFile = file.files && file.files.length > 0 ? file.files[0] : null;

    dataRequest.append('file', currentFile);

    this.api.buttonLoading(button);

    // if no timeline tour exists, create one before uploading panos
    this.getTimeline().then(
        function(timelineTour) {
            _this.promiseApi.createTimelinePano(timelineTour.id, payload).then(
                function(createdRoom) {
                    if (createdRoom) {
                        _this.promiseApi.uploadTimelinePano(timelineTour.id, createdRoom.details.id, dataRequest).then(
                            function(uploadedPano) {
                                _this.api.buttonUnloading(button);
                                _this.updateJsonDataAfterAddPano(timelineTour, uploadedPano);
                                _this.visualizationCommon.showWarning(window.lang.translate('visualization.notification.warning'), 4000);

                                _this.modal.modal('hide');
                            },
                            function(error) {
                                _this.visualizationCommon.showError(window.lang.translate('modals.contact.error.description'), 4000);
                            }
                        );
                    }
                },
                function(error) {
                    _this.visualizationCommon.showError(window.lang.translate('modals.contact.error.description'), 4000);
                }
            );
        },
        function(error) {
            console.error(error);
        }
    );
};

VisualizationEditor.prototype.getTimeline = function() {
    var _this = this;

    return new Promise(function(resolve, reject) {
        if (Object.keys(jsonData.timelineTours).length) {
            var firstKey = Object.keys(jsonData.timelineTours)[0];
            resolve(jsonData.timelineTours[firstKey]);
        } else {
            var timelineTourName = 'Home Staging - ' + jsonData.tour.name;
            _this.promiseApi.createTimelineTour(jsonData.tour.id, timelineTourName, jsonData.tour.agentID).then(
                function(data) {
                    if (data.type === 'TOUR_CREATED') {
                        var newTour = {};
                        newTour[data.details.id] = $.extend(data.details, { rooms: [] });
                        $.extend(jsonData.timelineTours, newTour);
                    }

                    var firstKey = Object.keys(jsonData.timelineTours)[0];
                    resolve(jsonData.timelineTours[firstKey]);
                },
                function(error) {
                    _this.visualizationCommon.showError(window.lang.translate('modals.contact.error.description'), 4000);
                }
            );
        }
    });
};

VisualizationEditor.prototype.deleteVisualization = function() {
    var _this = this;

    this.modal.find('.delete-pano').on(this.detectedEvent, function(e) {
        var button = $(this);
        var timelineData = _this.visualizationCommon.findTimelineTourAndRoom();

        _this.api.buttonLoading($(this));
        _this.promiseApi.deleteTimelinePano(timelineData.tour.id, timelineData.room.id).then(
            function(pano) {
                button.closest('.image-preview').attr('src', '');
                button.closest('.preview-current-image-wrapper').hide();

                _this.updateJsonDataAfterDeletePano(timelineData);
                _this.api.buttonUnloading(button);
                _this.visualizationCommon.showInfo(window.lang.translate('default.message.top.success.default'), 4000);
            },
            function(error) {
                _this.visualizationCommon.showError(window.lang.translate('modals.contact.error.description'), 4000);
            }
        );
    });
};

VisualizationEditor.prototype.updateJsonDataAfterAddPano = function(timelineTour, uploadedPano) {
    var newRoom = {};
    newRoom[uploadedPano.details.filename] = uploadedPano.details;
    $.extend(timelineTour.rooms, newRoom);

    if (!jsonData.tour.activeRoom.timelineRooms) {
        var timelineRooms = {};
        timelineRooms.timelineRooms = [];
        $.extend(jsonData.tour.activeRoom, timelineRooms);
    }

    jsonData.tour.activeRoom.timelineRooms.push(uploadedPano.details.id)
    jsonData.tour.rooms[jsonData.tour.activeRoom.filename].timelineRooms.push(uploadedPano.details.id);
};

VisualizationEditor.prototype.updateJsonDataAfterDeletePano = function(timelineData) {
    jsonData.tour.activeRoom.timelineRooms = jsonData.tour.activeRoom.timelineRooms.filter(function(room) {
        return room != timelineData.room.id;
    });

    for (var filename in timelineData.tour.rooms) {
        if (filename === timelineData.room.filename) {
            delete timelineData.tour.rooms[filename];
        }
    }

    delete timelineData.room;

    if (!Object.keys(timelineData.tour.rooms).length) {
        this.deleteTour(timelineData.tour.id);
    }
};

VisualizationEditor.prototype.deleteTour = function(tourID) {
    this.promiseApi.deleteTour(tourID).then(function(pano) {
        delete jsonData.timelineTours[tourID];
    });
};

