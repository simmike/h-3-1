/** krpano-content/krpano-wrapper-style/js/language_switcher.js **/
window.lang = new Lang();

lang.dynamic('en_gb', window.baseUrl +'/krpano-wrapper-style/lang/gb.json');
lang.dynamic('en_us', window.baseUrl +'/krpano-wrapper-style/lang/en.json');
lang.dynamic('en_ca', window.baseUrl +'/krpano-wrapper-style/lang/en.json');
lang.dynamic('en_za', window.baseUrl +'/krpano-wrapper-style/lang/en.json');
lang.dynamic('en_nz', window.baseUrl +'/krpano-wrapper-style/lang/en.json');
lang.dynamic('en_au', window.baseUrl +'/krpano-wrapper-style/lang/au.json');
lang.dynamic('es_es', window.baseUrl +'/krpano-wrapper-style/lang/es.json');
lang.dynamic('pt_pt', window.baseUrl +'/krpano-wrapper-style/lang/pt.json');
lang.dynamic('it_it', window.baseUrl +'/krpano-wrapper-style/lang/it.json');
lang.dynamic('it_ch', window.baseUrl +'/krpano-wrapper-style/lang/it.json');
lang.dynamic('fr_fr', window.baseUrl +'/krpano-wrapper-style/lang/fr.json');
lang.dynamic('fr_ca', window.baseUrl +'/krpano-wrapper-style/lang/fr.json');
lang.dynamic('de_de', window.baseUrl +'/krpano-wrapper-style/lang/de.json');
lang.dynamic('de_ch', window.baseUrl +'/krpano-wrapper-style/lang/de.json');
lang.dynamic('de_at', window.baseUrl +'/krpano-wrapper-style/lang/de.json');
lang.dynamic('nl_nl', window.baseUrl +'/krpano-wrapper-style/lang/nl.json');
lang.dynamic('nl_be', window.baseUrl +'/krpano-wrapper-style/lang/nl.json');
lang.dynamic('tr_tr', window.baseUrl +'/krpano-wrapper-style/lang/tr.json');

Lang.prototype.attrList.splice(3, 1);
Lang.prototype.attrList.push('data-title');

$(function() {
  lang.onChange = function () {
    if (window._DollHouse) {
      window._DollHouse.ui.setLanguage(lang.currentLang);
    } else if (jsonData && jsonData.visibility && jsonData.visibility.showDollHouse) {
      setTimeout(function () {
        lang.onChange(); // Try again
      }, 250);
    }
  }

  lang.init({
      defaultLang: jsonData.lang.toLowerCase() || 'en_us',
      cookie:{
          name: 'activeFlag'
      }
  });
});

